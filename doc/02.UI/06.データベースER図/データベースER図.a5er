ViewOption=
Query=--*DataTitle V_C_BUN_JUCHU_SHO_K(分析受注詳細項目別サンプル共通ビュー)\nSELECT TBL_A.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_A.JURI_EDA_NO               AS  JURI_EDA_NO               -- 枝番\n      ,TBL_A.SEQ                       AS  SEQ                       -- SEQ\n      ,TBL_A.GYO_NO                    AS  GYO_NO                    -- 行No.\n      ,TBL_A.BUNSEKI_CD                AS  BUNSEKI_CD                -- 分析項目CD\n      ,TBL_A.BUNSEKI_MEI               AS  BUNSEKI_MEI               -- 分析項目名\n      ,TBL_A.BUNSEKI_TANI              AS  BUNSEKI_TANI              -- 単位\n      ,TBL_A.BUNSEKI_DATA              AS  BUNSEKI_DATA              -- データ\n      ,TBL_A.BUNSEKI_NO                AS  BUNSEKI_NO                -- 分析番号\n      ,TBL_A.URAGAKI_PRINT_FLG         AS  URAGAKI_PRINT_FLG         -- 裏書印刷済フラグ\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,ISNULL(TBL_B.STATE_KBN,1)       AS  STATE_KBN                 -- 状態\n      ,M_STATE_KBN.DATA_1              AS  STATE_MEI                 -- 状態名\n      ,TBL_B.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_B.SOKUTEI_DATE              AS  SOKUTEI_DATE              -- 測定日\n      ,TBL_B.SOKUTEISHA_CD             AS  SOKUTEISHA_CD             -- 測定者\n      ,M_SOKUTEISHA_CD.SHIMEI          AS  SOKUTEISHA_MEI            -- 測定者名\n      ,TBL_B.COLUMN_01                 AS  COLUMN_01                 -- 汎用カラム０１\n      ,TBL_B.COLUMN_02                 AS  COLUMN_02                 -- 汎用カラム０２\n      ,TBL_B.COLUMN_03                 AS  COLUMN_03                 -- 汎用カラム０３\n      ,TBL_B.COLUMN_04                 AS  COLUMN_04                 -- 汎用カラム０４\n      ,TBL_B.COLUMN_05                 AS  COLUMN_05                 -- 汎用カラム０５\n      ,TBL_B.COLUMN_06                 AS  COLUMN_06                 -- 汎用カラム０６\n      ,TBL_B.COLUMN_07                 AS  COLUMN_07                 -- 汎用カラム０７\n      ,TBL_B.COLUMN_08                 AS  COLUMN_08                 -- 汎用カラム０８\n      ,TBL_B.COLUMN_09                 AS  COLUMN_09                 -- 汎用カラム０９\n      ,TBL_B.COLUMN_10                 AS  COLUMN_10                 -- 汎用カラム１０\n      ,TBL_B.COLUMN_11                 AS  COLUMN_11                 -- 汎用カラム１１\n      ,TBL_B.COLUMN_12                 AS  COLUMN_12                 -- 汎用カラム１２\n      ,TBL_B.COLUMN_13                 AS  COLUMN_13                 -- 汎用カラム１３\n      ,TBL_B.COLUMN_14                 AS  COLUMN_14                 -- 汎用カラム１４\n      ,TBL_B.COLUMN_15                 AS  COLUMN_15                 -- 汎用カラム１５\n      ,TBL_B.COLUMN_16                 AS  COLUMN_16                 -- 汎用カラム１６\n      ,TBL_B.COLUMN_17                 AS  COLUMN_17                 -- 汎用カラム１７\n      ,TBL_B.COLUMN_18                 AS  COLUMN_18                 -- 汎用カラム１８\n      ,TBL_B.COLUMN_19                 AS  COLUMN_19                 -- 汎用カラム１９\n      ,TBL_B.COLUMN_20                 AS  COLUMN_20                 -- 汎用カラム２０\n      ,TBL_B.COLUMN_21                 AS  COLUMN_21                 -- 汎用カラム２１\n      ,TBL_B.COLUMN_22                 AS  COLUMN_22                 -- 汎用カラム２２\n      ,TBL_B.COLUMN_23                 AS  COLUMN_23                 -- 汎用カラム２３\n      ,TBL_B.COLUMN_24                 AS  COLUMN_24                 -- 汎用カラム２４\n      ,TBL_B.COLUMN_25                 AS  COLUMN_25                 -- 汎用カラム２５\n      ,TBL_B.COLUMN_26                 AS  COLUMN_26                 -- 汎用カラム２６\n      ,  \q|\q + TBL_A.BUNSEKI_CD                    -- 分析項目CD\n       + \q|\q + TBL_A.BUNSEKI_MEI                   -- 分析項目名\n       + \q|\q + TBL_A.BUNSEKI_DATA                  -- データ\n       + \q|\q + TBL_A.BUNSEKI_NO                    -- 分析番号\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS_S              -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER_S         -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE_S         -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER_S         -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE_S         -- 更新日時\n      ,TBL_B.DBS_STATUS                AS  DBS_STATUS_K              -- レコード状態\n      ,TBL_B.DBS_CREATE_USER           AS  DBS_CREATE_USER_K         -- 作成ユーザＩＤ\n      ,TBL_B.DBS_CREATE_DATE           AS  DBS_CREATE_DATE_K         -- 作成日時\n      ,TBL_B.DBS_UPDATE_USER           AS  DBS_UPDATE_USER_K         -- 更新ユーザＩＤ\n      ,TBL_B.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE_K         -- 更新日時\n  FROM T_BUN_JUCHU_SHOSAI              AS  TBL_A                     -- 【分析受注詳細テーブル】\n  LEFT JOIN\n       T_KOMOKU_SAMPLE                 AS  TBL_B                     -- 【項目別サンプルテーブル】\n    ON TBL_B.JURI_NO                   =   TBL_A.JURI_NO\n   AND TBL_B.JURI_EDA_NO               =   TBL_A.JURI_EDA_NO\n   AND TBL_B.SEQ                       =   TBL_A.SEQ\n  LEFT JOIN                                                          -- 【状態用】\n       M_MEISHO_B                      AS  M_STATE_KBN\n    ON M_STATE_KBN.MEISHO_KBN          =   \qK019\q\n   AND M_STATE_KBN.MEISHO_CD           =   ISNULL(TBL_B.STATE_KBN,1)\n  LEFT JOIN                                                          -- 【測定者用】\n       M_SHAIN                         AS  M_SOKUTEISHA_CD\n    ON M_SOKUTEISHA_CD.SHAIN_CD        =   TBL_B.SOKUTEISHA_CD\n
