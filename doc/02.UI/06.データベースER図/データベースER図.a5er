Query=SELECT J_AFTER_STS.DATA_1                     AS  AFTER_STS       --変更後ステータス\n      ,J_BEFOR_STS.DATA_1                     AS  BEFORE_STS      --変更前ステータス\n      ,MOD_DATE_TIME                          AS  UPDATE_DATE     --変更日時\n      ,J_SHAIN.SHIMEI                         AS  UPDATE_USER     --更新者名\n      ,T_KOBAI_STS_R.KOBAI_NO                 AS  KOBAI_NO        --購買申請No.\n      ,T_KOBAI_STS_R.KOBAI_SEQ                AS  KOBAI_SEQ       --購買申請SEQ\n      ,T_KOBAI_STS_R.AFTER_STS                AS  AFTER_STS_CD    --変更後ステータスCD\n      ,T_KOBAI_STS_R.BEFORE_STS               AS  BEFORE_STS_CD   --変更前ステータスCD\n  FROM T_KOBAI_STS_R\n\n  LEFT JOIN M_MEISHO_B                     AS J_BEFOR_STS       --変更前ステータス\n    ON J_BEFOR_STS.MEISHO_KBN = \qS005\q\n   AND T_KOBAI_STS_R.BEFORE_STS = J_BEFOR_STS.MEISHO_CD\n   \n  LEFT JOIN M_MEISHO_B                     AS J_AFTER_STS       --変更後ステータス\n    ON J_AFTER_STS.MEISHO_KBN = \qS005\q\n   AND T_KOBAI_STS_R.AFTER_STS = J_AFTER_STS.MEISHO_CD\n\n  LEFT JOIN M_SHAIN                        AS J_SHAIN\n    ON T_KOBAI_STS_R.DBS_UPDATE_USER = J_SHAIN.SHAIN_CD\n\n
