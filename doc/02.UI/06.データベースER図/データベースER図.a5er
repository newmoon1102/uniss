[View]
PName=V_BUN_STS_R_B
LName=分析ステータス変更履歴(ボディー)ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT J_AFTER_STS.DATA_1                     AS  AFTER_STS       --変更後ステータス\n      ,J_BEFOR_STS.DATA_1                     AS  BEFORE_STS      --変更前ステータス            \n      ,STUFF(STUFF(SUBSTRING(CONVERT(VARCHAR(25), T_BUN_STS_R.MOD_DATE_TIME, 120), 1, 19), 8, 1, \q/\q), 5, 1, \q/\q)\n                                              AS  UPDATE_DATE     --変更日時\n      ,J_SHAIN.SHIMEI                         AS  UPDATE_USER     --更新者名\n      ,T_BUN_STS_R.JURI_NO                    AS  JURI_NO         --受理No.\n  \n  FROM T_BUN_STS_R                        \n     \n  LEFT JOIN M_MEISHO_B                     AS J_BEFOR_STS       --変更前ステータス\n    ON J_BEFOR_STS.MEISHO_KBN = \qS003\q\n   AND T_BUN_STS_R.BEFORE_STS = J_BEFOR_STS.MEISHO_CD\n   \n  LEFT JOIN M_MEISHO_B                     AS J_AFTER_STS       --変更後ステータス\n    ON J_AFTER_STS.MEISHO_KBN = \qS003\q\n   AND T_BUN_STS_R.AFTER_STS = J_AFTER_STS.MEISHO_CD\n  LEFT JOIN M_SHAIN                        AS J_SHAIN\n    ON T_BUN_STS_R.DBS_UPDATE_USER = J_SHAIN.SHAIN_CD\n  \n WHERE T_BUN_STS_R.AFTER_STS != T_BUN_STS_R.BEFORE_STS\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="分析受注関連",3200,1650
Page=分析受注関連
Left=3200
