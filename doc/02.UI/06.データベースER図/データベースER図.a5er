Page=分析受注関連
Left=2550
Top=100

[View]
PName=V_BUN_JUCHU_LABEL_DATA
LName=分析受注試料ラベルデータビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_BUN_JUCHU_LABEL_DATA(分析受注試料ラベルデータビュー)\nSELECT T_BUN_JUCHU_H.JURI_NO                 AS JURI_NO              --受理No.\n      ,T_BUN_JUCHU_B.JURI_EDA_NO             AS JURI_EDA_NO          --枝番\n      ,T_BUN_JUCHU_H.NYURYOKU_DATETIME       AS NYURYOKU_DATETIME    --入力日時\n      ,T_BUN_JUCHU_B.SHIRYO_SHURUI           AS SHIRYO_SHURUI_MEI    --試料種類\n      ,T_BUN_JUCHU_H.ATENA                   AS ATENA                --報告書宛名\n      ,J_SEIKYU.AITE_MEI                     AS SEIKYU_MEI           --請求先名\n--      ,J_SEIKYU.BUSHO_MEI                    AS SEIKYU_BUSHO_MEI     --請求先部署名\n--      ,J_SEIKYU.TANTO_MEI                    AS SEIKYU_TANTO_MEI     --請求先担当者名\n      ,T_BUN_JUCHU_H.BUSHO_1                 AS HOKOKU_BUSHO_MEI     --報告先部署名\n      ,CASE ISNULL(T_BUN_JUCHU_H.TANTO_1,\q\q)\n            WHEN \q\q THEN \q\q\n            ELSE CASE RIGHT(T_BUN_JUCHU_H.TANTO_1,1)\n                 WHEN \q様\q THEN T_BUN_JUCHU_H.TANTO_1\n                 ELSE T_BUN_JUCHU_H.TANTO_1 + \q様\q\n            END\n       END                                   AS HOKOKU_TANTO_MEI     --報告先担当者名\n      ,T_BUN_JUCHU_H.KANSEI_DATE             AS KANSEI_DATE          --報告書完成日/速報日\n      ,T_BUN_JUCHU_H.KANRYO_DATE             AS TANTO_KANRYO_DATE    --分析担当者完了日\n      ,T_BUN_JUCHU_H.HENKYAKU                AS HENKYAKU             --試料・容器返却\n      ,M_BUN_HOHO.DATA_1                     AS BUNSEKI_HOHO         --分析方法名\n      ,T_BUN_JUCHU_B.SHIRYO_MEI              AS SHIRYO_MEI           --試料名\n      ,J_EIGYO_TANTO.SHIMEI                  AS EIGYO_TANTO_MEI      --営業担当者名\n  FROM T_BUN_JUCHU_H\n INNER JOIN\n       T_BUN_JUCHU_B\n    ON T_BUN_JUCHU_B.JURI_NO = T_BUN_JUCHU_H.JURI_NO\n  --請求先\n  LEFT JOIN\n       M_AITESAKI AS J_SEIKYU\n    ON J_SEIKYU.AITE_CD = T_BUN_JUCHU_H.SEIKYU_CD\n  --分析方法\n  LEFT JOIN\n       M_MEISHO_B                      AS  M_BUN_HOHO\n    ON M_BUN_HOHO.MEISHO_KBN           =   \qK017\q\n   AND M_BUN_HOHO.MEISHO_CD            =   T_BUN_JUCHU_H.BUNSEKI_HOHO_CD\n  --営業担当者名\n  LEFT JOIN\n       M_SHAIN AS J_EIGYO_TANTO\n    ON J_EIGYO_TANTO.SHAIN_CD = T_BUN_JUCHU_H.EIGYO_TANTO_CD\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
