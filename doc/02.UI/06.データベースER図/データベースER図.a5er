[View]
PName=V_URIAGE_JUCHU_DATA
LName=売上接続対象受注データビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_URIAGE_JUCHU_DATA(売上接続対象受注データビュー)\n--分析依頼\nSELECT\n        W_URIAGE_JUCHU.W_USER_ID            AS W_USER_ID                --ユーザーID\n       ,W_URIAGE_JUCHU.W_SERIAL             AS W_SERIAL                 --シリアル\n       ,NULL                                AS W_PAGE                   --ページ番号\n       ,ROW_NUMBER() OVER (PARTITION BY W_URIAGE_JUCHU.JURI_NO ORDER BY GYO_NO)\n                                            AS W_ROW                    --行番号\n       ,0                                   AS W_MODE                   --モード\n       ,NULL                                AS URIAGE_NO                --売上No.\n       ,1                                   AS DEMPYO_KBN               --伝票区分\n       ,2                                   AS RENDO_KBN                --商魂連動区分\n       ,0                                   AS DENKU                    --売上伝区\n       ,TBL_KANJO.DATA_2                    AS KANJO_KAMOKU             --勘定科目\n       ,CONVERT(VARCHAR(10), GETDATE(),111) AS URIAGE_DATE              --売上日\n       ,T_BUN_JUCHU_H.BUSHO_CD              AS BUSHO_CD                 --部署CD\n       ,W_URIAGE_JUCHU.W_USER_ID            AS URIAGE_NYURYOKUSHA_CD    --入力者CD\n       ,T_BUN_JUCHU_H.EIGYO_TANTO_CD        AS EIGYO_TANTO_CD           --営業担当者CD\n       ,W_URIAGE_JUCHU.JUCHU_KBN            AS JUCHU_KBN                --受注区分\n       ,W_URIAGE_JUCHU.JURI_NO              AS JURI_NO                  --受理No.\n       ,T_BUN_JUCHU_H.MITSU_NO              AS MITSU_NO                 --見積No.\n       ,NULL                                AS SAKI_URIAGE_NO           --結合先売上No.\n       ,T_BUN_JUCHU_H.NOHIN_DATE            AS NOHIN_DATE               --納品日\n       ,1                                   AS URIAGE_STS               --売上ステータス\n       ,\qTrue\q                              AS DOJI_FLG                 --同時印刷フラグ\n       ,T_BUN_JUCHU_H.SEIKYU_CD             AS SEIKYU_CD                --請求先CD\n       ,NULL                                AS KAISHU_DATE              --回収日\n       ,T_BUN_JUCHU_H.BUNSEKI_HOHO_CD       AS BUNSEKI_HOHO_CD          --分析方法CD\n       ,T_BUN_JUCHU_H.KENMEI                AS TEKIYO_H                 --摘要(ヘッダ)\n       ,M_AITESAKI.AITE_MEI                 AS DEMPYO_SOFU_MEI          --伝票送付先名\n       ,M_AITESAKI.KEISHO                   AS KEISHO                   --敬称\n       ,M_AITESAKI.YUBIN_NO                 AS YUBIN_NO                 --郵便番号\n       ,M_AITESAKI.ADDRESS_1                AS ADDRESS_1                --住所１\n       ,M_AITESAKI.ADDRESS_2                AS ADDRESS_2                --住所２\n       ,M_AITESAKI.TEL                      AS TEL                      --ＴＥＬ\n       ,M_AITESAKI.FAX                      AS FAX                      --ＦＡＸ\n       ,CASE ISNULL(T_BUN_JUCHU_H.BUSHO_1,\q\q)\n        WHEN \q\q THEN\n          CASE ISNULL(T_BUN_JUCHU_H.TANTO_1,\q\q)\n          WHEN \q\q THEN NULL\n          ELSE T_BUN_JUCHU_H.TANTO_1\n          END\n        ELSE\n          CASE ISNULL(T_BUN_JUCHU_H.TANTO_1,\q\q)\n          WHEN \q\q THEN T_BUN_JUCHU_H.BUSHO_1\n          ELSE T_BUN_JUCHU_H.BUSHO_1 + \q　\q + T_BUN_JUCHU_H.TANTO_1\n          END\n        END                                 AS SOFU_TANTO               --送付先担当者名\n       ,NULL                                AS TEISEI_BIKO              --訂正備考\n       ,\qFalse\q                             AS JUSHO_NO_PRINT_FLG       --住所印字不要フラグ\n       ,NULL                                AS TAX_RATE                 --税率\n       ,T_MITSU_H.NUKI_TOTAL                AS NUKI_TOTAL               --税抜合計\n       ,T_MITSU_H.TAX                       AS TAX                      --消費税\n       ,T_MITSU_H.KOMI_TOTAL                AS KOMI_TOTAL               --税込合計\n       ,T_MITSU_H.GENKA_TOTAL               AS GENKA_TOTAL              --原価合計\n       ,T_MITSU_H.ARARIEKI                  AS ARARIEKI_H               --粗利益(ヘッダ)\n       ,T_MITSU_H.RIEKI_RATE                AS ARARIRITSU_H             --粗利率(ヘッダ)\n       ,NULL                                AS SHOKON_NO                --商魂No.\n       ,NULL                                AS SHOKON_LIST_DATE_TIME    --商魂連動日時\n\n       ,ROW_NUMBER() OVER (PARTITION BY W_URIAGE_JUCHU.JURI_NO ORDER BY GYO_NO)\n                                            AS GYO_NO                   --行番号\n       ,T_MITSU_B.ROW_KBN                   AS ROW_KBN                  --行区分\n       ,CASE T_MITSU_B.SHOHIN_CD\n        WHEN \qnebiki_per\q THEN \qnebiki_dir\q\n        ELSE T_MITSU_B.SHOHIN_CD\n        END                                 AS SHOHIN_CD                --商品CD\n       ,CASE ISNULL(M_SHOHIN.AITE_SHOHIN_MEI,\q\q)\n        WHEN \q\q THEN T_MITSU_B.SHOHIN_MEI\n        ELSE M_SHOHIN.AITE_SHOHIN_MEI\n        END                                 AS SHOHIN_MEI               --商品名\n       ,CASE T_MITSU_B.SHOHIN_CD\n        WHEN \qnebiki_per\q THEN NULL\n        ELSE T_MITSU_B.SURYO\n        END                                 AS URIAGE_SURYO             --売上数量\n       ,CASE T_MITSU_B.SHOHIN_CD\n        WHEN \qnebiki_per\q THEN NULL\n        ELSE T_MITSU_B.TANI\n        END                                 AS URIAGE_SURYO_TANI        --売上数量単位\n       ,T_MITSU_B.TANKA                     AS HAMBAI_TANKA             --販売単価\n       ,T_MITSU_B.KINGAKU                   AS HAMBAI_KINGAKU           --販売金額\n       ,T_MITSU_B.TEKIYO                    AS TEKIYO_B                 --摘要(ボディ)\n       ,T_MITSU_B.GENTANKA                  AS GENTANKA                 --原単価\n       ,CASE T_MITSU_B.SHOHIN_CD\n        WHEN \qnebiki_per\q THEN NULL\n        ELSE T_MITSU_B.SURYO\n        END                                 AS GENKA_SURYO              --原価数量\n       ,M_SHOHIN.SHIIRE_IRISU_TANI          AS IRISU_TANI               --入数単位\n       ,NULL                                AS SOSU                     --総数\n       ,M_SHOHIN.BARA_TANI                  AS BARA_TANI                --バラ単位\n       ,T_MITSU_B.GENKA                     AS GENKA_KINGAKU            --原価金額\n       ,T_MITSU_B.ARARIRITSU                AS ARARIRITSU_B             --粗利率(ボディ)\n       ,T_MITSU_B.ARARIEKI                  AS ARARIEKI_B               --粗利益(ボディ)\n       ,T_MITSU_B.TEIKA                     AS TEIKA                    --定価\n       ,\qFalse\q                             AS DEL_FLG                  --削除フラグ\n\n       ,1                                   AS DBS_STATUS               --DBSステータス\n       ,W_URIAGE_JUCHU.DBS_UPDATE_USER      AS DBS_CREATE_USER          --DBS作成ユーザー\n       ,W_URIAGE_JUCHU.DBS_UPDATE_DATE      AS DBS_CREATE_DATE          --DBS作成日時\n       ,W_URIAGE_JUCHU.DBS_UPDATE_USER      AS DBS_UPDATE_USER          --DBS更新ユーザー\n       ,W_URIAGE_JUCHU.DBS_UPDATE_DATE      AS DBS_UPDATE_DATE          --DBS更新日時\n\n   FROM W_URIAGE_JUCHU\n   LEFT JOIN\n        T_BUN_JUCHU_H\n     ON T_BUN_JUCHU_H.JURI_NO = W_URIAGE_JUCHU.JURI_NO\n   LEFT JOIN\n        T_MITSU_H\n     ON T_MITSU_H.MITSU_NO = T_BUN_JUCHU_H.MITSU_NO\n   LEFT JOIN\n        T_MITSU_B\n     ON T_MITSU_B.MITSU_NO = T_BUN_JUCHU_H.MITSU_NO\n    AND T_MITSU_B.ROW_KBN               <> 4\n    AND T_MITSU_B.SHOHIN_CD             <> \q\q\n   LEFT JOIN\n        M_AITESAKI\n     ON M_AITESAKI.AITE_CD = T_BUN_JUCHU_H.SEIKYU_CD\n   LEFT JOIN\n        M_SHOHIN\n     ON M_SHOHIN.SHOHIN_CD = T_MITSU_B.SHOHIN_CD\n   LEFT JOIN\n        (SELECT * FROM M_MEISHO_B WHERE M_MEISHO_B.MEISHO_KBN = \qK017\q) AS TBL_KANJO\n     ON MEISHO_CD=T_BUN_JUCHU_H.BUNSEKI_HOHO_CD\n  WHERE W_URIAGE_JUCHU.JUCHU_KBN   = 1\n    AND W_URIAGE_JUCHU.SELECT_FLG  = \qTrue\q\n--    AND T_MITSU_B.ROW_KBN          <> 4\n--    AND T_MITSU_B.SHOHIN_CD        <> \q\q\nUNION\n--販売商品\nSELECT\n        W_URIAGE_JUCHU.W_USER_ID            --ユーザーID\n       ,W_URIAGE_JUCHU.W_SERIAL             --シリアル\n       ,NULL                                --ページ番号\n       ,ROW_NUMBER() OVER (PARTITION BY W_URIAGE_JUCHU.JURI_NO ORDER BY GYO_NO)\n                                            --行番号\n       ,0                                   --モード\n       ,NULL                                --売上No.\n       ,1                                   --伝票区分\n       ,2                                   --商魂連動区分\n       ,0                                   --売上伝区 \n       ,(SELECT DATA_1\n         FROM M_MEISHO_B\n            WHERE MEISHO_KBN = \qZ001\q AND MEISHO_CD = 1)      --勘定科目\n--       ,CONVERT(VARCHAR(10), GETDATE(),111) --売上日\n       ,CASE ISNULL(T_HAN_JUCHU_H.NOHIN_DATE,\q\q)\n        WHEN \q\q THEN\n             CONVERT(VARCHAR(10), GETDATE(),111)\n        ELSE\n             T_HAN_JUCHU_H.NOHIN_DATE\n        END                                 --売上日\n       ,27                                  --部署CD\n       ,W_URIAGE_JUCHU.W_USER_ID            --入力者CD\n       ,T_HAN_JUCHU_H.EIGYO_TANTO_CD        --営業担当者CD\n       ,W_URIAGE_JUCHU.JUCHU_KBN            --受注区分\n       ,W_URIAGE_JUCHU.JURI_NO              --受理No.\n       ,T_HAN_JUCHU_H.MITSU_NO              --見積No.\n       ,NULL                                --結合先売上No.\n       ,T_HAN_JUCHU_H.NOHIN_DATE            --納品日\n       ,1                                   --売上ステータス\n       ,\qTrue\q                              --同時印刷フラグ\n       ,T_HAN_JUCHU_H.SEIKYU_CD             --請求先CD\n       ,NULL                                --回収日  \n       ,(SELECT DATA_1\n         FROM M_MEISHO_B\n             WHERE MEISHO_KBN = \qZ002\q AND MEISHO_CD = 1)      --分析方法\n       ,T_HAN_JUCHU_H.KENMEI                --摘要(ヘッダ)\n       ,M_AITESAKI.AITE_MEI                 --伝票送付先名\n       ,M_AITESAKI.KEISHO                   --敬称\n       ,M_AITESAKI.YUBIN_NO                 --郵便番号\n       ,M_AITESAKI.ADDRESS_1                --住所１\n       ,M_AITESAKI.ADDRESS_2                --住所２\n       ,M_AITESAKI.TEL                      --ＴＥＬ\n       ,M_AITESAKI.FAX                      --ＦＡＸ\n       ,T_HAN_JUCHU_H.SEIKYU_TANTO_MEI      --送付先担当者名\n       ,NULL                                --訂正備考\n       ,\qFalse\q                             --住所印字不要フラグ\n       ,NULL                                --税率\n       ,T_HAN_JUCHU_H.NUKI_TOTAL            --税抜合計\n       ,T_HAN_JUCHU_H.TAX                   --消費税\n       ,T_HAN_JUCHU_H.KOMI_TOTAL            --税込合計\n       ,T_HAN_JUCHU_H.GENKA_TOTAL           --原価合計\n       ,T_HAN_JUCHU_H.ARARIEKI              --粗利益(ヘッダ)\n       ,T_HAN_JUCHU_H.ARARIRITSU            --粗利率(ヘッダ)\n       ,NULL                                --商魂No.\n       ,NULL                                --商魂連動日時\n\n       ,ROW_NUMBER() OVER (PARTITION BY W_URIAGE_JUCHU.JURI_NO ORDER BY GYO_NO)\n                                            --行番号\n       ,T_HAN_JUCHU_B.ROW_KBN               --行区分\n       ,T_HAN_JUCHU_B.SHOHIN_CD             --商品CD\n       ,CASE ISNULL(M_SHOHIN.AITE_SHOHIN_MEI,\q\q)\n        WHEN \q\q THEN T_HAN_JUCHU_B.SHOHIN_MEI\n        ELSE M_SHOHIN.AITE_SHOHIN_MEI\n        END                                 --商品名\n       ,T_HAN_JUCHU_B.JUCHU_SURYO           --売上数量\n       ,T_HAN_JUCHU_B.JUCHU_TANI            --売上数量単位\n       ,T_HAN_JUCHU_B.HAMBAI_TANKA          --販売単価\n       ,T_HAN_JUCHU_B.HAMBAI_KINGAKU        --販売金額\n       ,T_HAN_JUCHU_B.TEKIYO                --摘要(ボディ)\n       ,T_HAN_JUCHU_B.GENTANKA              --原単価\n       ,T_HAN_JUCHU_B.GENKA_SURYO           --原価数量\n       ,T_HAN_JUCHU_B.IRISU_TANI            --入数単位\n       ,T_HAN_JUCHU_B.TOTAL                 --総数\n       ,T_HAN_JUCHU_B.BARA_TANI             --バラ単位\n       ,T_HAN_JUCHU_B.GENKA_KINGAKU         --原価金額\n       ,T_HAN_JUCHU_B.ARARIRITSU            --粗利率(ボディ)\n       ,T_HAN_JUCHU_B.ARARIEKI              --粗利益(ボディ)\n       ,T_HAN_JUCHU_B.TEIKA                 --定価\n       ,\qFalse\q                             --削除フラグ\n\n       ,1                                   --DBSステータス\n       ,W_URIAGE_JUCHU.DBS_UPDATE_USER      --DBS作成ユーザー\n       ,W_URIAGE_JUCHU.DBS_UPDATE_DATE      --DBS作成日時\n       ,W_URIAGE_JUCHU.DBS_UPDATE_USER      --DBS更新ユーザー\n       ,W_URIAGE_JUCHU.DBS_UPDATE_DATE      --DBS更新日時\n\n   FROM W_URIAGE_JUCHU\n   LEFT JOIN\n        T_HAN_JUCHU_H\n     ON T_HAN_JUCHU_H.JURI_NO = W_URIAGE_JUCHU.JURI_NO\n   LEFT JOIN\n        T_HAN_JUCHU_B\n     ON T_HAN_JUCHU_B.JURI_NO = T_HAN_JUCHU_H.JURI_NO\n     AND (  T_HAN_JUCHU_B.HAMBAI_KINGAKU <> 0\n         OR T_HAN_JUCHU_B.ROW_KBN = 2 )\n   LEFT JOIN\n        M_AITESAKI\n     ON M_AITESAKI.AITE_CD = T_HAN_JUCHU_H.SEIKYU_CD\n   LEFT JOIN\n        M_SHOHIN\n     ON M_SHOHIN.SHOHIN_CD = T_HAN_JUCHU_B.SHOHIN_CD\n\n  WHERE W_URIAGE_JUCHU.JUCHU_KBN   = 2\n    AND W_URIAGE_JUCHU.SELECT_FLG  = \qTrue\q\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
