Query=--*DataTitle V_BUN_JUCHU_H(分析受注ヘッダビュー)\nSELECT DISTINCT\n       W_BUN_JUCHU.JURI_NO                  AS JURI_NO              -- 受理No.\n      ,W_BUN_JUCHU.BUNSEKI_STS              AS BUNSEKI_STS          -- 分析ステータス\n      ,J_STS.DATA_1                         AS BUNSEKI_STS_MEI      -- 分析ステータス名\n      ,J_JUCHU_KBN.DATA_1                   AS JUCHU_KBN_MEI        -- 分析区分名\n      ,W_BUN_JUCHU.NYURYOKU_DATETIME        AS NYURYOKU_DATETIME    -- 入力日時\n      ,W_BUN_JUCHU.BUSHO_CD                 AS BUSHO_CD             -- 分析担当部署CD\n      ,J_BUSHO.DATA_1                       AS BUSHO_MEI            -- 分析担当部署名\n      ,W_BUN_JUCHU.NYURYOKUSHA_CD           AS NYURYOKUSHA_CD       -- 入力者CD\n      ,J_NYURYOKUSHA.SHIMEI                 AS NYURYOKUSHA_MEI      -- 入力者名\n      ,W_BUN_JUCHU.REF_JURI_NO              AS REF_JURI_NO          -- 参照元受理No.\n      ,W_BUN_JUCHU.MITSU_NO                 AS MITSU_NO             -- 見積No.\n      ,W_BUN_JUCHU.ATENA_CD                 AS ATENA_CD             -- 報告書宛名CD\n      ,W_BUN_JUCHU.ATENA                    AS ATENA                -- 報告書宛名\n      ,W_BUN_JUCHU.SEIKYU_CD                AS SEIKYU_CD            -- 請求先CD\n      ,J_SEIKYU.AITE_MEI                    AS SEIKYU_MEI           -- 請求先名\n      ,W_BUN_JUCHU.BUNSEKI_HOHO_CD          AS BUNSEKI_HOHO_CD      -- 分析方法CD\n      ,M_BUN_HOHO.DATA_1                    AS BUNSEKI_HOHO         -- 分析方法名\n      ,J_KEISHIKI_KBN.DATA_1                AS KEISHIKI_KBN_MEI     -- 報告書形式名\n      ,W_BUN_JUCHU.NOHIN_HOHO               AS NOHIN_HOHO           -- 納品方法実績\n      ,W_BUN_JUCHU.KOSU                     AS KOSU                 -- 工数\n      ,W_BUN_JUCHU.EIGYO_TANTO_CD           AS EIGYO_TANTO_CD       -- 営業担当者CD\n      ,J_EIGYO_TANTO.SHIMEI                 AS EIGYO_TANTO_MEI      -- 営業担当者名\n      ,W_BUN_JUCHU.IRAI_DATE                AS IRAI_DATE            -- 依頼日／受付日\n      ,W_BUN_JUCHU.KANRYO_DATE              AS KANRYO_DATE          -- 分析担当者完了日\n      ,W_BUN_JUCHU.KANSEI_DATE              AS KANSEI_DATE          -- 報告書完成日／速報日\n      ,J_SOKUHO.DATA_1                      AS SOKUHO_KBN_MEI       -- 速報区分名\n      ,W_BUN_JUCHU.HAKKO_DATE               AS HAKKO_DATE           -- 報告書指定発行日\n      ,W_BUN_JUCHU.NOHIN_DATE               AS NOHIN_DATE           -- 納品日\n      ,W_BUN_JUCHU.KENMEI                   AS KENMEI               -- 件名\n      ,W_BUN_JUCHU.MOKUTEKI                 AS MOKUTEKI             -- 目的\n      ,W_BUN_JUCHU.KISAI_JIKO               AS KISAI_JIKO           -- 報告書記載事項\n      ,W_BUN_JUCHU.EDA_SHIRYO_FLG           AS EDA_SHIRYO_FLG       -- 枝番試料登録フラグ\n      ,W_BUN_JUCHU.SAISHU_KAISHA_MEI        AS SAISHU_KAISHA_MEI    -- 採取会社名\n      ,W_BUN_JUCHU.SAISHU_SHA               AS SAISHU_SHA           -- 採取者\n      ,W_BUN_JUCHU.SAISHU_HOHO              AS SAISHU_HOHO          -- 採取方法\n      ,W_BUN_JUCHU.SAISHU_BASHO             AS SAISHU_BASHO         -- 採取場所\n      ,W_BUN_JUCHU.TENKO                    AS TENKO                -- 天候\n      ,W_BUN_JUCHU.KION                     AS KION                 -- 気温\n      ,W_BUN_JUCHU.SUION                    AS SUION                -- 水温\n      ,W_BUN_JUCHU.SAISHU_DATE              AS SAISHU_DATE          -- 採取日\n      ,W_BUN_JUCHU.SAISHU_TIME              AS SAISHU_TIME          -- 採取時間\n      ,W_BUN_JUCHU.SHOKEN_FLG               AS SHOKEN_FLG           -- 報告書所見フラグ\n      ,J_GAISO.DATA_1                       AS GAISO_KBN_MEI        -- 報告書外装\n      ,W_BUN_JUCHU.YOKI                     AS YOKI                 -- 容器種類\n      ,W_BUN_JUCHU.HENKYAKU                 AS HENKYAKU             -- 試料・容器返却\n      ,J_HOKOKU_HOHO.DATA_1                 AS HOKOKU_HOHO_MEI      -- 報告方法\n      ,W_BUN_JUCHU.BUSU                     AS BUSU                 -- 発行部数\n      ,W_BUN_JUCHU.CHUKAN_DATE_1            AS CHUKAN_DATE_1        -- 中間報告日１\n      ,W_BUN_JUCHU.CHUKAN_NAIYO_1           AS CHUKAN_NAIYO_1       -- 中間報告内容１\n      ,W_BUN_JUCHU.CHUKAN_DATE_2            AS CHUKAN_DATE_2        -- 中間報告日２\n      ,W_BUN_JUCHU.CHUKAN_NAIYO_2           AS CHUKAN_NAIYO_2       -- 中間報告内容２\n      ,W_BUN_JUCHU.CHUKAN_DATE_3            AS CHUKAN_DATE_3        -- 中間報告日３\n      ,W_BUN_JUCHU.CHUKAN_NAIYO_3           AS CHUKAN_NAIYO_3       -- 中間報告内容３\n      ,W_BUN_JUCHU.BIKO                     AS BIKO                 -- 備考\n      ,W_BUN_JUCHU.FUYO_FLG                 AS FUYO_FLG             -- 営業確認不要フラグ\n      ,W_BUN_JUCHU.NEW_FLG                  AS NEW_FLG              -- 新規フラグ\n      ,W_BUN_JUCHU.HENKO_FLG                AS HENKO_FLG            -- 記載内容変更予定フラグ\n      ,W_BUN_JUCHU.HOKOKU_CD_1              AS HOKOKU_CD_1          -- 報告先CD１\n      ,W_BUN_JUCHU.HOKOKU_MEI_1             AS HOKOKU_MEI_1         -- 報告先名１\n      ,W_BUN_JUCHU.BUSHO_1                  AS BUSHO_1              -- 部署１\n      ,W_BUN_JUCHU.TANTO_1                  AS TANTO_1              -- 担当者１\n      ,W_BUN_JUCHU.TEL_1                    AS TEL_1                -- TEL１\n      ,W_BUN_JUCHU.FAX_1                    AS FAX_1                -- FAX１\n      ,W_BUN_JUCHU.MAIL_1                   AS MAIL_1               -- E-MAIL１\n      ,W_BUN_JUCHU.YUBIN_NO_1               AS YUBIN_NO_1           -- 郵便番号１\n      ,W_BUN_JUCHU.ADDRESS_1                AS ADDRESS_1            -- 住所１\n      ,W_BUN_JUCHU.CHUKAN_SOFU_FLG_1        AS CHUKAN_SOFU_FLG_1    -- 中間報告送付フラグ１\n      ,W_BUN_JUCHU.SOKUHO_SOFU_FLG_1        AS SOKUHO_SOFU_FLG_1    -- 速報送付フラグ１\n      ,W_BUN_JUCHU.HOKOKUSHO_SOFU_FLG_1     AS HOKOKUSHO_SOFU_FLG_1 -- 報告書送付フラグ１\n      ,W_BUN_JUCHU.HOKOKU_CD_2              AS HOKOKU_CD_2          -- 報告先CD２\n      ,W_BUN_JUCHU.HOKOKU_MEI_2             AS HOKOKU_MEI_2         -- 報告先名２\n      ,W_BUN_JUCHU.BUSHO_2                  AS BUSHO_2              -- 部署２\n      ,W_BUN_JUCHU.TANTO_2                  AS TANTO_2              -- 担当者２\n      ,W_BUN_JUCHU.TEL_2                    AS TEL_2                -- TEL２\n      ,W_BUN_JUCHU.FAX_2                    AS FAX_2                -- FAX２\n      ,W_BUN_JUCHU.MAIL_2                   AS MAIL_2               -- E-MAIL２\n      ,W_BUN_JUCHU.YUBIN_NO_2               AS YUBIN_NO_2           -- 郵便番号２\n      ,W_BUN_JUCHU.ADDRESS_2                AS ADDRESS_2            -- 住所２\n      ,W_BUN_JUCHU.CHUKAN_SOFU_FLG_2        AS CHUKAN_SOFU_FLG_2    -- 中間報告送付フラグ２\n      ,W_BUN_JUCHU.SOKUHO_SOFU_FLG_2        AS SOKUHO_SOFU_FLG_2    -- 速報送付フラグ２\n      ,W_BUN_JUCHU.HOKOKUSHO_SOFU_FLG_2     AS HOKOKUSHO_SOFU_FLG_2 -- 報告書送付フラグ２\n      ,W_BUN_JUCHU.HOKOKU_CD_3              AS HOKOKU_CD_3          -- 報告先CD３\n      ,W_BUN_JUCHU.HOKOKU_MEI_3             AS HOKOKU_MEI_3         -- 報告先名３\n      ,W_BUN_JUCHU.BUSHO_3                  AS BUSHO_3              -- 部署３\n      ,W_BUN_JUCHU.TANTO_3                  AS TANTO_3              -- 担当者３\n      ,W_BUN_JUCHU.TEL_3                    AS TEL_3                -- TEL３\n      ,W_BUN_JUCHU.FAX_3                    AS FAX_3                -- FAX３\n      ,W_BUN_JUCHU.MAIL_3                   AS MAIL_3               -- E-MAIL３\n      ,W_BUN_JUCHU.YUBIN_NO_3               AS YUBIN_NO_3           -- 郵便番号３\n      ,W_BUN_JUCHU.ADDRESS_3                AS ADDRESS_3            -- 住所３\n      ,W_BUN_JUCHU.CHUKAN_SOFU_FLG_3        AS CHUKAN_SOFU_FLG_3    -- 中間報告送付フラグ３\n      ,W_BUN_JUCHU.SOKUHO_SOFU_FLG_3        AS SOKUHO_SOFU_FLG_3    -- 速報送付フラグ３\n      ,W_BUN_JUCHU.HOKOKUSHO_SOFU_FLG_3     AS HOKOKUSHO_SOFU_FLG_3 -- 報告書送付フラグ３\n      ,J_DEMPYO_KBN.DATA_1                  AS DEMPYO_HAKKO_KBN     -- 伝票発行区分  \n\t  ,W_BUN_JUCHU.BUN_SORT_SEQ             AS BUN_SORT_SEQ         -- 分析項目並び順\n      ,W_BUN_JUCHU.W_USER_ID                AS W_USER_ID            -- ユーザーID\n      ,W_BUN_JUCHU.W_SERIAL                 AS W_SERIAL             -- シリアル\n  FROM W_BUN_JUCHU\n  --ステータス\n  LEFT JOIN\n       M_MEISHO_B AS J_STS\n    ON J_STS.MEISHO_KBN = \qS003\q\n   AND J_STS.MEISHO_CD  = W_BUN_JUCHU.BUNSEKI_STS\n  --分析区分\n  LEFT JOIN\n       M_MEISHO_B AS J_JUCHU_KBN\n    ON J_JUCHU_KBN.MEISHO_KBN = \qK012\q\n   AND J_JUCHU_KBN.MEISHO_CD  = W_BUN_JUCHU.JUCHU_KBN\n   --部署名\n  LEFT JOIN\n       M_MEISHO_B AS J_BUSHO\n    ON J_BUSHO.MEISHO_KBN = \qM001\q\n   AND J_BUSHO.MEISHO_CD  = W_BUN_JUCHU.BUSHO_CD\n  --入力者名\n  LEFT JOIN\n       M_SHAIN AS J_NYURYOKUSHA\n    ON J_NYURYOKUSHA.SHAIN_CD = W_BUN_JUCHU.NYURYOKUSHA_CD\n  --請求先名\n  LEFT JOIN\n       M_AITESAKI AS J_SEIKYU\n    ON J_SEIKYU.AITE_CD = W_BUN_JUCHU.SEIKYU_CD\n  --分析方法\n  LEFT JOIN\n       M_MEISHO_B                      AS  M_BUN_HOHO\n    ON M_BUN_HOHO.MEISHO_KBN           =   \qK017\q\n   AND M_BUN_HOHO.MEISHO_CD            =   W_BUN_JUCHU.BUNSEKI_HOHO_CD\n  --報告書形式\n  LEFT JOIN\n       M_MEISHO_B AS J_KEISHIKI_KBN\n    ON J_KEISHIKI_KBN.MEISHO_KBN = \qK013\q\n   AND J_KEISHIKI_KBN.MEISHO_CD  = W_BUN_JUCHU.KEISHIKI_KBN\n  --営業担当者名\n  LEFT JOIN\n       M_SHAIN AS J_EIGYO_TANTO\n    ON J_EIGYO_TANTO.SHAIN_CD = W_BUN_JUCHU.EIGYO_TANTO_CD\n  --速報区分\n  LEFT JOIN\n       M_MEISHO_B AS J_SOKUHO\n    ON J_SOKUHO.MEISHO_KBN = \qK027\q\n   AND J_SOKUHO.MEISHO_CD  = W_BUN_JUCHU.SOKUHO_KBN\n  --報告書外装区分\n  LEFT JOIN\n       M_MEISHO_B AS J_GAISO\n    ON J_GAISO.MEISHO_KBN = \qK014\q\n   AND J_GAISO.MEISHO_CD  = W_BUN_JUCHU.GAISO_KBN\n  --報告方法区分\n  LEFT JOIN\n       M_MEISHO_B AS J_HOKOKU_HOHO\n    ON J_HOKOKU_HOHO.MEISHO_KBN = \qK016\q\n   AND J_HOKOKU_HOHO.MEISHO_CD  = W_BUN_JUCHU.HOKOKU_HOHO_KBN\n  --伝票発行区分\n  LEFT JOIN\n       M_MEISHO_B AS J_DEMPYO_KBN\n    ON J_DEMPYO_KBN.MEISHO_KBN = \qK026\q\n   AND J_DEMPYO_KBN.MEISHO_CD  = W_BUN_JUCHU.DEMPYO_HAKKO_KBN\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="分析受注関連",100,100
Page=分析受注関連
Left=100
Top=100

[View]
PName=V_BUN_JUCHU_SHOSAI
LName=分析受注詳細ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT W_BUN_JUCHU.GYO_NO               AS GYO_NO               --行No.\n      ,W_BUN_JUCHU.BUNSEKI_CD           AS BUNSEKI_CD           --分析項目CD\n      ,W_BUN_JUCHU.BUNSEKI_MEI          AS BUNSEKI_MEI          --分析項目名\n      ,W_BUN_JUCHU.BUNSEKI_TANI         AS BUNSEKI_TANI         --単位\n      ,W_BUN_JUCHU.BUNSEKI_DATA         AS BUNSEKI_DATA         --データ\n      ,NULL                             AS BLANK1               --結合セル用空白列１\n      ,NULL                             AS BLANK2               --結合セル用空白列２\n      ,NULL                             AS BLANK3               --結合セル用空白列３\n      ,W_BUN_JUCHU.BUNSEKI_NO           AS BUNSEKI_NO           --分析番号\n      ,NULL                             AS BLANK4               --結合セル用空白列４\n      ,W_BUN_JUCHU.SEQ                  AS SEQ                  --シーケンス\n      ,W_BUN_JUCHU.JURI_NO              AS JURI_NO              --受理No.\n      ,W_BUN_JUCHU.REF_JURI_NO          AS REF_JURI_NO          --参照元受理No.\n      ,W_BUN_JUCHU.MITSU_NO             AS MITSU_NO             --見積No.\n      ,W_BUN_JUCHU.JURI_EDA_NO          AS JURI_EDA_NO          --枝番\n      ,W_BUN_JUCHU.URAGAKI_PRINT_FLG    AS URAGAKI_PRINT_FLG    --裏書印刷済フラグ\n      ,W_BUN_JUCHU.W_USER_ID            AS W_USER_ID            --ユーザーID\n      ,W_BUN_JUCHU.W_SERIAL             AS W_SERIAL             --シリアル\n\n  FROM W_BUN_JUCHU\n WHERE W_BUN_JUCHU.BUNSEKI_CD IS NOT NULL\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="分析受注関連",700,700
Page=分析受注関連
Left=700
Top=700

[View]
PName=V_BUN_JUCHU_B
LName=分析受注ボディビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT DISTINCT\n       W_BUN_JUCHU.JURI_NO              AS JURI_NO              --受理No.\n      ,W_BUN_JUCHU.JURI_EDA_NO          AS JURI_EDA_NO          --枝番\n      ,W_BUN_JUCHU.SAISHU_KAISHA_MEI_B  AS SAISHU_KAISHA_MEI    --採取会社名\n      ,W_BUN_JUCHU.SAISHU_SHA_B         AS SAISHU_SHA           --採取者\n      ,W_BUN_JUCHU.SAISHU_HOHO_B        AS SAISHU_HOHO          --採取方法\n      ,W_BUN_JUCHU.SAISHU_BASHO_B       AS SAISHU_BASHO         --採取場所\n      ,W_BUN_JUCHU.TENKO_B              AS TENKO                --天候\n      ,W_BUN_JUCHU.KION_B               AS KION                 --気温\n      ,W_BUN_JUCHU.SUION_B              AS SUION                --水温\n      ,W_BUN_JUCHU.SAISHU_DATE_B        AS SAISHU_DATE          --採取日\n      ,W_BUN_JUCHU.SAISHU_TIME_B        AS SAISHU_TIME          --採取時間\n      ,W_BUN_JUCHU.SHIRYO_SHURUI        AS SHIRYO_SHURUI_MEI    --試料種類\n      ,W_BUN_JUCHU.SHIRYO_MEI           AS SHIRYO_MEI           --試料名\n      ,W_BUN_JUCHU.IKKATSU_M            AS IKKATSU_M            --一括記載m(左辺)\n      ,W_BUN_JUCHU.IKKATSU_N            AS IKKATSU_N            --一括記載n(右辺)\n      ,W_BUN_JUCHU.BUNSEKI_GUN_CD       AS BUNSEKI_GUN_CD       --分析項目群CD\n      ,J_BUNSEKI_GUN.BUNSEKI_GUN_MEI    AS BUNSEKI_GUN_MEI      --分析項目群名\n      ,W_BUN_JUCHU.EBIKO                AS EBIKO                --枝番備考\n      ,W_BUN_JUCHU.W_USER_ID            AS W_USER_ID            --ユーザーID\n      ,W_BUN_JUCHU.W_SERIAL             AS W_SERIAL             --シリアル\n      ,W_BUN_JUCHU.REF_JURI_NO          AS REF_JURI_NO          --参照元受理No.\n      ,W_BUN_JUCHU.MITSU_NO             AS MITSU_NO             --見積No.\n\n  FROM W_BUN_JUCHU\n  --分析項目群\n  LEFT JOIN\n       M_BUNSEKI_GUN_H AS J_BUNSEKI_GUN\n    ON J_BUNSEKI_GUN.BUNSEKI_GUN_CD = W_BUN_JUCHU.BUNSEKI_GUN_CD\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="分析受注関連",700,100
Page=分析受注関連
Left=700
Top=100

[View]
PName=V_BUN_JUCHU_B_EXCEL
