[View]
PName=V_HAN_JUCHU_DOKU_DATA
LName=毒劇物譲受書データビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT T_HAN_JUCHU_H.JURI_NO            AS JURI_NO              --受理No.\n      ,T_HAN_JUCHU_B.GYO_NO             AS GYO_NO               --行No.\n      ,M_SHOHIN.DOKU_MEISHO             AS DOKU_MEISHO          --毒劇物名称\n      ,M_SHOHIN.IRISU                   AS IRISU                --入数\n      ,T_HAN_JUCHU_B.IRISU_TANI         AS IRISU_TANI           --入数単位\n      ,T_HAN_JUCHU_B.JUCHU_SURYO        AS JUCHU_SURYO          --受注数量\n      ,T_HAN_JUCHU_B.JUCHU_TANI         AS JUCHU_TANI           --受注数量単位\n      ,M_SHOHIN.DOKU_SURYO              AS DOKU_SURYO           --毒劇物基本数量\n      ,T_HAN_JUCHU_H.NOHIN_DATE         AS NOHIN_DATE           --納品日\n      ,T_HAN_JUCHU_H.SEIKYU_TANTO_MEI   AS SEIKYU_TANTO_MEI     --相手先担当者名\n      ,M_AITESAKI.AITE_MEI              AS SEIKYUSAKI_MEI       --請求先名\n      ,M_AITESAKI.ADDRESS_1 +\n       M_AITESAKI.ADDRESS_2             AS JUSHO\n      ,M_SHOHIN.BIKO                    AS BIKO\n  FROM T_HAN_JUCHU_H\n\n  --ボディ\n  LEFT JOIN\n       T_HAN_JUCHU_B\n    ON T_HAN_JUCHU_B.JURI_NO = T_HAN_JUCHU_H.JURI_NO\n  --商品\n  LEFT JOIN\n       M_SHOHIN\n    ON M_SHOHIN.SHOHIN_CD = T_HAN_JUCHU_B.SHOHIN_CD\n  --請求先\n  LEFT JOIN\n       M_AITESAKI\n    ON M_AITESAKI.AITE_CD = T_HAN_JUCHU_H.SEIKYU_CD\n\n WHERE T_HAN_JUCHU_B.ROW_KBN = 1\n   AND M_SHOHIN.DOKU_FLG     = \qTrue\q\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="販売商品関連",3600,600
Page=販売商品関連
Left=3600
Top=600

[View]
PName=V_HAN_JUCHU_SHIYAKU_DATA
LName=調製試薬注文票データビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT T_HAN_JUCHU_H.JURI_NO            AS JURI_NO              --受理No.\n      ,T_HAN_JUCHU_B.GYO_NO             AS GYO_NO               --行No.\n      ,M_AITESAKI.AITE_MEI              AS SEIKYUSAKI_MEI       --請求先名\n      ,T_HAN_JUCHU_H.SEIKYU_TANTO_MEI   AS SEIKYU_TANTO_MEI     --相手先担当者名\n      ,M_SHOHIN.SHIYAKU_MEI             AS SHIYAKU_MEI          --調製試薬名\n      ,M_SHOHIN.SHIYAKU_NAIYO           AS SHIYAKU_NAIYO        --調製試薬内容\n      ,CASE M_SHOHIN.DOKU_FLG\n       WHEN \qTrue\q THEN \q○\q\n       ELSE NULL\n       END                              AS DOKU_FLG             --毒劇物フラグ\n      ,T_HAN_JUCHU_B.JUCHU_SURYO        AS SURYO                --数量\n      ,T_HAN_JUCHU_B.JUCHU_TANI         AS TANI                 --数量単位\n      ,M_SHOHIN.IRISU                   AS IRISU                --入数\n      ,M_SHOHIN.BARA_TANI               AS BARA_TANI            --バラ単位\n      ,T_HAN_JUCHU_H.NOHIN_DATE         AS NOHIN_DATE           --納品日\n      ,M_SHAIN.SHIMEI                   AS EIGYO_TANTO_MEI      --営業担当者名\n      ,M_SHOHIN.BIKO                    AS BIKO                 --備考\n      ,T_HAN_JUCHU_B.TEKIYO             AS TEKIYO               --摘要\n  FROM T_HAN_JUCHU_H\n\n  --ボディ\n  LEFT JOIN\n       T_HAN_JUCHU_B\n    ON T_HAN_JUCHU_B.JURI_NO = T_HAN_JUCHU_H.JURI_NO\n  --商品\n  LEFT JOIN\n       M_SHOHIN\n    ON M_SHOHIN.SHOHIN_CD = T_HAN_JUCHU_B.SHOHIN_CD\n  --請求先\n  LEFT JOIN\n       M_AITESAKI\n    ON M_AITESAKI.AITE_CD = T_HAN_JUCHU_H.SEIKYU_CD\n  --営業担当者\n  LEFT JOIN\n       M_SHAIN\n    ON M_SHAIN.SHAIN_CD = T_HAN_JUCHU_H.EIGYO_TANTO_CD\n\n WHERE T_HAN_JUCHU_B.ROW_KBN       = 1\n   AND M_SHOHIN.CHOSEI_SHIYAKU_FLG = \qTrue\q\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="販売商品関連",3600,100
Page=販売商品関連
Left=3600
Top=100

