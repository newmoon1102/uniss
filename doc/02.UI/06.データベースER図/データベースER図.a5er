[View]
PName=V_C_SHIIRE_B
LName=仕入ボディ共通ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_C_SHIIRE_B(仕入ボディ共通ビュー)\nSELECT TBL_A.SHIIRE_NO                 AS  SHIIRE_NO                 -- 仕入No.\n      ,TBL_A.GYO_NO                    AS  GYO_NO                    -- 行No.\n      ,TBL_A.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_A.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_A.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_A.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_A.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_A.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_A.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_A.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_A.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_A.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,M_MAKER_CD.AITE_MEI             AS  MAKER_MEI                 -- メーカー名\n      ,TBL_A.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,M_YOSAN_CD.YOSAN_MEI            AS  YOSAN_MEI                 -- 予算名\n      ,TBL_A.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_A.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_A.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_A.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_B.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,  \q|\q + ISNULL(TBL_A.KOBAIHIN_CD  ,\q\q)       -- 購買品CD\n       + \q|\q + ISNULL(TBL_A.KOBAIHIN_MEI ,\q\q)       -- 購買品名\n       + \q|\q + ISNULL(TBL_A.MAKER_CD     ,\q\q)       -- メーカーCD\n       + \q|\q + ISNULL(M_MAKER_CD.AITE_MEI,\q\q)       -- メーカー名\n       + \q|\q + ISNULL(TBL_A.BIKO         ,\q\q)       -- 備考\n       + \q|\q + ISNULL(TBL_A.JURI_NO      ,\q\q)       -- 受理No.\n       + \q|\q + ISNULL(TBL_A.YOSAN_CD     ,\q\q)       -- 予算コード\n       + \q|\q + ISNULL(TBL_A.KOBAI_NO     ,\q\q)       -- 購買申請No.\n       + \q|\q + ISNULL(TBL_B.IRAI_NO      ,\q\q)       -- 依頼No.\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_SHIIRE_B                      AS  TBL_A                     -- 【購買申請ボディテーブル】\n  LEFT JOIN                                                          -- 【依頼No.用】\n       T_IRAI                          AS  TBL_B\n    ON TBL_B.KOBAI_NO                  =   TBL_A.KOBAI_NO\n   AND TBL_B.KOBAI_SEQ                 =   TBL_A.KOBAI_SEQ\n  LEFT JOIN                                                          -- 【メーカーCD用】\n       M_AITESAKI                      AS  M_MAKER_CD\n    ON M_MAKER_CD.AITE_CD              =   TBL_A.MAKER_CD\n  LEFT JOIN                                                          -- 【予算コード用】\n       M_YOSAN                         AS  M_YOSAN_CD\n    ON M_YOSAN_CD.YOSAN_CD             =   TBL_A.YOSAN_CD\n\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="仕入関連",100,1000
Page=仕入関連
Left=100
Top=1000

[View]
PName=V_C_SHIIRE_H
LName=仕入ヘッダ共通ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_C_SHIIRE_H(仕入ヘッダ共通ビュー)\nSELECT TBL_A.SHIIRE_NO                 AS  SHIIRE_NO                 -- 仕入No.\n      ,TBL_A.SHIIRE_DATE               AS  SHIIRE_DATE               -- 仕入日\n      ,TBL_A.SHIIRE_KAMOKU             AS  SHIIRE_KAMOKU             -- 仕入科目\n      ,M_SHIIRE_KAMOKU.DATA_1          AS  SHIIRE_KAMOKU_MEI         -- 仕入科目名\n      ,TBL_A.DENKU                     AS  DENKU_KBN                 -- 伝区\n      ,M_DENKU.DATA_1                  AS  DENKU_KBN_MEI             -- 伝区名\n      ,TBL_A.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,M_KOBAI_KBN.DATA_1              AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_A.KANJO_KAMOKU              AS  KANJO_KAMOKU              -- 勘定科目\n      ,M_KANJO_KAMOKU.DATA_1           AS  KANJO_KAMOKU_MEI          -- 勘定科目名\n      ,TBL_A.SHIIRE_NYURYOKUSHA_CD     AS  SHIIRE_NYURYOKUSHA_CD     -- 仕入入力者CD\n      ,M_SHIIRE_NYURYOKUSHA.SHIMEI     AS  SHIIRE_NYURYOKUSHA_MEI    -- 仕入入力者名\n      ,TBL_A.SHIHARAI_DATE             AS  SHIHARAI_DATE             -- 支払予定日\n      ,TBL_A.SHIIRE_STS                AS  SHIIRE_STS                -- 仕入ステータス\n      ,M_SHIIRE_STS.DATA_1             AS  SHIIRE_STS_MEI            -- 仕入ステータス名\n      ,TBL_A.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,M_SHIIRE_CD.AITE_MEI            AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_A.TAX_RATE                  AS  TAX_RATE                  -- 税率\n      ,TBL_A.NUKI_TOTAL                AS  NUKI_TOTAL                -- 税抜合計\n      ,TBL_A.TAX                       AS  TAX                       -- 消費税\n      ,TBL_A.KOMI_TOTAL                AS  KOMI_TOTAL                -- 税込合計\n      ,TBL_A.SHOKAN_NO                 AS  SHOKAN_NO                 -- 商管伝票No.\n      ,TBL_A.SHOKAN_LINK_DATETIME      AS  SHOKAN_LINK_DATETIME      -- 連動日時\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,  \q|\q + M_SHIIRE_CD.AITE_MEI        -- 仕入先名\n                                       AS  FREEWD1                   -- フリーワード検索用１\n      ,  \q|\q + M_SHIIRE_CD.AITE_MEI        -- 仕入先名\n       + \q|\q + TBL_F.FREEWD                -- フリーワード(ボディ)\n                                       AS  FREEWD2                   -- フリーワード検索用２\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_SHIIRE_H                      AS  TBL_A                     -- 【仕入ヘッダテーブル】\n  LEFT JOIN                                                          -- 【仕入科目用】\n       M_MEISHO_B                      AS  M_SHIIRE_KAMOKU\n    ON M_SHIIRE_KAMOKU.MEISHO_KBN      =   \qK034\q\n   AND M_SHIIRE_KAMOKU.MEISHO_CD       =   TBL_A.SHIIRE_KAMOKU\n  LEFT JOIN                                                          -- 【伝区用】\n       M_MEISHO_B                      AS  M_DENKU\n    ON M_DENKU.MEISHO_KBN              =   \qK035\q\n   AND M_DENKU.MEISHO_CD               =   TBL_A.DENKU\n  LEFT JOIN                                                          -- 【購買区分用】\n       M_MEISHO_B                      AS  M_KOBAI_KBN\n    ON M_KOBAI_KBN.MEISHO_KBN          =   \qK021\q\n   AND M_KOBAI_KBN.MEISHO_CD           =   TBL_A.KOBAI_KBN\n  LEFT JOIN                                                          -- 【勘定科目用】\n       M_MEISHO_B                      AS  M_KANJO_KAMOKU\n    ON M_KANJO_KAMOKU.MEISHO_KBN       =   \qK036\q\n   AND M_KANJO_KAMOKU.MEISHO_CD        =   TBL_A.KANJO_KAMOKU\n  LEFT JOIN                                                          -- 【仕入入力者CD用】\n       M_SHAIN                         AS  M_SHIIRE_NYURYOKUSHA\n    ON M_SHIIRE_NYURYOKUSHA.SHAIN_CD   =   TBL_A.SHIIRE_NYURYOKUSHA_CD\n  LEFT JOIN                                                          -- 【仕入ステータス用】\n       M_MEISHO_B                      AS  M_SHIIRE_STS\n    ON M_SHIIRE_STS.MEISHO_KBN         =   \qS006\q\n   AND M_SHIIRE_STS.MEISHO_CD          =   TBL_A.SHIIRE_STS\n  LEFT JOIN                                                          -- 【仕入先用】\n       M_AITESAKI                      AS  M_SHIIRE_CD\n    ON M_SHIIRE_CD.AITE_CD             =   TBL_A.SHIIRE_CD\n  LEFT JOIN                                                          -- 【フリーワード(ボディ)】\n     (\n       SELECT TBL_A.SHIIRE_NO\n             ,(                                    -- フリーワード検索用\n               SELECT \q|\q + TBL_B.FREEWD         AS  [data()]\n                 FROM V_C_SHIIRE_B               AS  TBL_B\n                WHERE TBL_B.SHIIRE_NO            =   TBL_A.SHIIRE_NO\n                FOR XML PATH (\q\q)\n              )\n                                                 AS  FREEWD\n         FROM V_C_SHIIRE_B TBL_A\n        GROUP BY\n              TBL_A.SHIIRE_NO\n     ) TBL_F\n    ON TBL_F.SHIIRE_NO                 =   TBL_A.SHIIRE_NO\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="仕入関連",100,100
Page=仕入関連
Left=100
Top=100

[View]
PName=V_URIAGE_JUCHU_LIST
LName=受注データ検索ビュー
Tag=ビュー
Comment=UNNHG0010【受注データ検索】
ViewOption=
Query=--*DataTitle V_URIAGE_JUCHU_LIST(受注データ検索ビュー)\nSELECT M_JUCHU_KBN.DATA_1              AS  JUCHU_KBN_MEI             -- 受注区分名\n      ,TBL_J.STS_MEI                   AS  STS_MEI                   -- ステータス名\n      ,TBL_J.BUN_HAN_KBN_MEI           AS  BUN_HAN_KBN_MEI           -- 分析販売区分名\n      ,TBL_J.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_J.KENMEI                    AS  KENMEI                    -- 件名\n      ,TBL_J.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,TBL_J.SEIKYU_MEI                AS  SEIKYU_MEI                -- 請求先名\n      ,M_NYURYOKUSHA.SHIMEI            AS  NYURYOKUSHA_MEI           -- 入力者名\n      ,M_EIGYO_TANTO.SHIMEI            AS  EIGYO_TANTO_MEI           -- 営業担当者名\n      ,TBL_J.NYURYOKU_DATETIME         AS  NYURYOKU_DATETIME         -- 入力日時\n      ,TBL_J.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_J.EDA_KENSU                 AS  EDA_KENSU                 -- 枝番件数\n      ,TBL_J.SHOHIN_KENSU              AS  SHOHIN_KENSU              -- 商品件数\n      ,TBL_J.MITSU_NO                  AS  MITSU_NO                  -- 見積No.\n      ,TBL_U.URIAGE_NO_S               AS  URIAGE_NO_S               -- 売上No.\n      ,TBL_J.HASSO_CD                  AS  HASSO_CD                  -- 発送先CD\n      ,TBL_J.JUCHU_KBN                 AS  JUCHU_KBN                 -- 受注区分\n      ,TBL_J.STS                       AS  STS                       -- ステータス\n      ,TBL_J.BUN_HAN_KBN               AS  BUN_HAN_KBN               -- 分析販売区分\n      ,TBL_J.NYURYOKUSHA_CD            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,TBL_J.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,CASE TBL_J.DEMPYO_HAKKO_KBN\n       WHEN \q1\q THEN \qFalse\q\n       WHEN \q3\q THEN \qFalse\q\n       ELSE \n          (SELECT CASE COUNT(*)\n                  WHEN 0\n                  THEN \qTrue\q\n                  ELSE \qFalse\q\n                  END\n             FROM T_URIAGE_H TBL_UH\n             LEFT JOIN\n                  T_URIAGE_JUCHU_SELECT TBL_UJ\n               ON TBL_UJ.URIAGE_NO           =   TBL_UH.URIAGE_NO\n            WHERE (    TBL_UH.JURI_NO        =   TBL_J.JURI_NO\n                    OR TBL_UJ.JURI_NO        =   TBL_J.JURI_NO\n                  )\n              AND TBL_UH.DEL_FLG           =   \qFalse\q\n          )\n       END                             AS  URI_MIKEIJO_FLG           -- 売上未計上フラグ\n      ,TBL_J.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n  FROM\n     (\n\n\n\n-- 分析依頼\nSELECT \q1\q                             AS  JUCHU_KBN                 -- 受注区分\n      ,TBL_H.BUNSEKI_STS               AS  STS                       -- ステータス\n      ,TBL_H.BUNSEKI_STS_MEI           AS  STS_MEI                   -- ステータス名\n      ,TBL_H.JUCHU_KBN                 AS  BUN_HAN_KBN               -- 分析販売区分\n      ,TBL_H.JUCHU_KBN_MEI             AS  BUN_HAN_KBN_MEI           -- 分析販売区分名\n      ,TBL_H.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_H.KENMEI                    AS  KENMEI                    -- 件名\n      ,TBL_H.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,TBL_H.SEIKYU_MEI                AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_H.NYURYOKUSHA_CD            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,TBL_H.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,TBL_H.NYURYOKU_DATETIME         AS  NYURYOKU_DATETIME         -- 入力日時\n      ,TBL_H.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_EDA_KENSU.EDA_KENSU         AS  EDA_KENSU                 -- 枝番件数\n      ,TBL_SHOHIN_KENSU.SHOHIN_KENSU   AS  SHOHIN_KENSU              -- 商品件数\n      ,TBL_H.MITSU_NO                  AS  MITSU_NO                  -- 見積No.\n      ,NULL                            AS  HASSO_CD                  -- 発送先CD\n      ,TBL_H.DEMPYO_HAKKO_KBN          AS  DEMPYO_HAKKO_KBN          -- 伝票発行区分\n      ,      TBL_H.FREEWD2                      +  -- フリーワード検索用２\n       \q|\q + ISNULL(TBL_F3.FREEWD,\q\q)           +  -- フリーワード(見積ボディ)\n       \q|\q                             AS  FREEWD                    -- フリーワード検索用\n  FROM V_C_BUN_JUCHU_H                 AS  TBL_H                     -- 【分析受注ヘッダ共通ビュー】\n  LEFT JOIN                                                          -- 【枝番件数用】\n      (SELECT TBL_B.JURI_NO\n             ,COUNT(*)                 AS  EDA_KENSU\n         FROM T_BUN_JUCHU_B            AS  TBL_B\n        GROUP BY\n              TBL_B.JURI_NO\n      )                                AS  TBL_EDA_KENSU\n    ON TBL_EDA_KENSU.JURI_NO           =   TBL_H.JURI_NO\n  LEFT JOIN                                                          -- 【商品件数用】\n      (SELECT TBL_B.MITSU_NO\n             ,COUNT(*)                 AS  SHOHIN_KENSU\n         FROM T_MITSU_B                AS  TBL_B\n        WHERE TBL_B.ROW_KBN            =   \q1\q\n        GROUP BY\n              TBL_B.MITSU_NO\n      )                                AS  TBL_SHOHIN_KENSU\n    ON TBL_SHOHIN_KENSU.MITSU_NO       =   TBL_H.MITSU_NO\n  LEFT JOIN                                                          -- 【フリーワード(見積ボディ)】\n     (\n       SELECT TBL_A.MITSU_NO\n             ,(                                    -- 商品CD\n               SELECT \q|\q + TBL_B.SHOHIN_CD      AS  [data()]\n                 FROM T_MITSU_B                  AS  TBL_B\n                WHERE TBL_B.MITSU_NO             =   TBL_A.MITSU_NO\n                FOR XML PATH (\q\q)\n              )\n                                                 AS  FREEWD\n         FROM T_MITSU_B                          AS  TBL_A\n        GROUP BY\n              TBL_A.MITSU_NO\n     ) TBL_F3\n    ON TBL_F3.MITSU_NO                 =  TBL_H.MITSU_NO\n WHERE TBL_H.DEL_FLG                   =  \qFalse\q\n   AND TBL_H.BUNSEKI_STS               <= 10\n--   AND TBL_H.DEMPYO_HAKKO_KBN          <> \q3\q\n--   AND TBL_H.MITSU_NO                  <> \q\q\nUNION ALL\n-- 販売商品\nSELECT \q2\q                             AS  JUCHU_KBN                 -- 受注区分\n      ,TBL_H.HAMBAI_STS                AS  STS                       -- ステータス\n      ,M_HAMBAI_STS.DATA_1             AS  STS_MEI                   -- ステータス名\n      ,TBL_H.HAMBAI_KBN                AS  BUN_HAN_KBN               -- 分析販売区分\n      ,M_HAMBAI_KBN.DATA_1             AS  BUN_HAN_KBN_MEI           -- 分析販売区分名\n      ,TBL_H.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_H.KENMEI                    AS  KENMEI                    -- 件名\n      ,TBL_H.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,M_SEIKYU_MEI.AITE_MEI           AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_H.NYURYOKUSHA_CD            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,TBL_H.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,TBL_H.NYURYOKU_DATETIME         AS  NYURYOKU_DATETIME         -- 入力日時\n      ,TBL_H.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,NULL                            AS  EDA_KENSU                 -- 枝番件数\n      ,TBL_SHOHIN_KENSU.SHOHIN_KENSU   AS  SHOHIN_KENSU              -- 商品件数\n      ,TBL_H.MITSU_NO                  AS  MITSU_NO                  -- 見積No.\n      ,TBL_H.HASSO_CD                  AS  HASSO_CD                  -- 発送先CD\n      ,TBL_H.DEMPYO_HAKKO_KBN          AS  DEMPYO_HAKKO_KBN          -- 伝票発行区分\n      , \q|\q + TBL_H.SEIKYU_CD                   +  -- 請求先CD\n        \q|\q + ISNULL(M_SEIKYU_MEI.AITE_MEI,\q\q)  +  -- 請求先名\n        \q|\q + TBL_H.SEIKYU_TANTO_MEI            +  -- 先方担当者名\n        \q|\q + TBL_H.KENMEI                      +  -- 件名\n        \q|\q + TBL_H.HASSO_MEI                   +  -- 発送先名\n        \q|\q + TBL_H.HASSO_TANTO_MEI             +  -- 発送先担当者名\n        \q|\q + TBL_F.FREEWD                      +  -- フリーワード(ボディ)\n        \q|\q                            AS  FREEWD                    -- フリーワード検索用\n  FROM T_HAN_JUCHU_H                   AS  TBL_H                     -- 【販売商品受注ヘッダテーブル】\n  LEFT JOIN                                                          -- 【販売ステータス用】\n       M_MEISHO_B                      AS  M_HAMBAI_STS\n    ON M_HAMBAI_STS.MEISHO_KBN         =   \qS004\q\n   AND M_HAMBAI_STS.MEISHO_CD          =   TBL_H.HAMBAI_STS\n  LEFT JOIN                                                          -- 【販売商品区分用】\n       M_MEISHO_B                      AS  M_HAMBAI_KBN\n    ON M_HAMBAI_KBN.MEISHO_KBN         =   \qK020\q\n   AND M_HAMBAI_KBN.MEISHO_CD          =   TBL_H.HAMBAI_KBN\n  LEFT JOIN                                                          -- 【請求先名用】\n       M_AITESAKI                      AS  M_SEIKYU_MEI\n    ON M_SEIKYU_MEI.AITE_CD            =   TBL_H.SEIKYU_CD\n  LEFT JOIN                                                          -- 【商品件数用】\n      (SELECT TBL_B.JURI_NO\n             ,COUNT(*)                 AS  SHOHIN_KENSU\n         FROM T_HAN_JUCHU_B            AS  TBL_B\n        WHERE TBL_B.ROW_KBN            =   \q1\q\n        GROUP BY\n              TBL_B.JURI_NO\n      )                                AS  TBL_SHOHIN_KENSU\n    ON TBL_SHOHIN_KENSU.JURI_NO        =   TBL_H.JURI_NO\n  LEFT JOIN                                                          -- 【フリーワード(ボディ)】\n     (\n       SELECT TBL_A.JURI_NO\n             ,(                                    -- 商品CD\n               SELECT \q|\q + TBL_B.SHOHIN_CD      AS  [data()]\n                 FROM T_HAN_JUCHU_B              AS  TBL_B\n                WHERE TBL_B.JURI_NO              =   TBL_A.JURI_NO\n                FOR XML PATH (\q\q)\n              )\n             +(                                    -- 商品名\n               SELECT \q|\q + TBL_B.SHOHIN_MEI     AS  [data()]\n                 FROM T_HAN_JUCHU_B TBL_B\n                WHERE TBL_B.JURI_NO              =   TBL_A.JURI_NO\n                FOR XML PATH (\q\q)\n              )\n             +(                                    -- 摘要\n               SELECT \q|\q + TBL_B.TEKIYO         AS  [data()]\n                 FROM T_HAN_JUCHU_B TBL_B\n                WHERE TBL_B.JURI_NO              =   TBL_A.JURI_NO\n                FOR XML PATH (\q\q)\n              )\n                                       AS  FREEWD\n         FROM T_HAN_JUCHU_B TBL_A\n        GROUP BY\n              TBL_A.JURI_NO\n     ) TBL_F\n    ON TBL_F.JURI_NO                   =  TBL_H.JURI_NO\n WHERE TBL_H.DEL_FLG = \qFalse\q\n--   AND TBL_H.DEMPYO_HAKKO_KBN          <> \q3\q\n\n\n     ) TBL_J\n  LEFT JOIN                                                          -- 【受注区分用】\n       M_MEISHO_B                      AS  M_JUCHU_KBN\n    ON M_JUCHU_KBN.MEISHO_KBN          =   \qK030\q\n   AND M_JUCHU_KBN.MEISHO_CD           =   TBL_J.JUCHU_KBN\n  LEFT JOIN                                                          -- 【入力者名用】\n       M_SHAIN                         AS  M_NYURYOKUSHA\n    ON M_NYURYOKUSHA.SHAIN_CD          =   TBL_J.NYURYOKUSHA_CD\n  LEFT JOIN                                                          -- 【営業担当者名用】\n       M_SHAIN                         AS  M_EIGYO_TANTO\n    ON M_EIGYO_TANTO.SHAIN_CD          =   TBL_J.EIGYO_TANTO_CD\n  LEFT JOIN                                                          -- 【売上No.用】\n     (\n       SELECT TBL_C.JURI_NO\n             ,REPLACE(\n              (\n               SELECT TBL_E.URIAGE_NO  AS  [data()]\n                 FROM (\n                       SELECT TBL_D1.URIAGE_NO      AS  URIAGE_NO\n                         FROM T_URIAGE_H            AS  TBL_D1\n                        WHERE TBL_D1.JURI_NO        =   TBL_C.JURI_NO\n                       UNION\n                       SELECT TBL_D2.URIAGE_NO      AS  URIAGE_NO\n                         FROM T_URIAGE_JUCHU_SELECT AS  TBL_D2\n                        WHERE TBL_D2.JURI_NO        =   TBL_C.JURI_NO\n                      ) AS TBL_E\n                FOR XML PATH (\q\q)\n              ),\q \q,\q,\q)                            AS  URIAGE_NO_S\n         FROM (\n               SELECT TBL_A.JURI_NO                 AS  JURI_NO\n                 FROM T_URIAGE_H                    AS  TBL_A\n               UNION\n               SELECT TBL_B.JURI_NO                 AS  JURI_NO\n                 FROM T_URIAGE_JUCHU_SELECT         AS  TBL_B\n              ) TBL_C\n        WHERE TBL_C.JURI_NO            <>  \q\q\n        GROUP BY\n              TBL_C.JURI_NO\n\n     ) TBL_U\n    ON TBL_U.JURI_NO                   =   TBL_J.JURI_NO\n  WHERE TBL_J.SEIKYU_CD                <>  \q\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="売上関連",750,100
Page=売上関連
Left=750
Top=100

[View]
PName=V_KOBAI_NYUKA_LIST
LName=購買申請入荷予定リストビュー
Tag=ビュー
Comment=UNKB0080(入荷予定一覧表発行)
ViewOption=
Query=--*DataTitle V_KOBAI_NYUKA_LIST(購買申請入荷予定リストビュー)\nSELECT TBL_V.NYUKA_HAKKO_DISP          AS  NYUKA_HAKKO_DISP          -- 入荷予定発行表示\n      ,TBL_V.WARNING                   AS  WARNING                   -- 警告\n      ,TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.KOBAI_STS_MEI             AS  KOBAI_STS_MEI             -- 購買ステータス名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,TBL_V.SHINSEI_DATETIME          AS  SHINSEI_DATETIME          -- 申請日時\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_V.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_V.HATCHU_DATE               AS  HATCHU_DATE               -- 発注日\n      ,TBL_V.NOKI                      AS  NOKI                      -- 希望納期\n      ,TBL_V.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_V.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_V.CHUMON_NO                 AS  CHUMON_NO                 -- 注文No.\n      ,TBL_V.NYUKA_HAKKO_FLG           AS  NYUKA_HAKKO_FLG           -- 入荷予定発行フラグ\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_V.KOBAI_STS                 AS  KOBAI_STS                 -- 購買ステータス\n      ,TBL_V.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,TBL_V.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n  LEFT JOIN\n       T_HAN_JUCHU_H                   AS  TBL_H\n    ON TBL_H.JURI_NO                   =   TBL_V.JURI_NO\n WHERE TBL_V.KOBAI_STS                 IN  (3,4,5,6)\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n--   AND ISNULL(TBL_H.MAKER_FLG,\q\q)      <>  \qTrue\q                    -- 直送フラグOFF\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="購買関連",3550,100
Page=購買関連
Left=3550
Top=100

[View]
PName=V_KOBAI_CHUMON_LIST
LName=購買申請注文確定リストビュー
Tag=ビュー
Comment=UNKB0050(購買申請注文確定)
ViewOption=
Query=--*DataTitle V_KOBAI_CHUMON_LIST(購買申請注文確定リストビュー)\nSELECT CASE TBL_V.KOBAI_STS\n       WHEN 1 THEN \qFalse\q\n       WHEN 2 THEN \qFalse\q\n       ELSE        \qTrue\q\n       END                             AS  CHUMON_FLG                -- 注文フラグ\n      ,TBL_V.GYO_MOJI                  AS  GYO_MOJI                  -- 選択\n      ,TBL_V.WARNING                   AS  WARNING                   -- 警告\n      ,TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.KOBAI_STS_MEI             AS  KOBAI_STS_MEI             -- 購買ステータス名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,TBL_V.SHINSEI_DATETIME          AS  SHINSEI_DATETIME          -- 申請日時\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,CASE ISNULL(TBL_V.HATCHU_DATE,\q\q)\n       WHEN \q\q THEN CONVERT(VARCHAR(10), GETDATE(),111)\n       ELSE TBL_V.HATCHU_DATE\n       END                             AS  HATCHU_DATE               -- 発注日\n      ,TBL_V.NOKI                      AS  NOKI                      -- 希望納期\n      ,TBL_V.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_V.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_V.CHUMON_NO                 AS  CHUMON_NO                 -- 注文No.\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_V.KOBAI_STS                 AS  KOBAI_STS                 -- 購買ステータス\n      ,TBL_V.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,TBL_V.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n WHERE TBL_V.KOBAI_STS                 IN  (1,2,3,4,5,6)\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n   AND TBL_V.JOGAI_FLG                 =   \qFalse\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="購買関連",2850,100
Page=購買関連
Left=2850
Top=100

[View]
PName=V_KOBAI_SHONIN_LIST
LName=購買申請承認リストビュー
Tag=ビュー
Comment=UNKB0040(購買申請承認)
ViewOption=
Query=--*DataTitle V_KOBAI_SHONIN_LIST(購買申請承認リストビュー)\nSELECT CASE TBL_V.KOBAI_STS\n       WHEN 1 THEN \qFalse\q\n       ELSE        \qTrue\q\n       END                             AS  SHONIN_FLG                -- 承認フラグ\n      ,TBL_V.GYO_MOJI                  AS  GYO_MOJI                  -- 選択\n      ,TBL_V.WARNING                   AS  WARNING                   -- 警告\n      ,TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.KOBAI_STS_MEI             AS  KOBAI_STS_MEI             -- 購買ステータス名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,TBL_V.SHINSEI_DATETIME          AS  SHINSEI_DATETIME          -- 申請日時\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_V.NOKI                      AS  NOKI                      -- 希望納期\n      ,TBL_V.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_V.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_V.KOBAI_STS                 AS  KOBAI_STS                 -- 購買ステータス\n      ,TBL_V.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,TBL_V.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n WHERE TBL_V.KOBAI_STS                 IN  (1,2)\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n   AND TBL_V.JOGAI_FLG                 =   \qFalse\q\n\n
