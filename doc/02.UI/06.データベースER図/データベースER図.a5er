[View]
PName=V_URIAGE_EXCEL
LName=売上Excel出力ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_URIAGE_EXCEL(売上Excel出力ビュー)\nSELECT \n       TBL_H.URIAGE_NO                                      -- 売上No.\n      ,TBL_H.DEMPYO_KBN                                     -- 伝票区分CD\n      ,TBL_H.DEMPYO_KBN_MEI                                 -- 伝票区分名\n      ,TBL_H.RENDO_KBN                                      -- 商魂連動区分CD\n      ,TBL_H.RENDO_KBN_MEI                                  -- 商魂連動区分名\n      ,TBL_H.DENKU_KBN                                      -- 売上伝区CD\n      ,TBL_H.DENKU_KBN_MEI                                  -- 売上伝区CD\n      ,TBL_H.KANJO_KAMOKU                                   -- 勘定科目CD\n      ,TBL_H.KANJO_KAMOKU_MEI                               -- 勘定科目名\n      ,TBL_H.URIAGE_DATE                                    -- 売上日\n      ,TBL_H.BUSHO_CD                                       -- 担当部署CD\n      ,TBL_H.BUSHO_MEI                                      -- 担当部署名\n      ,TBL_H.URIAGE_NYURYOKUSHA_CD                          -- 売上入力者CD\n      ,TBL_H.URI_NYU_MEI                                    -- 売上入力者名\n      ,TBL_H.EIGYO_TANTO_CD                                 -- 営業担当者CD\n      ,TBL_H.EIGYO_TANTO_MEI                                -- 営業担当者名\n      ,TBL_H.NYURYOKU_DATE_TIME                             -- 入力日時\n      ,TBL_H.JUCHU_KBN                                      -- 受注区分CD\n      ,TBL_H.JUCHU_KBN_MEI                                  -- 受注区分名\n      ,CASE WHEN\n            ( SELECT COUNT(*)\n                FROM T_URIAGE_JUCHU_SELECT AS TBL_W\n               WHERE TBL_W.URIAGE_NO       =  TBL_H.URIAGE_NO\n            ) >= 1\n       THEN\n            ( SELECT IIF( ISNULL(T_URIAGE_JUCHU_SELECT.JURI_NO,\q\q)=\q\q,\q\q, T_URIAGE_JUCHU_SELECT.JURI_NO + \q,\q)\n                FROM T_URIAGE_JUCHU_SELECT\n               WHERE T_URIAGE_JUCHU_SELECT.URIAGE_NO = TBL_H.URIAGE_NO\n                 FOR XML PATH (\q\q)\n            )\n       ELSE TBL_H.JURI_NO\n       END                           AS  JURI_NO            -- 対象受理No.\n      ,TBL_H.MITSU_NO                                       -- 対象見積No.\n      ,TBL_H.SAKI_URIAGE_NO                                 -- 結合先売上No.\n      ,TBL_H.NOHIN_DATE                                     -- 納品日\n      ,TBL_H.URIAGE_STS                                     -- 売上ステータスCD\n      ,TBL_H.URIAGE_STS_MEI                                 -- 売上ステータス名\n      ,TBL_H.DOJI_FLG                                       -- 同時発行フラグ\n      ,TBL_H.SEIKYU_CD                                      -- 請求先CD\n      ,TBL_H.SEIKYU_MEI                                     -- 請求先名\n      ,J_SEIKYU.SEIKYU_FLG                                  -- 請求先フラグ\n      ,J_SEIKYU.SHIRE_FLG                                   -- 仕入先フラグ\n      ,J_SEIKYU.HASSO_FLG                                   -- 発送先フラグ\n      ,J_SEIKYU.HOKOKU_FLG                                  -- 報告先フラグ\n      ,J_SEIKYU.MAKER_FLG                                   -- メーカーフラグ\n      ,J_SEIKYU.GAICHU_FLG                                  -- 外注先フラグ\n      ,J_SEIKYU.KANKO_FLG                                   -- 官公庁フラグ\n      ,J_SEIKYU.DOGYO_FLG                                   -- 同業者フラグ\n      ,J_SEIKYU.SPOT_FLG                                    -- スポットフラグ\n      ,J_SEIKYU.SEIKYU_BUNRUI                               -- 請求先分類区分CD\n      ,J_SEIKYU_KBN.DATA_1           AS SEIKYU_BUNRUI_MEI   -- 請求先分類区分名\n      ,TBL_H.KAISHU_DATE                                    -- 回収予定日\n      ,TBL_H.BUNSEKI_HOHO_CD                                -- 分析方法CD\n      ,TBL_H.BUNSEKI_HOHO                                   -- 分析方法名\n      ,TBL_H.TEKIYO                  AS TEKIYO_H            -- 摘要\n      ,TBL_H.DEMPYO_SOFU_MEI                                -- 伝票送付先名\n      ,TBL_H.KEISHO                                         -- 敬称\n      ,TBL_H.YUBIN_NO                                       -- 送付先郵便番号\n      ,TBL_H.ADDRESS_1                                      -- 送付先住所１\n      ,TBL_H.ADDRESS_2                                      -- 送付先住所２\n      ,TBL_H.TEL                                            -- 送付先TEL\n      ,TBL_H.FAX                                            -- 送付先FAX\n      ,TBL_H.SOFU_TANTO                                     -- 送付先担当者\n      ,TBL_H.TEISEI_BIKO                                    -- 訂正備考\n      ,TBL_H.JUSHO_NO_PRINT_FLG                             -- 住所印字不要フラグ\n      ,TBL_H.TAX_RATE                                       -- 税率\n      ,TBL_H.NUKI_TOTAL                                     -- 税抜合計\n      ,TBL_H.TAX                                            -- 消費税\n      ,TBL_H.KOMI_TOTAL                                     -- 税込合計\n      ,TBL_H.GENKA_TOTAL                                    -- 原価合計\n      ,TBL_H.ARARIEKI               AS ARARIEKI_H           -- 粗利益\n      ,TBL_H.ARARIRITSU             AS ARARIRITSU_H         -- 粗利率\n      ,TBL_H.SHOKON_NO                                      -- 商魂伝票No.\n      ,TBL_H.SHOKON_LIST_DATE_TIME                          -- 連動日時\n      ,TBL_B.GYO_NO                                         -- 行No.\n      ,TBL_B.ROW_KBN                                        -- 行区分\n      ,TBL_B.SHOHIN_CD                                      -- 商品CD\n      ,TBL_B.SHOHIN_MEI                                     -- 商品名\n      ,J_SHOHIN.SHOHIN_BUNRUI                               -- 商品分類CD\n      ,J_SHOHIN_BUNRUI.DATA_1       AS SHOHIN_BUNRUI_MEI    -- 商品分類名\n      ,J_SHOHIN.SHIYAKU_FLG                                 -- 試薬フラグ\n      ,J_SHOHIN.CHOSEI_SHIYAKU_FLG                          -- 調製試薬フラグ\n      ,J_SHOHIN.DOKU_FLG                                    -- 毒劇物フラグ\n      ,TBL_B.URIAGE_SURYO                                   -- 売上数量\n      ,TBL_B.URIAGE_SURYO_TANI                              -- 売上数量単位\n      ,TBL_B.HAMBAI_TANKA                                   -- 販売単価\n      ,TBL_B.HAMBAI_KINGAKU                                 -- 販売金額\n      ,TBL_B.TEKIYO                 AS TEKIYO_B             -- 摘要\n      ,TBL_B.GENTANKA                                       -- 原単価\n      ,TBL_B.GENKA_SURYO                                    -- 原価数量\n      ,TBL_B.IRISU_TANI                                     -- 入数単位\n      ,TBL_B.SOSU                                           -- 総数\n      ,TBL_B.BARA_TANI                                      -- バラ単位\n      ,TBL_B.GENKA_KINGAKU                                  -- 原価金額\n      ,TBL_B.ARARIRITSU             AS ARARIRITSU_B         -- 粗利率\n      ,TBL_B.ARARIEKI               AS ARARIEKI_B           -- 粗利益\n      ,TBL_B.TEIKA                                          -- 定価\n      ,TBL_B.DEL_FLG                                        -- 削除フラグ\n\n  FROM V_C_URIAGE_H                 AS TBL_H\n  LEFT JOIN\n       T_URIAGE_B                   AS TBL_B\n    ON TBL_B.URIAGE_NO              =  TBL_H.URIAGE_NO\n  -- 相手先マスタ\n  LEFT JOIN\n       M_AITESAKI                   AS J_SEIKYU\n    ON J_SEIKYU.AITE_CD             =  TBL_H.SEIKYU_CD\n  -- 請求先分類区分\n  LEFT JOIN\n       M_MEISHO_B                   AS J_SEIKYU_KBN\n    ON J_SEIKYU_KBN.MEISHO_KBN      =  \qK001\q\n   AND J_SEIKYU_KBN.MEISHO_CD       =  J_SEIKYU.SEIKYU_BUNRUI\n  -- 商品マスタ\n  LEFT JOIN\n       M_SHOHIN                     AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD           =  TBL_B.SHOHIN_CD\n  -- 商品分類区分\n  LEFT JOIN\n       M_MEISHO_B                   AS J_SHOHIN_BUNRUI\n    ON J_SHOHIN_BUNRUI.MEISHO_KBN   =  \qK025\q\n   AND J_SHOHIN_BUNRUI.MEISHO_CD    =  J_SHOHIN.SHOHIN_BUNRUI\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="EXCEL出力",1750,100
Page=EXCEL出力
Left=1750
Top=100

[View]
PName=V_HAN_JUCHU_EXCEL
LName=販売受注Excel出力ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT \n       TBL_H.JURI_NO                                        -- 受理No.\n      ,TBL_H.HAMBAI_STS             AS HAMBAI_STS_CD        -- 販売ステータスCD\n      ,J_STS.DATA_1                 AS HAMBAI_STS_MEI       -- 販売ステータス名\n      ,TBL_H.HAMBAI_KBN             AS HAMBAI_KBN_CD        -- 販売商品区分CD\n      ,J_HAN_KBN.DATA_1             AS HAMBAI_KBN_MEI       -- 販売商品区分名\n      ,TBL_H.REF_JURI_NO                                    -- 参照元受理No.\n      ,TBL_H.NYURYOKU_DATETIME                              -- 入力日時\n      ,TBL_H.NYURYOKUSHA_CD         AS NYURYOKUSHA_CD       -- 入力者CD\n      ,J_NYURYOKUSHA.SHIMEI         AS NYURYOKUSHA_MEI      -- 入力者名\n      ,TBL_H.MITSU_NO                                       -- 見積No.\n      ,TBL_H.JUCHU_DATE                                     -- 受注日\n      ,TBL_H.NOHIN_DATE                                     -- 納品日\n      ,TBL_H.SEIKYU_CD              AS SEIKYU_CD            -- 請求先CD\n      ,J_SEIKYU.AITE_MEI            AS SEIKYU_MEI           -- 請求先名\n      ,TBL_H.SEIKYU_TANTO_MEI                               -- 先方担当者名\n      ,TBL_H.KENMEI                                         -- 件名\n      ,TBL_H.EIGYO_TANTO_CD         AS EIGYO_TANTO_CD       -- 営業担当者CD\n      ,J_EIGYO.SHIMEI               AS EIGYO_TANTO_MEI      -- 営業担当者名\n      ,TBL_H.HASSO_MEI                                      -- 発送先名\n      ,TBL_H.HASSO_YUBIN_NO                                 -- 発送先郵便番号\n      ,TBL_H.HASSO_ADDRESS_1                                -- 発送先住所１\n      ,TBL_H.HASSO_ADDRESS_2                                -- 発送先住所２\n      ,TBL_H.HASSO_TEL                                      -- 発送先TEL\n      ,TBL_H.HASSO_FAX                                      -- 発送先FAX\n      ,TBL_H.HASSO_TANTO_MEI                                -- 発送先担当者名\n      ,TBL_H.MAKER_FLG                                      -- メーカー直送フラグ\n      ,TBL_H.TAX_RATE                                       -- 税率\n      ,TBL_H.NUKI_TOTAL                                     -- 税抜合計\n      ,TBL_H.TAX                                            -- 消費税\n      ,TBL_H.KOMI_TOTAL                                     -- 税込合計\n      ,TBL_H.GENKA_TOTAL                                    -- 原価合計\n      ,TBL_H.ARARIEKI               AS ARARIEKI_H           -- 粗利益\n      ,TBL_H.ARARIRITSU             AS ARARIRITSU_H         -- 粗利率\n      ,TBL_H.DEMPYO_HAKKO_KBN       AS DEMPYO_HAKKO_KBN_CD  -- 伝票発行区分CD\n      ,J_DEMPYO_KBN.DATA_1          AS DEMPYO_HAKKO_KBN_MEI -- 伝票発行区分名\n      ,TBL_B.GYO_NO                                         -- 行No.\n      ,CASE TBL_B.ROW_KBN \n       WHEN 1 THEN \q商品\q\n       ELSE \qコメント\q\n       END                          AS ROW_KBN              -- 行区分\n      ,J_SHOHIN.SHOHIN_BUNRUI       AS SHOHIN_BUNRUI_CD     -- 商品分類区分CD\n      ,J_SHOHIN_BUNRUI.DATA_1       AS SHOHIN_BUNRUI_MEI    -- 商品分類区分名\n      ,TBL_B.SHOHIN_CD                                      -- 商品CD\n      ,TBL_B.SHOHIN_MEI                                     -- 商品名\n      ,TBL_B.JUCHU_SURYO                                    -- 受注数量\n      ,TBL_B.JUCHU_TANI                                     -- 受注数量単位\n      ,TBL_B.HAMBAI_TANKA                                   -- 販売単価\n      ,TBL_B.HAMBAI_KINGAKU                                 -- 販売金額\n      ,TBL_B.TEKIYO                                         -- 摘要\n      ,TBL_B.GENTANKA                                       -- 原単価\n      ,TBL_B.GENKA_SURYO                                    -- 原価数量\n      ,TBL_B.IRISU_TANI                                     -- 入数単位\n      ,TBL_B.TOTAL                                          -- 総数\n      ,TBL_B.BARA_TANI                                      -- バラ単位\n      ,TBL_B.GENKA_KINGAKU                                  -- 原価金額\n      ,TBL_B.ARARIRITSU             AS ARARIRITSU_B         -- 粗利率\n      ,TBL_B.ARARIEKI               AS ARARIEKI_B           -- 粗利益\n      ,TBL_B.TEIKA                                          -- 定価\n      ,TBL_B.SHIIRE_CD              AS SHIIRE_CD            -- 仕入先CD\n      ,J_SHIIRE.AITE_MEI            AS SHIIRE_MEI           -- 仕入先名\n      ,TBL_B.MAKER_CD               AS MAKER_CD             -- メーカーCD\n      ,J_MAKER.AITE_MEI             AS MAKER_MEI            -- メーカー名\n      ,TBL_H.DEL_FLG                                        -- 削除フラグ\n\n  FROM T_HAN_JUCHU_H                AS TBL_H\n  LEFT JOIN\n       T_HAN_JUCHU_B                AS TBL_B\n    ON TBL_B.JURI_NO                =  TBL_H.JURI_NO\n\n  -- 販売ステータス\n  LEFT JOIN\n       M_MEISHO_B                   AS J_STS\n    ON J_STS.MEISHO_KBN             =  \qS004\q\n   AND J_STS.MEISHO_CD              =  TBL_H.HAMBAI_STS\n  -- 販売商品区分\n  LEFT JOIN\n       M_MEISHO_B                   AS J_HAN_KBN\n    ON J_HAN_KBN.MEISHO_KBN         =  \qK020\q\n   AND J_HAN_KBN.MEISHO_CD          =  TBL_H.HAMBAI_KBN\n  -- 入力者\n  LEFT JOIN\n       M_SHAIN                      AS J_NYURYOKUSHA\n    ON J_NYURYOKUSHA.SHAIN_CD       =  TBL_H.NYURYOKUSHA_CD\n  -- 請求先\n  LEFT JOIN\n       M_AITESAKI                   AS J_SEIKYU\n    ON J_SEIKYU.AITE_CD             =  TBL_H.SEIKYU_CD\n  -- 営業担当者\n  LEFT JOIN\n       M_SHAIN                      AS J_EIGYO\n    ON J_EIGYO.SHAIN_CD             =  TBL_H.EIGYO_TANTO_CD\n  -- 伝票発行区分\n  LEFT JOIN\n       M_MEISHO_B                   AS J_DEMPYO_KBN\n    ON J_DEMPYO_KBN.MEISHO_KBN      =  \qK026\q\n   AND J_DEMPYO_KBN.MEISHO_CD       =  TBL_H.DEMPYO_HAKKO_KBN\n  -- 商品\n  LEFT JOIN\n       M_SHOHIN                     AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD           =  TBL_B.SHOHIN_CD\n  -- 商品分類区分\n  LEFT JOIN\n       M_MEISHO_B                   AS J_SHOHIN_BUNRUI\n    ON J_SHOHIN_BUNRUI.MEISHO_KBN   =  \qK025\q\n   AND J_SHOHIN_BUNRUI.MEISHO_CD    =  J_SHOHIN.SHOHIN_BUNRUI\n  -- 仕入先\n  LEFT JOIN\n       M_AITESAKI                   AS J_SHIIRE\n    ON J_SHIIRE.AITE_CD             =  TBL_B.SHIIRE_CD\n  -- メーカー\n  LEFT JOIN\n       M_AITESAKI                   AS J_MAKER\n    ON J_MAKER.AITE_CD              =  TBL_B.MAKER_CD\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="EXCEL出力",1200,100
Page=EXCEL出力
Left=1200
Top=100

[View]
PName=V_BUN_JUCHU_EXCEL
LName=分析受注Excel出力ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_BUN_JUCHU_EXCEL(分析受注Excel出力ビュー)\nSELECT \n        TBL_H.JURI_NO                                               -- 受理No.\n       ,TBL_H.BUNSEKI_STS                                           -- 分析ステータスCD\n       ,TBL_H.BUNSEKI_STS_MEI                                       -- 分析ステータス名\n       ,TBL_H.JUCHU_KBN                                             -- 分析受注区分CD\n       ,TBL_H.JUCHU_KBN_MEI                                         -- 分析受注区分名\n       ,TBL_H.BUSHO_CD                                              -- 分析担当部署CD\n       ,TBL_H.BUSHO_MEI                                             -- 分析担当部署名\n       ,TBL_H.NYURYOKU_DATETIME                                     -- 入力日時\n       ,TBL_H.NYURYOKUSHA_CD                                        -- 入力者CD\n       ,TBL_H.NYURYOKUSHA_MEI                                       -- 入力者名\n       ,TBL_H.REF_JURI_NO                                           -- 参照元受理No.\n       ,TBL_H.MITSU_NO                                              -- 見積No.\n       ,TBL_H.ATENA_CD                                              -- 報告書宛名CD\n       ,TBL_H.ATENA                                                 -- 報告書宛名\n       ,TBL_H.SEIKYU_CD                                             -- 請求先CD\n       ,TBL_H.SEIKYU_MEI                                            -- 請求先名\n       ,TBL_H.BUNSEKI_HOHO_CD                                       -- 分析方法CD\n       ,TBL_H.BUNSEKI_HOHO                                          -- 分析方法名\n       ,TBL_H.KEISHIKI_KBN                                          -- 報告書形式CD\n       ,TBL_H.KEISHIKI_KBN_MEI                                      -- 報告書形式名\n       ,TBL_H.NOHIN_HOHO                                            -- 納品方法実績\n       ,TBL_H.KOSU                                                  -- 工数\n       ,TBL_H.EIGYO_TANTO_CD                                        -- 営業担当者CD\n       ,TBL_H.EIGYO_TANTO_MEI                                       -- 営業担当者名\n       ,TBL_H.IRAI_DATE                                             -- 依頼日／受付日\n       ,TBL_H.KANRYO_DATE                                           -- 分析担当者完了日\n       ,TBL_H.KANSEI_DATE                                           -- 報告書完成日／速報日\n       ,TBL_H.SOKUHO_KBN                                            -- 速報区分CD\n       ,TBL_H.SOKUHO_KBN_MEI                                        -- 速報区分名\n       ,TBL_H.HAKKO_DATE                                            -- 報告書指定発行日\n       ,TBL_H.NOHIN_DATE                                            -- 納品日\n       ,TBL_H.KENMEI                                                -- 件名\n       ,TBL_H.MOKUTEKI                                              -- 目的\n       ,TBL_H.KISAI_JIKO                                            -- 報告書記載事項\n       ,TBL_H.EDA_SHIRYO_FLG                                        -- 枝番試料登録フラグ\n       ,TBL_H.SAISHU_KAISHA_MEI             AS SAISHU_KAISHA_MEI_H  -- 採取会社名\n       ,TBL_H.SAISHU_SHA                    AS SAISHU_SHA_H         -- 採取者\n       ,TBL_H.SAISHU_HOHO                   AS SAISHU_HOHO_H        -- 採取方法\n       ,TBL_H.SAISHU_BASHO                  AS SAISHU_BASHO_H       -- 採取場所\n       ,TBL_H.TENKO                         AS TENKO_H              -- 天候\n       ,TBL_H.KION                          AS KION_H               -- 気温\n       ,TBL_H.SUION                         AS SUION_H              -- 水温\n       ,TBL_H.SAISHU_DATE                   AS SAISHU_DATE_H        -- 採取日\n       ,TBL_H.SAISHU_TIME                   AS SAISHU_TIME_H        -- 採取時間\n       ,TBL_H.SHOKEN_FLG                                            -- 報告書所見フラグ\n       ,TBL_H.GAISO_KBN                                             -- 報告書外装CD\n       ,TBL_H.GAISO_KBN_MEI                                         -- 報告書外装名\n       ,TBL_H.YOKI                                                  -- 容器種類\n       ,TBL_H.HENKYAKU                                              -- 試料・容器返却\n       ,TBL_H.HOKOKU_HOHO_KBN                                       -- 報告方法CD\n       ,TBL_H.HOKOKU_HOHO_KBN_MEI                                   -- 報告方法名\n       ,TBL_H.BUSU                                                  -- 発行部数\n       ,TBL_H.CHUKAN_DATE_1                                         -- 中間報告日１\n       ,TBL_H.CHUKAN_NAIYO_1                                        -- 中間報告内容１\n       ,TBL_H.CHUKAN_DATE_2                                         -- 中間報告日２\n       ,TBL_H.CHUKAN_NAIYO_2                                        -- 中間報告内容２\n       ,TBL_H.CHUKAN_DATE_3                                         -- 中間報告日３\n       ,TBL_H.CHUKAN_NAIYO_3                                        -- 中間報告内容３\n       ,TBL_H.BIKO                                                  -- 備考\n       ,TBL_H.FUYO_FLG                                              -- 営業確認不要フラグ\n       ,TBL_H.NEW_FLG                                               -- 新規フラグ\n       ,TBL_H.HENKO_FLG                                             -- 記載内容変更予定フラグ\n       ,TBL_H.HOKOKU_MEI_1                                          -- 報告先名１\n       ,TBL_H.BUSHO_1                                               -- 部署１\n       ,TBL_H.TANTO_1                                               -- 担当者１\n       ,TBL_H.TEL_1                                                 -- TEL１\n       ,TBL_H.FAX_1                                                 -- FAX１\n       ,TBL_H.MAIL_1                                                -- E-MAIL１\n       ,TBL_H.YUBIN_NO_1                                            -- 郵便番号１\n       ,TBL_H.ADDRESS_1                                             -- 住所１\n       ,TBL_H.CHUKAN_SOFU_FLG_1                                     -- 中間報告送付フラグ１\n       ,TBL_H.SOKUHO_SOFU_FLG_1                                     -- 速報送付フラグ１\n       ,TBL_H.HOKOKUSHO_SOFU_FLG_1                                  -- 報告書送付フラグ１\n       ,TBL_H.HOKOKU_MEI_2                                          -- 報告先名２\n       ,TBL_H.BUSHO_2                                               -- 部署２\n       ,TBL_H.TANTO_2                                               -- 担当者２\n       ,TBL_H.TEL_2                                                 -- TEL２\n       ,TBL_H.FAX_2                                                 -- FAX２\n       ,TBL_H.MAIL_2                                                -- E-MAIL２\n       ,TBL_H.YUBIN_NO_2                                            -- 郵便番号２\n       ,TBL_H.ADDRESS_2                                             -- 住所２\n       ,TBL_H.CHUKAN_SOFU_FLG_2                                     -- 中間報告送付フラグ２\n       ,TBL_H.SOKUHO_SOFU_FLG_2                                     -- 速報送付フラグ２\n       ,TBL_H.HOKOKUSHO_SOFU_FLG_2                                  -- 報告書送付フラグ２\n       ,TBL_H.HOKOKU_MEI_3                                          -- 報告先名３\n       ,TBL_H.BUSHO_3                                               -- 部署３\n       ,TBL_H.TANTO_3                                               -- 担当者３\n       ,TBL_H.TEL_3                                                 -- TEL３\n       ,TBL_H.FAX_3                                                 -- FAX３\n       ,TBL_H.MAIL_3                                                -- E-MAIL３\n       ,TBL_H.YUBIN_NO_3                                            -- 郵便番号３\n       ,TBL_H.ADDRESS_3                                             -- 住所３\n       ,TBL_H.CHUKAN_SOFU_FLG_3                                     -- 中間報告送付フラグ３\n       ,TBL_H.SOKUHO_SOFU_FLG_3                                     -- 速報送付フラグ３\n       ,TBL_H.HOKOKUSHO_SOFU_FLG_3                                  -- 報告書送付フラグ３\n       ,TBL_H.DEMPYO_HAKKO_KBN                                      -- 伝票発行区分CD\n       ,TBL_H.DEMPYO_HAKKO_KBN_MEI                                  -- 伝票発行区分名\n       ,TBL_B.JURI_EDA_NO                                           -- 枝番\n       ,TBL_B.SAISHU_KAISHA_MEI             AS SAISHU_KAISHA_MEI_B  -- 採取会社名\n       ,TBL_B.SAISHU_SHA                    AS SAISHU_SHA_B         -- 採取者\n       ,TBL_B.SAISHU_HOHO                   AS SAISHU_HOHO_B        -- 採取方法\n       ,TBL_B.SAISHU_BASHO                  AS SAISHU_BASHO_B       -- 採取場所\n       ,TBL_B.TENKO                         AS TENKO_B              -- 天候\n       ,TBL_B.KION                          AS KION_B               -- 気温\n       ,TBL_B.SUION                         AS SUION_B              -- 水温\n       ,TBL_B.SAISHU_DATE                   AS SAISHU_DATE_B        -- 採取日\n       ,TBL_B.SAISHU_TIME                   AS SAISHU_TIME_B        -- 採取時間\n       ,TBL_B.SHIRYO_SHURUI                                         -- 試料種類\n       ,TBL_B.SHIRYO_MEI                                            -- 試料名\n       ,TBL_B.IKKATSU_M                                             -- 一括記載m\n       ,TBL_B.IKKATSU_N                                             -- 一括記載n\n       ,TBL_B.BUNSEKI_GUN_CD                                        -- 分析項目群CD\n       ,TBL_B.BUNSEKI_GUN_MEI                                       -- 分析項目群名 \n       ,TBL_B.EBIKO                          AS EBIKO               -- 枝番備考\n       ,TBL_S.GYO_NO                                                -- 行No.\n       ,TBL_S.BUNSEKI_CD                                            -- 分析項目CD\n       ,TBL_S.BUNSEKI_MEI                                           -- 分析項目名\n       ,TBL_S.BUNSEKI_TANI                                          -- 単位\n       ,TBL_S.BUNSEKI_DATA                                          -- データ\n       ,TBL_S.BUNSEKI_NO                                            -- 分析番号\n       ,TBL_S.URAGAKI_PRINT_FLG                                     -- 裏書印刷済フラグ\n       ,TBL_S.DEL_FLG                                               -- 削除フラグ\n\n  FROM V_C_BUN_JUCHU_H AS TBL_H\n  LEFT JOIN\n       V_C_BUN_JUCHU_B AS TBL_B\n    ON TBL_B.JURI_NO   =  TBL_H.JURI_NO\n  LEFT JOIN\n       V_C_BUN_JUCHU_SHO_K  AS TBL_S\n    ON TBL_S.JURI_NO        =  TBL_B.JURI_NO\n   AND TBL_S.JURI_EDA_NO    =  TBL_B.JURI_EDA_NO\n
