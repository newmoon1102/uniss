Query=SELECT \n       TBL_H.KOBAI_NO                                           -- 購買申請No.\n      ,TBL_H.SHINSEISHA_CD                                      -- 申請者CD\n      ,TBL_H.SHINSEISHA_MEI                                     -- 申請者名\n      ,TBL_H.SHINSEI_DATETIME                                   -- 申請日時\n      ,TBL_H.SHINSEI_GAIYO                                      -- 申請概要\n      ,TBL_H.KOBAI_KBN                                          -- 購買区分CD\n      ,TBL_H.KOBAI_KBN_MEI                                      -- 購買区分名\n      ,TBL_H.TAX_RATE                                           -- 税率\n      ,TBL_H.NUKI_TOTAL                                         -- 税抜合計\n      ,TBL_H.TAX                                                -- 消費税\n      ,TBL_H.KOMI_TOTAL                                         -- 税込合計\n      ,TBL_B.KOBAI_GYO_NO                                       -- 行No.\n      ,TBL_B.KOBAI_STS                                          -- 購買ステータスCD\n      ,TBL_B.KOBAI_STS_MEI                                      -- 購買ステータス名\n      ,TBL_B.JOGAI_FLG                                          -- 除外フラグ\n      ,TBL_B.KOBAIHIN_CD                                        -- 購買品CD\n      ,TBL_B.KOBAIHIN_MEI                                       -- 購買品名\n      ,J_SHOHIN.SHOHIN_BUNRUI                                   -- 商品分類CD\n      ,J_SHOHIN_BUNRUI.DATA_1           AS SHOHIN_BUNRUI_MEI    -- 商品分類名\n      ,J_SHOHIN.SHIYAKU_FLG                                     -- 試薬フラグ\n      ,J_SHOHIN.CHOSEI_SHIYAKU_FLG                              -- 調製試薬フラグ\n      ,J_SHOHIN.DOKU_FLG                                        -- 毒劇物フラグ\n      ,J_SHOHIN.HYOJUN_KOSU                                     -- PRTR\n      ,TBL_B.TANKA                                              -- 単価\n      ,TBL_B.IRISU                                              -- 入数\n      ,TBL_B.IRISU_TANI                                         -- 入数単位\n      ,TBL_B.SURYO                                              -- 数量\n      ,TBL_B.BARA_TANI                                          -- バラ単位\n      ,TBL_B.SOSU                                               -- 注文総数\n      ,TBL_B.TOTAL                                              -- 合計金額\n      ,TBL_B.NOKI                                               -- 希望納期\n      ,TBL_B.HOKAN_BASHO_KBN                                    -- 保管場所区分CD\n      ,TBL_B.HOKAN_BASHO_KBN_MEI                                -- 保管場所区分名\n      ,TBL_B.SHIIRE_CD                                          -- 仕入先CD\n      ,TBL_B.SHIIRE_MEI                                         -- 仕入先名\n      ,TBL_B.MAKER_CD                                           -- メーカーCD\n      ,TBL_B.MAKER_MEI                                          -- メーカー名\n      ,TBL_B.YOSAN_CD                                           -- 予算コード\n      ,TBL_B.JURI_NO                                            -- 受理No.\n      ,TBL_B.BIKO                                               -- 備考\n      ,TBL_L.IRAI_NO                                            -- 依頼No.\n      ,TBL_L.CHUMON_NO                                          -- 注文No.\n      ,TBL_L.NYUKA_HAKKO_FLG                                    -- 入荷予定表発行フラグ\n      ,TBL_H.DEL_FLG                                            -- 削除フラグ\n\n  FROM V_C_KOBAI_H                      AS TBL_H\n  LEFT JOIN\n       V_C_KOBAI_B                      AS TBL_B\n    ON TBL_B.KOBAI_NO                   =  TBL_H.KOBAI_NO\n  LEFT JOIN\n       V_KOBAI_LIST                     AS TBL_L\n    ON TBL_L.KOBAI_NO                   =  TBL_B.KOBAI_NO\n   AND TBL_L.KOBAI_SEQ                  =  TBL_B.KOBAI_SEQ\n  -- 商品マスタ\n  LEFT JOIN\n       M_SHOHIN                         AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD               =  TBL_B.KOBAIHIN_CD\n  -- 商品分類区分\n  LEFT JOIN\n       M_MEISHO_B                       AS J_SHOHIN_BUNRUI\n    ON J_SHOHIN_BUNRUI.MEISHO_KBN       =  \qK025\q\n   AND J_SHOHIN_BUNRUI.MEISHO_CD        =  J_SHOHIN.SHOHIN_BUNRUI\n\n
