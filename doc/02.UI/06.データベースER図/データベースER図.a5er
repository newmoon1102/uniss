[View]
PName=V_MITSU_SHOSAI_H_R
LName=見積履歴詳細(ヘッダ)ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT T_MITSU_H_R.MITSU_DATE           AS MITSU_DATE           --見積日\n      ,T_MITSU_H_R.MITSU_NO             AS MITSU_NO             --見積No.\n      ,T_MITSU_H_R.MODE_KBN             AS MODE_KBN             --見積モード\n      ,T_MITSU_H_R.MITSU_STS            AS MITSU_STS            --見積ステータスCD\n      ,J_MITSU_STS.DATA_1               AS MITSU_STS_MEI        --見積ステータス名\n      ,T_MITSU_H_R.NYURYOKUSHA_CD       AS NYURYOKUSHA_CD       --入力者CD\n      ,J_NYURYOKUSHA.SHIMEI             AS NYURYOKUSHA_MEI      --入力者名\n      ,T_MITSU_H_R.TORIHIKI_BUNRUI      AS TORIHIKI_BUNRUI_CD   --取引分類CD\n      ,J_TORIHIKI_BUNRUI.DATA_1         AS TORIHIKI_BUNRUI_MEI  --取引分類名\n      ,T_MITSU_H_R.REF_MITSU_NO         AS REF_MITSU_NO         --参照元見積No.\n      ,T_MITSU_H_R.KOYOMI_KBN           AS KOYOMI_KBN           --西暦和暦区分\n      ,T_MITSU_H_R.SEIKYU_CD            AS SEIKYU_CD            --請求先CD\n      ,T_MITSU_H_R.SEIKYU_MEI           AS SEIKYU_MEI           --請求先名\n      ,T_MITSU_H_R.SEMPO_TANTO_MEI      AS SEMPO_TANTO_MEI      --先方担当者名\n      ,T_MITSU_H_R.KEISHO               AS KEISHO               --敬称\n      ,T_MITSU_H_R.KENMEI               AS KENMEI               --件名\n      ,T_MITSU_H_R.EIGYO_TANTO_CD       AS EIGYO_TANTO_CD       --営業担当者CD\n      ,J_EIGYO_TANTO.SHIMEI             AS EIGYO_TANTO_MEI      --営業担当者名 \n      ,T_MITSU_H_R.MEMO                 AS MEMO                 --メモ\n      ,T_MITSU_H_R.UKE_KIJITSU          AS UKE_KIJITSU          --受渡期日\n      ,T_MITSU_H_R.UKE_BASHO            AS UKE_BASHO            --受渡場所\n      ,T_MITSU_H_R.TORI_JOKEN           AS TORI_JOKEN           --取引条件\n      ,T_MITSU_H_R.YUKO_KIGEN           AS YUKO_KIGEN           --有効期限\n      ,T_MITSU_H_R.TANTO_MARK_FLG       AS TANTO_MARK_FLG       --担当者印フラグ\n      ,T_MITSU_H_R.DAIHYO_MEI_FLG       AS DAIHYO_MEI_FLG       --代表者名フラグ\n      ,T_MITSU_H_R.KAISHA_MARK_FLG      AS KAISHA_MARK_FLG      --会社各印フラグ\n      ,T_MITSU_H_R.SHONIN_RAN_FLG       AS SHONIN_RAN_FLG       --承認印欄フラグ\n      ,T_MITSU_H_R.ZEIKOMI_PRINT_FLG    AS ZEIKOMI_PRINT_FLG    --税込印刷フラグ\n      ,J_AITESAKI.SEIKYU_HASU_KBN       AS HASU_KBN             --請求先端数区分\n      ,T_MITSU_H_R.DEL_FLG              AS DEL_FLG              --削除フラグ\n      ,T_MITSU_H_R.RIREKI_NO            AS RIREKI_NO            --履歴No.\n  FROM T_MITSU_H_R\n  --入力者\n  LEFT JOIN\n       M_SHAIN AS J_NYURYOKUSHA\n    ON J_NYURYOKUSHA.SHAIN_CD = T_MITSU_H_R.NYURYOKUSHA_CD\n  --営業担当者\n  LEFT JOIN\n       M_SHAIN AS J_EIGYO_TANTO\n    ON J_EIGYO_TANTO.SHAIN_CD = T_MITSU_H_R.EIGYO_TANTO_CD\n  --見積ステータス\n  LEFT JOIN\n       M_MEISHO_B AS J_MITSU_STS\n    ON J_MITSU_STS.MEISHO_KBN = \qS002\q\n   AND J_MITSU_STS.MEISHO_CD  = T_MITSU_H_R.MITSU_STS\n  --取引分類\n  LEFT JOIN\n       M_MEISHO_B AS J_TORIHIKI_BUNRUI\n    ON J_TORIHIKI_BUNRUI.MEISHO_KBN = \qK010\q\n   AND J_TORIHIKI_BUNRUI.MEISHO_CD  = T_MITSU_H_R.TORIHIKI_BUNRUI\n  --相手先\n  LEFT JOIN\n       M_AITESAKI AS J_AITESAKI\n    ON J_AITESAKI.AITE_CD = T_MITSU_H_R.SEIKYU_CD\n
LNameByComment=1
