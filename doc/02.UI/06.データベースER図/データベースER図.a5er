Comment=UNKB0130(受入検収検索)、UNKB0140(受入検収入力)－修正
ViewOption=
Query=--*DataTitle V_UKEIRE_KENSHU_LIST(受入検収検索リストビュー)\nSELECT TBL_V.GYO_MOJI                  AS  GYO_MOJI                  -- 選択\n      ,TBL_N.NYUSHUKKO_NO              AS  NYUSHUKKO_NO              -- 入出庫No.\n      ,TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,CONVERT(VARCHAR(10),TBL_V.SHINSEI_DATETIME,111)\n                                       AS  SHINSEI_DATE              -- 申請日\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_N.NYUKO_SURYO               AS  NYUKO_SURYO               -- 入庫数量\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_V.NOKI                      AS  NOKI                      -- 希望納期\n      ,TBL_N.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_N.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_V.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_N.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN           -- 保管場所区分\n      ,CASE ISNULL(TBL_N.KENSHU_DATE,\q\q)\n       WHEN \q\q THEN \qTrue\q\n       ELSE \qFalse\q\n       END                             AS  MIKENSHU_FLG              -- 未検収フラグ\n      ,TBL_V.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n INNER JOIN                                                          -- 【入出庫テーブル】\n       T_NYUSHUKKO                     AS  TBL_N\n    ON TBL_N.IRAI_NO                   =   TBL_V.IRAI_NO\n WHERE TBL_V.KOBAI_STS                 IN  (3,4,5,6)\n   AND TBL_N.NYUSHUKKO_KBN             =   1\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="入出庫関連",1350,100
Page=入出庫関連
Left=1350
Top=100

[View]
PName=V_ZAIKO_DATA
LName=在庫調整入力データビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT V_C_ZAIKO.ZAIKO_NO             AS ZAIKO_NO             --在庫管理No.\n      ,V_C_ZAIKO.HOKAN_BASHO_KBN      AS HOKAN_BASHO_KBN      --保管場所区分\n      ,V_C_ZAIKO.HOKAN_BASHO_KBN_MEI  AS HOKAN_BASHO_KBN_MEI  --保管場所\n      ,V_C_ZAIKO.KOBAIHIN_CD          AS KOBAIHIN_CD          --購買品CD\n      ,V_C_ZAIKO.KOBAIHIN_MEI         AS KOBAIHIN_MEI         --購買品名\n      ,V_C_ZAIKO.TANKA                AS TANKA                --単価\n      ,V_C_ZAIKO.ZAIKO_SURYO          AS ZAIKO_SURYO          --在庫数量\n      ,V_C_ZAIKO.BARA_TANI            AS BARA_TANI            --バラ単位\n      ,V_C_ZAIKO.IRISU                AS IRISU                --入数\n      ,V_C_ZAIKO.IRISU_TANI           AS IRISU_TANI           --入数単位\n      ,V_C_ZAIKO.SHIIRE_CD            AS SHIIRE_CD            --仕入先CD\n      ,V_C_ZAIKO.SHIIRE_MEI           AS SHIIRE_MEI           --仕入先名\n      ,V_C_ZAIKO.MAKER_CD             AS MAKER_CD             --メーカーCD\n      ,V_C_ZAIKO.MAKER_MEI            AS MAKER_MEI            --メーカー名\n      ,V_C_ZAIKO.LAST_NYUKO_DATE      AS LAST_NYUKO_DATE      --最終入庫日\n      ,V_C_ZAIKO.LAST_SHUKKO_DATE     AS LAST_SHUKKO_DATE     --最終出庫日\n  FROM V_C_ZAIKO\n  LEFT JOIN\n       M_SHOHIN\n    ON M_SHOHIN.SHOHIN_CD = V_C_ZAIKO.KOBAIHIN_CD\n WHERE M_SHOHIN.SHIYAKU_FLG = \qTrue\q\n\n
