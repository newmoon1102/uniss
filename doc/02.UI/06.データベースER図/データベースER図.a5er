Query=SELECT V_C_KOBAI_B.KOBAI_GYO_NO         AS KOBAI_GYO_NO         --行\n      ,NULL                             AS ALERT                --警告\n      ,NULL                             AS JOGAI_CHECK          --除外チェックボックス\n      ,V_C_KOBAI_B.JOGAI_FLG            AS JOGAI_FLG            --除外フラグ\n      ,V_C_KOBAI_B.KOBAI_SEQ            AS KOBAI_SEQ            --SEQ\n      ,V_C_KOBAI_B.KOBAI_STS            AS KOBAI_STS            --ステータスCD\n      ,V_C_KOBAI_B.HOKAN_BASHO_KBN      AS HOKAN_BASHO_KBN      --保管場所区分\n      ,V_C_KOBAI_B.YOSAN_MEI            AS YOSAN_MEI            --予算コード名\n      ,V_C_KOBAI_B.SHIIRE_MEI           AS SHIIRE_MEI           --仕入先名\n      ,V_C_KOBAI_B.MAKER_MEI            AS MAKER_MEI            --メーカー名\n      ,J_SHIIRE.SHIIRE_HASU_KBN         AS SHIIRE_HASU_KBN      --仕入先端数処理区分\n      ,V_C_KOBAI_B.SHIKIICHI            AS SHIKIICHI            --閾値\n      ,V_C_KOBAI_B.HYOJUN_NOKI          AS HYOJUN_NOKI          --標準納期\n      ,V_C_KOBAI_B.LAST_SHINSEI_DATE    AS LAST_SHINSEI_DATE    --最終申請日\n      ,V_C_KOBAI_B.SHOHIN_BUNRUI_CD     AS SHOHIN_BUNRUI_CD     --商品分類区分\n      ,V_C_KOBAI_B.SHIYAKU_FLG          AS SHIYAKU_FLG          --試薬フラグ\n      ,V_C_KOBAI_B.KOBAIHIN_KBN         AS KOBAIHIN_KBN         --購買品区分\n      ,V_C_KOBAI_B.KOBAIHIN_CD          AS KOBAIHIN_CD          --購買品CD\n      ,V_C_KOBAI_B.KOBAIHIN_MEI         AS KOBAIHIN_MEI         --購買品名\n      ,V_C_KOBAI_B.TANKA                AS TANKA                --単価\n      ,V_C_KOBAI_B.IRISU                AS IRISU                --入数\n      ,V_C_KOBAI_B.BARA_TANI            AS BARA_TANI            --バラ単位\n      ,V_C_KOBAI_B.SURYO                AS SURYO                --数量\n      ,V_C_KOBAI_B.IRISU_TANI           AS IRISU_TANI           --入数単位\n      ,V_C_KOBAI_B.SOSU                 AS SOSU                 --注文総数\n      ,V_C_KOBAI_B.TOTAL                AS TOTAL                --合計金額\n      ,V_C_KOBAI_B.NOKI                 AS NOKI                 --希望納期\n      ,V_C_KOBAI_B.HOKAN_BASHO_KBN_MEI  AS HOKAN_BASHO_KBN_MEI  --保管場所\n      ,V_C_KOBAI_B.SHIIRE_CD            AS SHIIRE_CD            --仕入先CD\n      ,NULL                             AS SHIIRE_MEI_DISP      --仕入先名(表示)\n      ,V_C_KOBAI_B.MAKER_CD             AS MAKER_CD             --メーカーCD\n      ,NULL                             AS MAKER_MEI_DISP       --メーカー名(表示)\n      ,V_C_KOBAI_B.YOSAN_CD             AS YOSAN_CD             --予算コード\n      ,V_C_KOBAI_B.JURI_NO              AS JURI_NO              --受理No.\n      ,V_C_KOBAI_B.BIKO                 AS BIKO                 --備考\n      ,V_C_KOBAI_B.KOBAI_STS_MEI        AS KOBAI_STS_MEI        --ステータス名\n      ,T_IRAI.IRAI_NO                   AS IRAI_NO              --依頼No.\n      ,V_C_KOBAI_B.KOBAI_NO             AS KOBAI_NO             --購買申請No.\n  FROM V_C_KOBAI_B\n  LEFT JOIN\n       T_IRAI\n    ON T_IRAI.KOBAI_NO  = V_C_KOBAI_B.KOBAI_NO\n   AND T_IRAI.KOBAI_SEQ = V_C_KOBAI_B.KOBAI_SEQ\n  LEFT JOIN\n       M_AITESAKI   AS J_SHIIRE\n    ON J_SHIIRE.AITE_CD = V_C_KOBAI_B.SHIIRE_CD\n\n
LNameByComment=1
Color=$000000
