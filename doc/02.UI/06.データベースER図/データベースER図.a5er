Query=SELECT \q＞\q                     AS GYO_MOJI             --選択\n      ,CASE M_SHOHIN.BUNSEKI_FLG            \n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                      AS BUNSEKI_FLG          --分析項目フラグ\n      ,CASE M_SHOHIN.SHOHIN_FLG         \n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                      AS HAMBAI_SHOHIN_FLG    --販売商品フラグ\n      ,CASE M_SHOHIN.KOBAIHIN_FLG        \n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                      AS KOBAIHIN_FLG         --購買品フラグ\n      ,CASE M_SHOHIN.SHOKEIHI_FLG   \n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                      AS SHOKEIHI_FLG         --諸経費フラグ\n      ,M_SHOHIN.SHOHIN_CD       AS SHOHIN_CD            --商品CD\n      ,M_SHOHIN.SHOHIN_MEI      AS SHOHIN_MEI           --商品名\n      ,M_SHOHIN.AITE_SHOHIN_MEI AS AITESAKI_SHOHIN_MEI  --相手先商品名\n      ,SHOHIN_STS.DATA_1        AS STATUS               --商品ステータス\n      ,SHIIRESAKI.AITE_MEI      AS SHIIRESAKI_MEI       --仕入先名\n      ,MAKER.AITE_MEI           AS MAKER_MEI            --メーカー名\n      ,CASE M_SHOHIN.MISHIYO_FLG \n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                      AS MISHIYO_FLG          --未使用フラグ\n      ,M_SHOHIN.SHIIRE_CD       AS SHIIRESAKI_CD        --仕入先CD\n      ,M_SHOHIN.MAKER_CD        AS MAKER_CD             --メーカーCD\n      ,\q|\q + M_SHOHIN.SHOHIN_MEI + \n       \q|\q + M_SHOHIN.AITE_SHOHIN_MEI + \n       \q|\q                      AS FREEWD               --フリーワード検索用\n\n  FROM M_SHOHIN \n  LEFT JOIN \n       M_MEISHO_B               AS SHOHIN_STS\n    ON SHOHIN_STS.MEISHO_KBN = \qS001\q\n   AND M_SHOHIN.SHOHIN_STS   = SHOHIN_STS.MEISHO_CD\n  LEFT JOIN \n       M_AITESAKI               AS SHIIRESAKI\n    ON M_SHOHIN.SHIIRE_CD    = SHIIRESAKI.AITE_CD\n  LEFT JOIN\n       M_AITESAKI               AS MAKER \n   ON  M_SHOHIN.MAKER_CD     = MAKER.AITE_CD\n
