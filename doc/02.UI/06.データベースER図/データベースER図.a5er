[View]
PName=V_NYUSHUKKO_LIST
LName=入出庫リストビュー
Tag=ビュー
Comment=入出庫関連ビューの元となるビュー
ViewOption=
Query=--*DataTitle V_NYUSHUKKO_LIST(入出庫リストビュー)\nSELECT \q＞\q                            AS  GYO_MOJI                  -- 選択\n      ,TBL_N.SHUKKO_DATE               AS  SHUKKO_DATE               -- 出庫日\n      ,TBL_N.NYURYOKUSHA_MEI           AS  NYURYOKUSHA_MEI           -- 入力者名\n      ,TBL_Z.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_Z.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_Z.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_Z.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_Z.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_N.SHUKKO_SURYO              AS  SHUKKO_SURYO              -- 出庫数量\n      ,TBL_Z.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_Z.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI_Z     -- 保管場所区分名(在庫管理)\n      ,TBL_N.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI_N     -- 保管場所区分名(入出庫)\n      ,TBL_Z.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_Z.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_Z.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_Z.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_N.SHUKKO_JIYU               AS  SHUKKO_JIYU               -- 出庫事由\n      ,TBL_N.NYUSHUKKO_NO              AS  NYUSHUKKO_NO              -- 入出庫No.\n      ,TBL_N.ZAIKO_NO                  AS  ZAIKO_NO                  -- 在庫管理No.\n      ,TBL_N.JURI_NO                   AS  JURI_NO_N                 -- 受理No.(入出庫)\n      ,TBL_N.NYURYOKUSHA_CD            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,TBL_Z.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN_Z         -- 保管場所区分(在庫管理)\n      ,TBL_N.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN_N         -- 保管場所区分(入出庫)\n      ,V_K.UKEIRE_DATE                 AS  UKEIRE_DATE               -- 受入日\n      ,V_K.KENSHU_DATE                 AS  KENSHU_DATE               -- 検収日\n      ,V_K.YOSAN_CD                    AS  YOSAN_CD                  -- 予算コード\n      ,V_K.BIKO                        AS  BIKO                      -- 備考\n      ,V_K.JURI_NO                     AS  JURI_NO_K                 -- 受理No.(購買申請)\n      ,TBL_Z.FREEWD\n      +TBL_N.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n      ,TBL_N.NYUSHUKKO_KBN             AS  NYUSHUKKO_KBN             -- 入出庫区分\n  FROM V_C_ZAIKO                       AS  TBL_Z                     -- 【在庫管理共通ビュー】\n INNER JOIN                                                          -- 【入出庫共通ビュー】\n       V_C_NYUSHUKKO                   AS  TBL_N\n    ON TBL_N.ZAIKO_NO                  =   TBL_Z.ZAIKO_NO\n\n  LEFT JOIN                                                          -- 【入出庫テーブル】（財務管理No.ごとに依頼No.が最大のレコード）\n     ( SELECT TBL_1.ZAIKO_NO                        -- 在庫管理No.\n             ,MAX(TBL_1.IRAI_NO)       AS  IRAI_NO  -- 依頼No.\n         FROM T_NYUSHUKKO              AS  TBL_1\n        GROUP BY\n              TBL_1.ZAIKO_NO\n     )                                 AS  TBL_2\n    ON TBL_2.ZAIKO_NO                  =   TBL_N.ZAIKO_NO\n\n  LEFT JOIN                                                          -- 【購買申請リストビュー】\n       V_KOBAI_LIST                    AS  V_K\n    ON V_K.IRAI_NO                     =   TBL_2.IRAI_NO\n
LNameByComment=1
