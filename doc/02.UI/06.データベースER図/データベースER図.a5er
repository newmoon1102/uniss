[View]
PName=V_URIAGE_JUCHU_SELECT
LName=受注選択ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT\n       \q＞\q                                 AS SENTAKU              --選択\n      ,J_BUN_HAN_KBN_MEISHO.MEISHO_CD       AS JUCHU_KBN_CD         --受注区分CD\n      ,J_BUN_HAN_KBN_MEISHO.DATA_1          AS JUCHU_KBN_MEI        --受注区分\n      ,V_C_BUN_JUCHU_H.BUNSEKI_STS_MEI      AS STS                  --ステータス\n      ,V_C_BUN_JUCHU_H.JUCHU_KBN_MEI        AS BUN_HAN_KBN_MEI      --分析販売区分\n      ,V_C_BUN_JUCHU_H.JURI_NO              AS JURI_NO              --受理No.\n      ,V_C_BUN_JUCHU_H.KENMEI               AS KENMEI               --件名\n      ,V_C_BUN_JUCHU_H.SEIKYU_CD            AS SEIKYU_CD            --請求先CD\n      ,V_C_BUN_JUCHU_H.SEIKYU_MEI           AS SEIKYU_MEI           --請求先名\n      ,V_C_BUN_JUCHU_H.NYURYOKUSHA_MEI      AS NYURYOKUSHA_MEI      --入力者名\n      ,V_C_BUN_JUCHU_H.EIGYO_TANTO_MEI      AS EIGYO_TANTO_MEI      --営業担当者名\n      ,CONVERT(VARCHAR(10),V_C_BUN_JUCHU_H.NYURYOKU_DATETIME,111)\n                                            AS NYURYOKU_DATE        --入力日\n      ,V_C_BUN_JUCHU_H.NOHIN_DATE           AS NOHIN_DATE           --納品日\n      ,TBL_EDA_KENSU.EDA_KENSU              AS EDA_KENSU            --枝番件数\n      ,TBL_SHOHIN_KENSU.SHOHIN_KENSU        AS SHOHIN_KENSU         --商品件数\n      ,V_C_BUN_JUCHU_H.MITSU_NO             AS MITSU_NO             --見積No.\n\n  FROM V_C_BUN_JUCHU_H\n\n  LEFT JOIN M_MEISHO_B                      AS J_BUN_HAN_KBN_MEISHO       --分析／販売区分名称用\n    ON J_BUN_HAN_KBN_MEISHO.MEISHO_KBN      =  \qK030\q\n   AND J_BUN_HAN_KBN_MEISHO.MEISHO_CD       =  1\n\n  LEFT JOIN                                                               --枝番件数用\n      (SELECT TBL_B.JURI_NO\n             ,COUNT(*)                      AS  EDA_KENSU\n         FROM T_BUN_JUCHU_B                 AS  TBL_B\n        GROUP BY\n              TBL_B.JURI_NO\n      )                                     AS  TBL_EDA_KENSU\n    ON TBL_EDA_KENSU.JURI_NO                =   V_C_BUN_JUCHU_H.JURI_NO\n\n  LEFT JOIN                                                               --商品件数用\n      (SELECT TBL_B.MITSU_NO\n             ,COUNT(*)                      AS  SHOHIN_KENSU\n         FROM T_MITSU_B                     AS  TBL_B\n        WHERE TBL_B.ROW_KBN                 =   \q1\q\n        GROUP BY\n              TBL_B.MITSU_NO\n      )                                     AS  TBL_SHOHIN_KENSU\n    ON TBL_SHOHIN_KENSU.MITSU_NO            =   V_C_BUN_JUCHU_H.MITSU_NO\n\n WHERE V_C_BUN_JUCHU_H.DEL_FLG              =  \qFalse\q\n\nUNION ALL\n\nSELECT\n       \q＞\q\n      ,J_BUN_HAN_KBN_MEISHO.MEISHO_CD\n      ,J_BUN_HAN_KBN_MEISHO.DATA_1\n      ,V_HAN_JUCHU_H.HAMBAI_STS\n      ,V_HAN_JUCHU_H.HAMBAI_KBN\n      ,V_HAN_JUCHU_H.JURI_NO\n      ,V_HAN_JUCHU_H.KENMEI\n      ,V_HAN_JUCHU_H.SEIKYUSAKI_CD\n      ,V_HAN_JUCHU_H.SEIKYUSAKI_MEI\n      ,V_HAN_JUCHU_H.NYURYOKUSHA\n      ,V_HAN_JUCHU_H.EIGYO_TANTO\n      ,CONVERT(VARCHAR(10),V_HAN_JUCHU_H.NYURYOKU_DATETIME,111)\n      ,V_HAN_JUCHU_H.NOHIN_DATE\n      ,NULL\n      ,TBL_SHOHIN_KENSU.SHOHIN_KENSU\n      ,V_HAN_JUCHU_H.MITSU_NO\n\n  FROM V_HAN_JUCHU_H\n\n  LEFT JOIN M_MEISHO_B                      AS J_BUN_HAN_KBN_MEISHO         --分析／販売区分名称用\n    ON J_BUN_HAN_KBN_MEISHO.MEISHO_KBN      =  \qK030\q\n   AND J_BUN_HAN_KBN_MEISHO.MEISHO_CD       =  2\n\n  LEFT JOIN                                                                 --商品件数用\n      (SELECT TBL_B.JURI_NO\n             ,COUNT(*)                      AS  SHOHIN_KENSU\n         FROM T_HAN_JUCHU_B                 AS  TBL_B\n        WHERE TBL_B.ROW_KBN                 =   \q1\q\n        GROUP BY\n              TBL_B.JURI_NO\n      )                                     AS  TBL_SHOHIN_KENSU\n    ON TBL_SHOHIN_KENSU.JURI_NO             =   V_HAN_JUCHU_H.JURI_NO\n\n WHERE V_HAN_JUCHU_H.DEL_FLG                =  \qFalse\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="売上関連",1350,1800
Page=売上関連
Left=1350
Top=1800

