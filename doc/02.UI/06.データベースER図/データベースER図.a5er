ViewOption=
Query=--*DataTitle V_BUN_JUCHU_LIST(分析受注リストビュー)\nSELECT \q＞\q                                             AS  GYO_MOJI                  -- 選択\n      ,RTRIM(CONVERT(CHAR,TBL_H.NYURYOKU_DATETIME,111)) AS  NYURYOKU_DATE             -- 入力日\n      ,TBL_H.BUNSEKI_STS_MEI                            AS  BUNSEKI_STS               -- ステータス\n      ,TBL_H.JURI_NO                                    AS  JURI_NO                   -- 受理No.\n      ,TBL_H.ATENA                                      AS  ATENA                     -- 報告書宛名\n      ,TBL_H.KENMEI                                     AS  KENMEI                    -- 件名\n      ,TBL_H.IRAI_DATE                                  AS  IRAI_DATE                 -- 依頼日/受付日\n      ,TBL_H.CHUKAN_DATE_1                              AS  CHUKAN_DATE_1             -- 中間報告日①\n      ,TBL_H.CHUKAN_DATE_2                              AS  CHUKAN_DATE_2             -- 中間報告日②\n      ,TBL_H.CHUKAN_DATE_3                              AS  CHUKAN_DATE_3             -- 中間報告日③\n      ,TBL_H.KANSEI_DATE                                AS  KANSEI_DATE               -- 報告書完成日\n      ,TBL_H.NOHIN_DATE                                 AS  NOHIN_DATE                -- 納品日\n      ,TBL_H.EIGYO_TANTO_MEI                            AS  EIGYO_TANTO_MEI           -- 営業担当者名\n      ,TBL_H.NYURYOKUSHA_MEI                            AS  NYURYOKUSHA_MEI           -- 入力者名\n      ,J_UKETSUKE_TANTO.SHIMEI                          AS  UKETSUKESHA_MEI        -- 受付確認担当者名\n      ,TBL_H.BUSHO_MEI                                  AS  BUSHO_MEI                 -- 担当部署名\n      ,TBL_H.KEISHIKI_KBN_MEI                           AS  HOKOKU_KEISHIKI           -- 報告書形式\n      ,TBL_H.SEIKYU_MEI                                 AS  SEIKYUSAKI_MEI            -- 請求先名\n      ,TBL_H.SOKUHO_KBN_MEI                             AS  SOKUHO_KBN_MEI            -- 速報区分名\n      ,TBL_H.MITSU_NO                                   AS  MITSU_NO                  -- 見積No.\n      ,TBL_H.DEMPYO_HAKKO_KBN_MEI                       AS  DEMPYO_HAKKO              -- 伝票発行\n      ,CASE TBL_H.DEL_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                              AS  DEL_FLG                   -- 削除フラグ\n      ,TBL_H.KEISHIKI_KBN                               AS  KEISHIKI_CD               -- 報告書形式区分コード\n      ,TBL_H.ATENA_CD                                   AS  ATENA_CD                  -- 報告書宛名CD\n      ,TBL_H.SEIKYU_CD                                  AS  SEIKYU_CD                 -- 請求先CD\n      ,TBL_H.BUNSEKI_STS                                AS  BUN_STS_CD                -- 分析ステータス区分コード\n      ,TBL_H.DEMPYO_HAKKO_KBN                           AS  DEMPYO_HAKKO_CD           -- 伝票発行区分コード\n      ,TBL_H.JUCHU_KBN                                  AS  JUCHU_KBN_CD              -- 分析区分コード\n      ,TBL_H.EIGYO_TANTO_CD                             AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,TBL_H.NYURYOKUSHA_CD                             AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,TBL_H.BUSHO_CD                                   AS  BUSHO_CD                  -- 担当部署CD\n      ,TBL_H.BUNSEKI_HOHO_CD                            AS  BUNSEKI_HOHO_CD           -- 分析方法CD\n      ,TBL_H.BUNSEKI_HOHO                               AS  BUNSEKI_HOHO              -- 分析方法名\n      ,TBL_H.FREEWD2                                    AS  FREEWD                    -- フリーワード検索用\n\n      ,TBL_R.DBS_CREATE_USER                            AS  UKETSUKESHA_CD         -- 受付確認担当者CD\n\n  FROM V_C_BUN_JUCHU_H                                  AS  TBL_H                     -- 【分析受注ヘッダ共通ビュー】\n  LEFT JOIN\n       T_BUN_STS_R                                      AS  TBL_R\n    ON TBL_R.JURI_NO                                    =   TBL_H.JURI_NO\n   AND TBL_R.AFTER_STS                                  =   \q2\q\n   AND TBL_R.MOD_DATE_TIME                              =   ( SELECT MAX(MOD_DATE_TIME)\n                                                                FROM T_BUN_STS_R      AS TBL_R2\n                                                               WHERE TBL_R2.JURI_NO   =  TBL_R.JURI_NO\n                                                                 AND TBL_R2.AFTER_STS =  \q2\q )\n  LEFT JOIN\n       M_SHAIN                                          AS  J_UKETSUKE_TANTO\n    ON J_UKETSUKE_TANTO.SHAIN_CD                        =   TBL_R.DBS_CREATE_USER\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="分析受注関連",700,2050
Page=分析受注関連
Left=700
Top=2050

[View]
PName=V_HAN_JUCHU_LIST
LName=販売商品受注リストビュー
Tag=ビュー
Comment=検索画面用
ViewOption=
Query=SELECT \q＞\q                                            AS  GYO_MOJI             --行文字\n      ,HAMBAI_STS.DATA_1                               AS  HAMBAI_STS           --ステータス\n      ,HAMBAI_KBN.DATA_1                               AS  HAMBAI_KBN           --販売商品区分\n      ,T_HAN_JUCHU_H.JURI_NO                           AS  JURI_NO              --受理No.\n      ,SEIKYU.AITE_MEI                                 AS  SEIKYUSAKI_MEI       --請求先名\n      ,T_HAN_JUCHU_H.KENMEI                            AS  KENMEI               --件名\n      ,T_HAN_JUCHU_H.JUCHU_DATE                        AS  JUCHU_DATE           --受注日\n      ,T_HAN_JUCHU_H.NOHIN_DATE                        AS  NOHIN_DATE           --納品日\n      ,T_HAN_JUCHU_H.HASSO_MEI                         AS  HASSO_MEI            --発送先名\n      ,CASE WHEN DOKU_TBL.CNT IS NULL\n       THEN NULL\n       ELSE \q×\q END                                   AS  DOKU_MARK            --毒劇物フラグ\n      ,EIGYO.SHIMEI                                    AS  EIGYO_SHIMEI         --営業担当者名\n      ,NYURYOKU.SHIMEI                                 AS  NYURYOKU_MEI         --入力者名\n      ,T_HAN_JUCHU_H.MITSU_NO                          AS  MITSU_NO             --見積No\n      ,DEMPYO.DATA_1                                   AS  DEMPYO_HAKKO_KBN     --伝票発行区分\n      ,CASE T_HAN_JUCHU_H.DEL_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                             AS  DEL_FLG              --削除フラグ\n      ,T_HAN_JUCHU_H.SEIKYU_CD                         AS  SEIKYUSAKI_CD        --請求先CD\n      ,T_HAN_JUCHU_H.HASSO_CD                          AS  HASSO_CD             --発送先CD\n      ,T_HAN_JUCHU_H.EIGYO_TANTO_CD                    AS  EIGYO_TANTO_CD       --営業担当CD\n      ,T_HAN_JUCHU_H.NYURYOKUSHA_CD                    AS  NYURYOKUSHA_CD       --入力者CD\n      ,T_HAN_JUCHU_H.HAMBAI_STS                        AS  HAMBAI_STS_CD        --販売ステータスCD\n      ,T_HAN_JUCHU_H.HAMBAI_KBN                        AS  HAMBAI_KBN_CD        --販売区分CD\n      ,T_HAN_JUCHU_H.DEMPYO_HAKKO_KBN                  AS  DEMPYO_HAKKO_KBN_CD  --伝票発行区分\n      , \q|\q + T_HAN_JUCHU_H.SEIKYU_CD           +  -- 請求先CD\n        \q|\q + ISNULL(SEIKYU.AITE_MEI,\q\q)        +  -- 請求先名\n        \q|\q + T_HAN_JUCHU_H.SEIKYU_TANTO_MEI    +  -- 先方担当者名\n        \q|\q + T_HAN_JUCHU_H.KENMEI              +  -- 件名\n        \q|\q + T_HAN_JUCHU_H.HASSO_MEI           +  -- 発送先名\n        \q|\q + T_HAN_JUCHU_H.HASSO_TANTO_MEI     +  -- 発送先担当者名\n        \q|\q + TBL_F.FREEWD                      +  -- フリーワード(ボディ)\n        \q|\q                                            AS FREEWD                --フリーワード検索用\n  FROM T_HAN_JUCHU_H\n  LEFT JOIN\n       M_MEISHO_B                                      AS  HAMBAI_STS\n    ON HAMBAI_STS.MEISHO_KBN = \qS004\q\n   AND HAMBAI_STS.MEISHO_CD = T_HAN_JUCHU_H.HAMBAI_STS\n  LEFT JOIN\n       M_MEISHO_B                                      AS  HAMBAI_KBN\n    ON HAMBAI_KBN.MEISHO_KBN = \qK020\q\n   AND HAMBAI_KBN.MEISHO_CD = T_HAN_JUCHU_H.HAMBAI_KBN\n--*営業担当者名\n  LEFT JOIN\n       M_SHAIN                                         AS  EIGYO\n    ON T_HAN_JUCHU_H.EIGYO_TANTO_CD = EIGYO.SHAIN_CD\n--*入力者名\n  LEFT JOIN\n       M_SHAIN                                         AS  NYURYOKU\n    ON T_HAN_JUCHU_H.NYURYOKUSHA_CD = NYURYOKU.SHAIN_CD\n--*請求先名\n  LEFT JOIN\n       M_AITESAKI                                      AS  SEIKYU\n    ON T_HAN_JUCHU_H.SEIKYU_CD = SEIKYU.AITE_CD\n--*毒劇物フラグ\n  LEFT JOIN( SELECT JURI_NO\n                   ,COUNT(*)                           AS  CNT\n               FROM T_HAN_JUCHU_B\n              INNER JOIN M_SHOHIN                      AS  DOKUGEKI\n                 ON T_HAN_JUCHU_B.SHOHIN_CD = DOKUGEKI.SHOHIN_CD\n                AND DOKUGEKI.DOKU_FLG = \qTrue\q\n              GROUP BY T_HAN_JUCHU_B.JURI_NO\n            )                                          AS DOKU_TBL\n    ON DOKU_TBL.JURI_NO = T_HAN_JUCHU_H.JURI_NO\n  LEFT JOIN\n       M_MEISHO_B                                      AS DEMPYO\n    ON DEMPYO.MEISHO_KBN = \qK026\q\n   AND DEMPYO.MEISHO_CD = T_HAN_JUCHU_H.DEMPYO_HAKKO_KBN\n\nLEFT  JOIN                                                                      --フリーワード(ボディ)\n    (\n      SELECT TBL_C.JURI_NO\n            ,TBL_C.SHOHIN_CD\n            ,TBL_C.SHOHIN_MEI\n            ,TBL_C.TEKIYO\n            ,TBL_C.SHOHIN_CD                       -- 商品CD\n            +TBL_C.SHOHIN_MEI                      -- 商品名\n            +TBL_C.TEKIYO                          -- 摘要\n            +TBL_C.SHOHIN_BUNRUI                   -- 商品分類CD\n            +TBL_C.SHIIRE_CD                       -- 仕入先CD\n            +TBL_C.MAKER_CD                        -- メーカーCD\n            AS  FREEWD\n        FROM\n           (\n             SELECT TBL_A.JURI_NO\n                   ,(\n                     SELECT \q|\q + TBL_B.SHOHIN_CD  AS [data()]\n                       FROM T_HAN_JUCHU_B TBL_B\n                      WHERE TBL_B.JURI_NO           =  TBL_A.JURI_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS SHOHIN_CD\n                   ,(\n                     SELECT \q|\q + TBL_B.SHOHIN_MEI  AS [data()]\n                       FROM T_HAN_JUCHU_B TBL_B\n                      WHERE TBL_B.JURI_NO           =  TBL_A.JURI_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS SHOHIN_MEI\n                   ,(\n                     SELECT \q|\q + TBL_B.TEKIYO      AS [data()]\n                       FROM T_HAN_JUCHU_B TBL_B\n                      WHERE TBL_B.JURI_NO           =  TBL_A.JURI_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS TEKIYO\n                   ,(\n                     SELECT \q|K025:\q + M_SHOHIN.SHOHIN_BUNRUI AS [data()]\n                       FROM T_HAN_JUCHU_B TBL_B\n                       LEFT JOIN\n                            M_SHOHIN\n                         ON M_SHOHIN.SHOHIN_CD = TBL_B.SHOHIN_CD\n                      WHERE TBL_B.JURI_NO           =  TBL_A.JURI_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS SHOHIN_BUNRUI\n                   ,(\n                     SELECT \q|\q + TBL_B.SHIIRE_CD   AS [data()]\n                       FROM T_HAN_JUCHU_B TBL_B\n                      WHERE TBL_B.JURI_NO           =  TBL_A.JURI_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS SHIIRE_CD\n                   ,(\n                     SELECT \q|\q + TBL_B.MAKER_CD    AS [data()]\n                       FROM T_HAN_JUCHU_B TBL_B\n                      WHERE TBL_B.JURI_NO           =  TBL_A.JURI_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS MAKER_CD\n               FROM T_HAN_JUCHU_B TBL_A\n              GROUP BY\n                    TBL_A.JURI_NO\n           ) TBL_C\n    ) TBL_F\n   ON TBL_F.JURI_NO = T_HAN_JUCHU_H.JURI_NO\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="販売商品関連",100,100
Page=販売商品関連
Left=100
Top=100

[View]
PName=V_MITSUMORI_LIST
LName=見積検索リストビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT \q＞\q                         AS GYO_MOJI             --選択\n      ,MITSU_STATUS.DATA_1          AS STATUS               --ステータス\n      ,TORIHIKI_MEISHO.DATA_1       AS TORIHIKI_BUNRUI      --取引分類\n      ,T_MITSU_H.MITSU_NO           AS MITSU_NO             --見積No.\n      ,T_MITSU_H.MITSU_DATE         AS MITSU_DATE           --見積日\n      ,T_MITSU_H.SEIKYU_CD          AS SEIKYU_CD            --請求先CD\n      ,T_MITSU_H.SEIKYU_MEI         AS SEIKYU_MEI           --請求先名\n      ,T_MITSU_H.KENMEI             AS KENMEI               --件名\n      ,EIGYO_TANTO.SHIMEI           AS EIGYO_TANTO_MEI      --営業担当者名\n      ,NYURYOKUSHA.SHIMEI           AS NYURYOKUSHA_MEI      --入力者名\n      ,T_MITSU_H.NUKI_TOTAL         AS NUKI_TOTAL           --税抜合計\n      ,T_MITSU_H.ARARIEKI           AS ARARIEKI             --粗利益\n      ,T_MITSU_H.RIEKI_RATE         AS RIEKI_RATE           --粗利率[%]\n      ,CASE T_MITSU_H.MODE_KBN\n       WHEN 1 THEN \q通常\q\n       ELSE \q単価\q\n       END                          AS MODE                 --種別\n      ,CASE ( SELECT COUNT(*)\n                FROM T_MITSU_B\n                LEFT JOIN\n                     M_SHOHIN\n                  ON M_SHOHIN.SHOHIN_CD = T_MITSU_B.SHOHIN_CD\n               WHERE T_MITSU_B.MITSU_NO = T_MITSU_H.MITSU_NO\n                 AND M_SHOHIN.SHOKEIHI_FLG = \qTrue\q )\n       WHEN 0 THEN \q\q\n       ELSE \q○\q\n       END                          AS SHOKEIHI_FLG         --諸経費フラグ\n      ,CASE T_MITSU_H.DEL_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                          AS DEL_FLG              --削除\n      ,T_MITSU_H.MITSU_DATE         AS MITSU_DATE_NUM       --見積日\n      ,T_MITSU_H.MITSU_STS          AS STATUS_NUM           --見積ステータス(KBN)\n      ,T_MITSU_H.TORIHIKI_BUNRUI    AS TORIHIKI_BUNRUI_KBN  --取引分類(KBN)\n      ,T_MITSU_H.EIGYO_TANTO_CD     AS EIGYO_TANTO_CD       --営業担当者CD\n      ,T_MITSU_H.NYURYOKUSHA_CD     AS NYURYOKUSSHA_CD      --入力者CD\n      ,T_MITSU_H.MODE_KBN           AS MODE_KBN             --モード(KBN)\n      , \q|\q + ISNULL(T_MITSU_H.SEIKYU_MEI     ,\q\q)  +\n        \q|\q + ISNULL(T_MITSU_H.SEMPO_TANTO_MEI,\q\q)  +\n        \q|\q + ISNULL(T_MITSU_H.KENMEI         ,\q\q)  + \n        \q|\q + ISNULL(T_MITSU_H.MEMO           ,\q\q)  +\n        \q|\q + ISNULL(TBL_F.FREEWD             ,\q\q)  +\n        \q|\q                         AS FREEWD               --フリーワード検索用\n\nFROM  T_MITSU_H\nLEFT  JOIN M_MEISHO_B               AS MITSU_STATUS         --見積ステータス用マスタ\n   ON MITSU_STATUS.MEISHO_KBN = \qS002\q\n  AND T_MITSU_H.MITSU_STS = MITSU_STATUS.MEISHO_CD\n\nLEFT  JOIN M_MEISHO_B               AS TORIHIKI_MEISHO      --取引分類用マスタ\n   ON TORIHIKI_MEISHO.MEISHO_KBN = \qK010\q\n  AND T_MITSU_H.TORIHIKI_BUNRUI = TORIHIKI_MEISHO.MEISHO_CD\n\nLEFT  JOIN M_SHAIN                  AS EIGYO_TANTO          --営業担当者名用マスタ\n   ON T_MITSU_H.EIGYO_TANTO_CD = EIGYO_TANTO.SHAIN_CD\n\nLEFT  JOIN M_SHAIN                  AS NYURYOKUSHA          --営業担当者名用マスタ\n   ON T_MITSU_H.NYURYOKUSHA_CD = NYURYOKUSHA.SHAIN_CD\n\nLEFT  JOIN                                                  --フリーワード(ボディ)\n    (\n      SELECT TBL_C.MITSU_NO\n            ,TBL_C.SHOHIN_MEI\n            ,TBL_C.TEKIYO\n            ,TBL_C.SHOHIN_MEI\n            +TBL_C.TEKIYO\n            AS  FREEWD\n        FROM\n           (\n             SELECT TBL_A.MITSU_NO\n                   ,(\n                     SELECT \q|\q + TBL_B.SHOHIN_MEI  AS [data()]\n                       FROM T_MITSU_B TBL_B\n                      WHERE TBL_B.MITSU_NO          =  TBL_A.MITSU_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS SHOHIN_MEI\n                   ,(\n                     SELECT \q|\q + TBL_B.TEKIYO      AS [data()]\n                       FROM T_MITSU_B TBL_B\n                      WHERE TBL_B.MITSU_NO          =  TBL_A.MITSU_NO\n                      FOR XML PATH (\q\q)\n                    )                               AS TEKIYO\n               FROM T_MITSU_B TBL_A\n              GROUP BY\n                    TBL_A.MITSU_NO\n           ) TBL_C\n    ) TBL_F\n   ON TBL_F.MITSU_NO = T_MITSU_H.MITSU_NO\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="見積関連",100,100
Page=見積関連
Left=100
Top=100

[View]
PName=V_URAGAKI_LABEL
LName=裏書ラベルビュー
Tag=ビュー
Comment=UNJC0210【裏書ラベル】
ViewOption=
Query=--*DataTitle V_URAGAKI_LABEL(裏書ラベルビュー)\nSELECT TBL_H.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_H.BUSHO_CD                  AS  BUSHO_CD                  -- 分析担当部署CD\n      ,TBL_H.BUSHO_MEI                 AS  BUSHO_MEI                 -- 分析担当部署名\n      ,TBL_H.NYURYOKU_DATETIME         AS  NYURYOKU_DATETIME         -- 入力日時\n      ,TBL_H.ATENA_CD                  AS  ATENA_CD                  -- 報告書宛名CD\n      ,TBL_H.ATENA                     AS  ATENA                     -- 報告書宛名\n      ,TBL_H.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,TBL_H.SEIKYU_MEI                AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_H.BUNSEKI_HOHO_CD           AS  BUNSEKI_HOHO_CD           -- 分析方法CD\n      ,TBL_H.BUNSEKI_HOHO              AS  BUNSEKI_HOHO              -- 分析方法名\n      ,TBL_H.IRAI_DATE                 AS  IRAI_DATE                 -- 依頼日／受付日\n      ,TBL_H.KANRYO_DATE               AS  KANRYO_DATE               -- 分析担当者完了日\n      ,TBL_H.KANSEI_DATE               AS  KANSEI_DATE               -- 報告書完成日／速報日\n      ,TBL_H.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_H.KENMEI                    AS  KENMEI                    -- 件名\n      ,TBL_H.MOKUTEKI                  AS  MOKUTEKI                  -- 目的\n      ,TBL_H.KISAI_JIKO                AS  KISAI_JIKO                -- 報告書記載事項\n      ,TBL_H.CHUKAN_DATE_1             AS  CHUKAN_DATE_1             -- 中間報告日１\n      ,TBL_H.CHUKAN_NAIYO_1            AS  CHUKAN_NAIYO_1            -- 中間報告内容１\n      ,TBL_H.CHUKAN_DATE_2             AS  CHUKAN_DATE_2             -- 中間報告日２\n      ,TBL_H.CHUKAN_NAIYO_2            AS  CHUKAN_NAIYO_2            -- 中間報告内容２\n      ,TBL_H.CHUKAN_DATE_3             AS  CHUKAN_DATE_3             -- 中間報告日３\n      ,TBL_H.CHUKAN_NAIYO_3            AS  CHUKAN_NAIYO_3            -- 中間報告内容３\n      ,TBL_H.BIKO                      AS  H_BIKO                    -- ヘッダ備考\n      ,TBL_H.HOKOKU_MEI_1              AS  HOKOKU_MEI_1              -- 報告先名１\n      ,TBL_H.BUSHO_1                   AS  BUSHO_1                   -- 部署１\n      ,TBL_H.TANTO_1                   AS  TANTO_1                   -- 担当者１\n      ,TBL_H.HOKOKU_MEI_2              AS  HOKOKU_MEI_2              -- 報告先名２\n      ,TBL_H.BUSHO_2                   AS  BUSHO_2                   -- 部署２\n      ,TBL_H.TANTO_2                   AS  TANTO_2                   -- 担当者２\n      ,TBL_H.HOKOKU_MEI_3              AS  HOKOKU_MEI_3              -- 報告先名３\n      ,TBL_H.BUSHO_3                   AS  BUSHO_3                   -- 部署３\n      ,TBL_H.TANTO_3                   AS  TANTO_3                   -- 担当者３\n      ,TBL_B.JURI_EDA_NO               AS  JURI_EDA_NO               -- 枝番\n      ,TBL_B.SHIRYO_SHURUI             AS  SHIRYO_SHURUI             -- 試料種類\n      ,TBL_B.SHIRYO_MEI                AS  SHIRYO_MEI                -- 試料名\n      ,TBL_S.SEQ                       AS  SEQ                       -- SEQ\n      ,TBL_S.GYO_NO                    AS  GYO_NO                    -- 行No.\n      ,TBL_S.BUNSEKI_CD                AS  BUNSEKI_CD                -- 分析項目CD\n      ,TBL_S.BUNSEKI_MEI               AS  BUNSEKI_MEI               -- 分析項目名\n      ,TBL_S.BUNSEKI_DATA              AS  BUNSEKI_DATA              -- データ\n      ,TBL_S.BUNSEKI_NO                AS  BUNSEKI_NO                -- 分析番号\n      ,TBL_S.URAGAKI_PRINT_FLG         AS  URAGAKI_PRINT_FLG         -- 裏書印刷済フラグ\n      ,CASE TBL_S.URAGAKI_PRINT_FLG\n       WHEN \qTrue\q\n       THEN \q済\q\n       ELSE NULL\n       END                             AS  URAGAKI_PRINT_DISP        -- 裏書印刷済表示\n      ,TBL_S.STATE_KBN                 AS  STATE_KBN                 -- 状態\n      ,TBL_S.STATE_MEI                 AS  STATE_MEI                 -- 状態名\n      ,TBL_S.BIKO                      AS  K_BIKO                    -- 項目別備考\n      ,TBL_S.SOKUTEI_DATE              AS  SOKUTEI_DATE              -- 測定日\n      ,TBL_S.SOKUTEISHA_CD             AS  SOKUTEISHA_CD             -- 測定者\n      ,TBL_S.SOKUTEISHA_MEI            AS  SOKUTEISHA_MEI            -- 測定者名\n      ,TBL_H.FREEWD1\n      +TBL_B.FREEWD1\n      +TBL_S.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n      ,M_S_BUNRUI.SHOHIN_BUNRUI        AS  SHOHIN_BUNRUI             -- 商品分類\n      ,M_S_BUNRUI.SHOHIN_BUNRUI_MEI    AS  SHOHIN_BUNRUI_MEI         -- 商品分類名\n      ,M_S_BUNRUI.BUNSEKI_FLOW         AS  BUNSEKI_FLOW              -- 分析フロー\n\n  FROM V_C_BUN_JUCHU_H                 AS  TBL_H                     -- 【分析受注ヘッダ共通ビュー】\n INNER JOIN                                                          -- 【分析受注ボディ共通ビュー】\n       V_C_BUN_JUCHU_B                 AS  TBL_B\n    ON TBL_B.JURI_NO                   =   TBL_H.JURI_NO\n INNER JOIN                                                          -- 【分析受注詳細項目別サンプル共通ビュー】\n       V_C_BUN_JUCHU_SHO_K             AS  TBL_S\n    ON TBL_S.JURI_NO                   =   TBL_H.JURI_NO\n   AND TBL_S.JURI_EDA_NO               =   TBL_B.JURI_EDA_NO\n  LEFT JOIN\n     ( SELECT M_SHOHIN.SHOHIN_CD\n             ,M_SHOHIN.SHOHIN_BUNRUI\n             ,M_MEISHO_B.DATA_1        AS  SHOHIN_BUNRUI_MEI\n             ,M_SHOHIN.BIKO\n             ,M_SHOHIN.BUNSEKI_FLOW\n         FROM M_SHOHIN\n         LEFT JOIN\n              M_MEISHO_B\n           ON M_MEISHO_B.MEISHO_KBN    =   \qK025\q\n          AND M_MEISHO_B.MEISHO_CD     =   M_SHOHIN.SHOHIN_BUNRUI\n     ) M_S_BUNRUI\n    ON M_S_BUNRUI.SHOHIN_CD            =   TBL_S.BUNSEKI_CD\n WHERE TBL_H.DEL_FLG                   =   \qFalse\q\n   AND TBL_H.NYURYOKU_DATETIME         IS  NOT  NULL\n   AND TBL_H.BUNSEKI_STS               <=  10\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="分析作業関連",1250,50
Page=分析作業関連
Left=1250
Top=50

