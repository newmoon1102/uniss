Comment=
ViewOption=
Query=SELECT\n       TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_N.NYUSHUKKO_NO              AS  NYUSHUKKO_NO              -- 入出庫No.\n      ,TBL_N.ZAIKO_NO                  AS  ZAIKO_NO                  -- 在庫No.\n      ,TBL_N.NYURYOKUSHA_CD            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,TBL_N.NYURYOKUSHA_MEI           AS  NYURYOKUSHA_MEI           -- 入力者名\n      ,TBL_N.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN           -- 保管場所区分\n      ,TBL_N.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_N.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_N.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.NOKI                      AS  NOKI                      -- 納期\n      ,TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,CONVERT(VARCHAR(10),TBL_V.SHINSEI_DATETIME,111)\n                                       AS  SHINSEI_DATE              -- 申請日\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_N.NYUKO_SURYO               AS  NYUKO_SURYO               -- 入庫数量\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 金額\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.YOSAN_MEI                 AS  YOSAN_MEI                 -- 予算名\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.CHUMON_NO                 AS  CHUMON_NO                 -- 注文No.\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分CD\n      ,TBL_S.SHIYAKU_FLG               AS  SHIYAKU_FLG               -- 試薬フラグ\n      ,TBL_S.DOKU_FLG                  AS  DOKU_FLG                  -- 毒劇物フラグ\n      ,TBL_V.HATCHU_DATE               AS  HATCHU_DATE               -- 発注日\n      ,ISNULL(\n       ( SELECT SUM(NYUKO_SURYO)\n           FROM V_C_NYUSHUKKO\n          WHERE NYUSHUKKO_KBN =  1\n            AND IRAI_NO       =  TBL_V.IRAI_NO\n            AND NYUSHUKKO_NO  <> TBL_N.NYUSHUKKO_NO\n          GROUP BY IRAI_NO )\n       ,0)                             AS  NYUKO_SUMI_SURYO          -- 入庫済み数量\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n\n  LEFT JOIN                                                          -- 【入出庫テーブル】\n       V_C_NYUSHUKKO                   AS  TBL_N\n    ON TBL_N.IRAI_NO                   =   TBL_V.IRAI_NO\n\n  LEFT JOIN                                                          -- 【商品マスタ】\n       M_SHOHIN                        AS  TBL_S\n    ON TBL_S.SHOHIN_CD                 =   TBL_V.KOBAIHIN_CD\n\n WHERE TBL_V.KOBAI_STS                 IN  (3,4,5,6)\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n\n
LNameByComment=1
