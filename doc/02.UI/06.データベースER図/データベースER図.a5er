    -- 備考\n      ,TBL_V.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n  FROM V_NYUSHUKKO_LIST                AS  TBL_V                     -- 【入出庫リストビュー】\n WHERE TBL_V.NYUSHUKKO_KBN             =   2\n\n
