[View]
PName=V_SHIYAKU_SHUKKO_DATA_SANSHO
LName=試薬出庫入力参照データビュー
Tag=ビュー
Comment=UNJC0230(試薬出庫入力) 参照・修正用データ取得 入出庫No.から取得
ViewOption=
Query=SELECT TBL_A.ZAIKO_NO               AS ZAIKO_NO             --在庫No.\n      ,TBL_A.NYUSHUKKO_NO           AS NYUSHUKKO_NO         --入出庫No.\n      ,TBL_A.IRAI_NO                AS IRAI_NO              --依頼No.\n      ,TBL_A.JURI_NO                AS JURI_NO              --受理No.\n      ,TBL_A.NYURYOKUSHA_CD         AS NYURYOKUSHA_CD       --入力者CD\n      ,TBL_A.NYURYOKUSHA_MEI        AS NYURYOKUSHA_MEI      --入力者名\n      ,TBL_C.HOKAN_BASHO_KBN        AS HOKAN_BASHO_KBN      --保管場所区分\n      ,TBL_C.HOKAN_BASHO_KBN_MEI    AS HOKAN_BASHO_KBN_MEI  --保管場所\n      ,TBL_A.SHUKKO_DATE            AS SHUKKO_DATE          --出庫日\n      ,TBL_A.UKEIRE_DATE            AS UKEIRE_DATE          --受入日\n      ,TBL_A.KENSHU_DATE            AS KENSHU_DATE          --検収日\n      ,TBL_B.YOSAN_CD               AS YOSAN_CD             --予算コード\n      ,TBL_B.YOSAN_MEI              AS YOSAN_MEI            --予算名\n      ,TBL_B.KOBAIHIN_CD            AS KOBAIHIN_CD          --購買品CD\n      ,TBL_B.KOBAIHIN_MEI           AS KOBAIHIN_MEI         --購買品名\n      ,TBL_B.TANKA                  AS TANKA                --単価\n      ,TBL_A.SHUKKO_SURYO           AS SHUKKO_SURYO         --出庫数量\n      ,TBL_B.BARA_TANI              AS BARA_TANI            --バラ単位\n      ,TBL_B.IRISU                  AS IRISU                --入数\n      ,TBL_B.IRISU_TANI             AS IRISU_TANI           --入数単位\n      ,TBL_B.SHIIRE_CD              AS SHIIRE_CD            --仕入先CD\n      ,TBL_B.SHIIRE_MEI             AS SHIIRE_MEI           --仕入先名\n      ,TBL_B.MAKER_CD               AS MAKER_CD             --メーカーCD\n      ,TBL_B.MAKER_MEI              AS MAKER_MEI            --メーカー名\n      ,TBL_B.BIKO                   AS BIKO                 --備考\n      ,TBL_A.SHUKKO_JIYU            AS SHUKKO_JIYU          --出庫事由\n      ,TBL_C.ZAIKO_SURYO            AS ZAIKO_SURYO          --在庫数量\n      ,( SELECT SUM(TBL_S.ZAIKO_SURYO)\n           FROM V_C_ZAIKO AS TBL_S\n          WHERE TBL_S.KOBAIHIN_CD       = TBL_C.KOBAIHIN_CD\n            AND TBL_S.HOKAN_BASHO_KBN   = TBL_C.HOKAN_BASHO_KBN )\n                                    AS HOKAN_ZAIKO_SURYO          --保管場所内在庫数量\n  FROM V_C_NYUSHUKKO    AS TBL_A\n  LEFT JOIN\n       V_KOBAI_LIST     AS TBL_B\n    ON TBL_B.IRAI_NO = TBL_A.IRAI_NO\n  LEFT JOIN\n       M_SHOHIN\n    ON M_SHOHIN.SHOHIN_CD   = TBL_B.KOBAIHIN_CD \n  LEFT JOIN\n       V_C_ZAIKO        AS TBL_C\n    ON TBL_C.ZAIKO_NO       = TBL_A.ZAIKO_NO\n WHERE TBL_A.NYUSHUKKO_KBN  = 2\n   AND M_SHOHIN.SHIYAKU_FLG = \qTrue\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="入出庫関連",3550,100
Page=入出庫関連
Left=3550
Top=100

[View]
PName=V_SHIYAKU_SHUKKO_DATA_SHINKI
LName=試薬出庫入力新規データビュー
Tag=ビュー
Comment=UNJC0230(試薬出庫入力) 新規入力用データ取得 在庫管理No.から取得
ViewOption=
Query=SELECT TBL_A.ZAIKO_NO               AS ZAIKO_NO             --在庫No.\n      ,NULL                         AS NYUSHUKKO_NO         --入出庫No.\n      ,TBL_A.IRAI_NO                AS IRAI_NO              --依頼No.\n      ,TBL_A.JURI_NO                AS JURI_NO              --受理No.\n      ,NULL                         AS NYURYOKUSHA_CD       --入力者CD\n      ,NULL                         AS NYURYOKUSHA_MEI      --入力者名\n      ,TBL_C.HOKAN_BASHO_KBN        AS HOKAN_BASHO_KBN      --保管場所区分\n      ,TBL_C.HOKAN_BASHO_KBN_MEI    AS HOKAN_BASHO_KBN_MEI  --保管場所\n      ,CONVERT(VARCHAR(10),GETDATE(),111)\n                                    AS SHUKKO_DATE          --出庫日\n      ,TBL_A.UKEIRE_DATE            AS UKEIRE_DATE          --受入日\n      ,TBL_A.KENSHU_DATE            AS KENSHU_DATE          --検収日\n      ,TBL_B.YOSAN_CD               AS YOSAN_CD             --予算コード\n      ,TBL_B.YOSAN_MEI              AS YOSAN_MEI            --予算名\n      ,TBL_B.KOBAIHIN_CD            AS KOBAIHIN_CD          --購買品CD\n      ,TBL_B.KOBAIHIN_MEI           AS KOBAIHIN_MEI         --購買品名\n      ,TBL_B.TANKA                  AS TANKA                --単価\n      ,NULL                         AS SHUKKO_SURYO         --出庫数量\n      ,TBL_B.BARA_TANI              AS BARA_TANI            --バラ単位\n      ,TBL_B.IRISU                  AS IRISU                --入数\n      ,TBL_B.IRISU_TANI             AS IRISU_TANI           --入数単位\n      ,TBL_B.SHIIRE_CD              AS SHIIRE_CD            --仕入先CD\n      ,TBL_B.SHIIRE_MEI             AS SHIIRE_MEI           --仕入先名\n      ,TBL_B.MAKER_CD               AS MAKER_CD             --メーカーCD\n      ,TBL_B.MAKER_MEI              AS MAKER_MEI            --メーカー名\n      ,TBL_B.BIKO                   AS BIKO                 --備考\n      ,NULL                         AS SHUKKO_JIYU          --出庫事由\n      ,TBL_C.ZAIKO_SURYO            AS ZAIKO_SURYO          --在庫数量\n      ,( SELECT SUM(TBL_S.ZAIKO_SURYO)\n           FROM V_C_ZAIKO AS TBL_S\n          WHERE TBL_S.KOBAIHIN_CD       = TBL_C.KOBAIHIN_CD\n            AND TBL_S.HOKAN_BASHO_KBN   = TBL_C.HOKAN_BASHO_KBN )\n                                    AS HOKAN_ZAIKO_SURYO          --保管場所内在庫数量\n  FROM V_C_NYUSHUKKO    AS TBL_A\n  LEFT JOIN\n       V_KOBAI_LIST     AS TBL_B\n    ON TBL_B.IRAI_NO = TBL_A.IRAI_NO\n  LEFT JOIN\n       V_C_ZAIKO        AS TBL_C\n    ON TBL_C.ZAIKO_NO = TBL_A.ZAIKO_NO\n  LEFT JOIN\n       M_SHOHIN\n    ON M_SHOHIN.SHOHIN_CD   = TBL_B.KOBAIHIN_CD\n WHERE TBL_A.NYUSHUKKO_KBN  = 1\n   AND M_SHOHIN.SHIYAKU_FLG = \qTrue\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="入出庫関連",3000,100
Page=入出庫関連
Left=3000
Top=100

[View]
PName=V_UKEIRE_KENSHU_DATA_SHINKI
LName=受入検収入力新規ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT\n       TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,NULL                            AS  NYUSHUKKO_NO              -- 入出庫No.\n      ,NULL                            AS  ZAIKO_NO                  -- 在庫No.\n      ,NULL                            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,NULL                            AS  NYURYOKUSHA_MEI           -- 入力者名\n      ,TBL_K.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN           -- 保管場所区分\n      ,TBL_K.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,CONVERT(VARCHAR(10),GETDATE(),111)\n                                       AS  UKEIRE_DATE               -- 受入日\n      ,CASE ISNULL(TBL_V.KOBAI_KBN,\q\q)\n       WHEN \q4\q THEN NULL\n       ELSE  CONVERT(VARCHAR(10),GETDATE(),111)\n       END                             AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.NOKI                      AS  NOKI                      -- 納期\n      ,TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,CONVERT(VARCHAR(10),TBL_V.SHINSEI_DATETIME,111)\n                                       AS  SHINSEI_DATE              -- 申請日\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.SURYO                     AS  SURYO                     -- 数量\n      ,NULL                            AS  NYUKO_SURYO               -- 入庫数量\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 金額\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.YOSAN_MEI                 AS  YOSAN_MEI                 -- 予算名\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.CHUMON_NO                 AS  CHUMON_NO                 -- 注文No.\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分CD\n      ,TBL_S.SHIYAKU_FLG               AS  SHIYAKU_FLG               -- 試薬フラグ\n      ,TBL_S.DOKU_FLG                  AS  DOKU_FLG                  -- 毒劇物フラグ\n      ,TBL_V.HATCHU_DATE               AS  HATCHU_DATE               -- 発注日\n      ,ISNULL(\n       ( SELECT SUM(NYUKO_SURYO)\n           FROM V_C_NYUSHUKKO\n          WHERE NYUSHUKKO_KBN = 1\n            AND IRAI_NO       = TBL_V.IRAI_NO\n          GROUP BY IRAI_NO )\n       ,0)                             AS  NYUKO_SUMI_SURYO          -- 入庫済み数量\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n\n  LEFT JOIN                                                          -- 【購買テーブル】\n       V_C_KOBAI_B                     AS  TBL_K\n    ON TBL_K.KOBAI_NO                  =   TBL_V.KOBAI_NO\n   AND TBL_K.KOBAI_SEQ                 =   TBL_V.KOBAI_SEQ\n\n  LEFT JOIN                                                          -- 【商品マスタ】\n       M_SHOHIN                        AS  TBL_S\n    ON TBL_S.SHOHIN_CD                 =   TBL_V.KOBAIHIN_CD\n\n WHERE TBL_V.KOBAI_STS                 IN  (3,4,5,6)\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="入出庫関連",1850,1000
Page=入出庫関連
Left=1850
Top=1000

