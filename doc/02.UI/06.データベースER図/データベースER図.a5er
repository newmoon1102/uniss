[View]
PName=V_MITSU_EXCEL
LName=見積Excel出力ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT \n       TBL_H.MITSU_NO                                       -- 見積No.\n      ,CASE TBL_H.MODE_KBN\n       WHEN 1 THEN \q通常\q\n       ELSE \q単価\q\n       END                          AS MODE                 -- 種別\n      ,TBL_H.MITSU_DATE                                     -- 見積日\n      ,TBL_H.MITSU_STS              AS MITSU_STS_CD         -- 見積ステータスCD\n      ,J_STS.DATA_1                 AS MITSU_STS_MEI        -- 見積ステータス名\n      ,TBL_H.NYURYOKUSHA_CD         AS NYURYOKUSHA_CD       -- 入力者CD\n      ,J_NYURYOKUSHA.SHIMEI         AS NYURYOKUSHA_MEI      -- 入力者名\n      ,TBL_H.TORIHIKI_BUNRUI        AS TORIHIKI_BUNRUI_CD   -- 取引分類CD\n      ,J_TORIHIKI.DATA_1            AS TORIHIKI_BUNRUI_MEI  -- 取引分類名\n      ,TBL_H.REF_MITSU_NO                                   -- 参照元見積No.\n      ,CASE TBL_H.KOYOMI_KBN\n       WHEN 1 THEN \q西暦\q\n       ELSE \q和暦\q\n       END                          AS KOYOMI               -- 西暦和暦区分\n      ,TBL_H.SEIKYU_CD                                      -- 請求先CD\n      ,J_SEIKYU.AITE_MEI            AS SEIKYU_MEI           -- 請求先名\n      ,TBL_H.SEMPO_TANTO_MEI                                -- 先方担当者名\n      ,TBL_H.KEISHO                                         -- 敬称\n      ,TBL_H.KENMEI                                         -- 件名\n      ,TBL_H.EIGYO_TANTO_CD                                 -- 営業担当者CD\n      ,J_EIGYO.SHIMEI               AS EIGYO_TANTO_MEI      -- 営業担当者名 \n      ,TBL_H.MEMO                                           -- メモ\n      ,TBL_H.UKE_KIJITSU                                    -- 受渡期日\n      ,TBL_H.UKE_BASHO                                      -- 受渡場所\n      ,TBL_H.TORI_JOKEN                                     -- 取引条件\n      ,TBL_H.YUKO_KIGEN                                     -- 有効期限\n      ,TBL_H.TANTO_MARK_FLG                                 -- 担当者印フラグ\n      ,TBL_H.DAIHYO_MEI_FLG                                 -- 代表者名フラグ\n      ,TBL_H.KAISHA_MARK_FLG                                -- 会社角印フラグ\n      ,TBL_H.SHONIN_RAN_FLG                                 -- 承認印欄フラグ\n      ,TBL_H.ZEIKOMI_PRINT_FLG                              -- 税込印刷フラグ\n      ,TBL_H.TAX_RATE                                       -- 税率\n      ,TBL_H.NUKI_TOTAL                                     -- 税抜合計\n      ,TBL_H.TAX                                            -- 消費税\n      ,TBL_H.KOMI_TOTAL                                     -- 税込合計\n      ,TBL_H.GENKA_TOTAL                                    -- 原価合計\n      ,TBL_H.ARARIEKI               AS ARARIEKI_H           -- 粗利益\n      ,TBL_H.RIEKI_RATE             AS ARARIRITSU_H         -- 粗利率\n      ,TBL_B.GYO_NO                                         -- 行No.\n      ,CASE TBL_B.ROW_KBN \n       WHEN 1 THEN \q商品\q\n       WHEN 2 THEN \qコメント\q\n       WHEN 3 THEN \q値引\q\n       ELSE \q小計\q\n       END                          AS ROW_KBN              -- 行区分\n      ,TBL_B.SHOHIN_CD                                      -- 商品CD\n      ,TBL_B.SHOHIN_MEI                                     -- 商品名\n      ,TBL_B.SURYO                                          -- 数量\n      ,TBL_B.TANI                                           -- 単位\n      ,TBL_B.TANKA                                          -- 単価\n      ,TBL_B.KINGAKU                                        -- 金額\n      ,TBL_B.TEKIYO                                         -- 摘要\n      ,TBL_B.GENTANKA                                       -- 原単価\n      ,TBL_B.GENKA                                          -- 原価\n      ,TBL_B.ARARIRITSU             AS ARARIRITSU_B         -- 粗利率\n      ,TBL_B.ARARIEKI               AS ARARIEKI_B           -- 粗利益\n      ,TBL_B.TEIKA                                          -- 定価\n      ,TBL_B.SHIIRE_CD                                      -- 仕入先CD\n      ,J_SHIIRE.AITE_MEI            AS SHIIRE_MEI           -- 仕入先名\n      ,TBL_B.NEBIKI_RITSU                                   -- 値引率\n      ,TBL_B.DEL_FLG                                        -- 削除フラグ\n\n  FROM T_MITSU_H                    AS TBL_H\n  LEFT JOIN\n       T_MITSU_B                    AS TBL_B\n    ON TBL_H.MITSU_NO               =  TBL_B.MITSU_NO\n  -- ステータス\n  LEFT JOIN\n       M_MEISHO_B                   AS J_STS\n    ON J_STS.MEISHO_KBN             =  \qS002\q\n   AND J_STS.MEISHO_CD              =  TBL_H.MITSU_STS\n  -- 入力者\n  LEFT JOIN\n       M_SHAIN                      AS J_NYURYOKUSHA\n    ON J_NYURYOKUSHA.SHAIN_CD       =  TBL_H.NYURYOKUSHA_CD\n  -- 取引分類\n  LEFT JOIN\n       M_MEISHO_B                   AS J_TORIHIKI\n    ON J_TORIHIKI.MEISHO_KBN        =  \qK010\q\n   AND J_TORIHIKI.MEISHO_CD         =  TBL_H.TORIHIKI_BUNRUI\n  -- 請求先\n  LEFT JOIN\n       M_AITESAKI                   AS J_SEIKYU\n    ON J_SEIKYU.AITE_CD             =  TBL_H.SEIKYU_CD\n  -- 営業担当者\n  LEFT JOIN\n       M_SHAIN                      AS J_EIGYO\n    ON J_EIGYO.SHAIN_CD             =  TBL_H.EIGYO_TANTO_CD\n  -- 仕入先\n  LEFT JOIN\n       M_AITESAKI                   AS J_SHIIRE\n    ON J_SHIIRE.AITE_CD             =  TBL_B.SHIIRE_CD\n\n
LNameByComment=1
