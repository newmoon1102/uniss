ViewOption=
Query=--*DataTitle V_C_URIAGE_H_R(売上ヘッダ履歴共通ビュー)\nSELECT TBL_A.URIAGE_NO                 AS  URIAGE_NO                 -- 売上No.\n      ,TBL_A.RIREKI_NO                 AS  RIREKI_NO                 -- 履歴No.\n      ,TBL_A.SHORI_KBN                 AS  SHORI_KBN                 -- 処理区分\n      ,TBL_A.DEMPYO_KBN                AS  DEMPYO_KBN                -- 伝票区分\n      ,M_DEMPYO_KBN.DATA_1             AS  DEMPYO_KBN_MEI            -- 伝票区分名\n      ,TBL_A.RENDO_KBN                 AS  RENDO_KBN                 -- 商魂連動区分\n      ,M_RENDO_KBN.DATA_1              AS  RENDO_KBN_MEI             -- 商魂連動区分名\n      ,TBL_A.DENKU                     AS  DENKU_KBN                 -- 売上伝区\n      ,M_DENKU_KBN.DATA_1              AS  DENKU_KBN_MEI             -- 売上伝区名\n      ,TBL_A.KANJO_KAMOKU              AS  KANJO_KAMOKU              -- 勘定科目\n      ,M_KANJO_KBN.DATA_1              AS  KANJO_KAMOKU_MEI          -- 勘定科目名\n      ,TBL_A.URIAGE_DATE               AS  URIAGE_DATE               -- 売上日\n      ,TBL_A.BUSHO_CD                  AS  BUSHO_CD                  -- 担当部署CD\n      ,M_BUSHO_CD.DATA_1               AS  BUSHO_MEI                 -- 担当部署名\n      ,TBL_A.URIAGE_NYURYOKUSHA_CD     AS  URIAGE_NYURYOKUSHA_CD     -- 売上入力者CD\n      ,M_URI_NYU.SHIMEI                AS  URI_NYU_MEI               -- 売上入力者名\n      ,TBL_A.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,M_EIGYO_TANTO.SHIMEI            AS  EIGYO_TANTO_MEI           -- 営業担当者名\n      ,TBL_A.NYURYOKU_DATE_TIME        AS  NYURYOKU_DATE_TIME        -- 入力日時\n      ,TBL_A.JUCHU_KBN                 AS  JUCHU_KBN                 -- 受注区分\n      ,M_JUCHU_KBN.DATA_1              AS  JUCHU_KBN_MEI             -- 受注区分名\n      ,TBL_A.JURI_NO                   AS  JURI_NO                   -- 対象受理No.\n      ,TBL_A.MITSU_NO                  AS  MITSU_NO                  -- 対象見積No.\n      ,TBL_A.SAKI_URIAGE_NO            AS  SAKI_URIAGE_NO            -- 結合先売上No.\n      ,TBL_A.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_A.URIAGE_STS                AS  URIAGE_STS                -- 売上ステータス\n      ,M_URIAGE_STS.DATA_1             AS  URIAGE_STS_MEI            -- 売上ステータス名\n      ,TBL_A.DOJI_FLG                  AS  DOJI_FLG                  -- 同時発行フラグ\n      ,TBL_A.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,M_SEIKYU.AITE_MEI               AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_A.KAISHU_DATE               AS  KAISHU_DATE               -- 回収予定日\n      ,TBL_A.BUNSEKI_HOHO_CD           AS  BUNSEKI_HOHO_CD           -- 分析方法CD\n      ,M_BUN_HOHO.DATA_1               AS  BUNSEKI_HOHO              -- 分析方法名\n      ,TBL_A.TEKIYO                    AS  TEKIYO                    -- 摘要\n      ,TBL_A.DEMPYO_SOFU_MEI           AS  DEMPYO_SOFU_MEI           -- 伝票送付先名\n      ,TBL_A.KEISHO                    AS  KEISHO                    -- 敬称\n      ,TBL_A.YUBIN_NO                  AS  YUBIN_NO                  -- 送付先郵便番号\n      ,TBL_A.ADDRESS_1                 AS  ADDRESS_1                 -- 送付先住所１\n      ,TBL_A.ADDRESS_2                 AS  ADDRESS_2                 -- 送付先住所２\n      ,TBL_A.TEL                       AS  TEL                       -- 送付先TEL\n      ,TBL_A.FAX                       AS  FAX                       -- 送付先FAX\n      ,TBL_A.SOFU_TANTO                AS  SOFU_TANTO                -- 送付先担当者\n      ,TBL_A.TEISEI_BIKO               AS  TEISEI_BIKO               -- 訂正備考\n      ,TBL_A.JUSHO_NO_PRINT_FLG        AS  JUSHO_NO_PRINT_FLG        -- 住所印字不要フラグ\n      ,TBL_A.TAX_RATE                  AS  TAX_RATE                  -- 税率\n      ,TBL_A.NUKI_TOTAL                AS  NUKI_TOTAL                -- 税抜合計\n      ,TBL_A.TAX                       AS  TAX                       -- 消費税\n      ,TBL_A.KOMI_TOTAL                AS  KOMI_TOTAL                -- 税込合計\n      ,TBL_A.GENKA_TOTAL               AS  GENKA_TOTAL               -- 原価合計\n      ,TBL_A.ARARIEKI                  AS  ARARIEKI                  -- 粗利益\n      ,TBL_A.ARARIRITSU                AS  ARARIRITSU                -- 粗利率\n      ,TBL_A.SHOKON_NO                 AS  SHOKON_NO                 -- 商魂伝票No.\n      ,TBL_A.SHOKON_LIST_DATE_TIME     AS  SHOKON_LIST_DATE_TIME     -- 連動日時\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_URIAGE_H_R                    AS  TBL_A                     -- 【売上ヘッダ履歴テーブル】\n  LEFT JOIN                                                          -- 【伝票区分用】\n       M_MEISHO_B                      AS  M_DEMPYO_KBN\n    ON M_DEMPYO_KBN.MEISHO_KBN         =   \qK023\q\n   AND M_DEMPYO_KBN.MEISHO_CD          =   TBL_A.DEMPYO_KBN\n  LEFT JOIN                                                          -- 【商魂連動区分用】\n       M_MEISHO_B                      AS  M_RENDO_KBN\n    ON M_RENDO_KBN.MEISHO_KBN          =   \qK029\q\n   AND M_RENDO_KBN.MEISHO_CD           =   TBL_A.RENDO_KBN\n  LEFT JOIN                                                          -- 【売上伝区用】\n       M_MEISHO_B                      AS  M_DENKU_KBN\n    ON M_DENKU_KBN.MEISHO_KBN          =   \qK032\q\n   AND M_DENKU_KBN.MEISHO_CD           =   TBL_A.DENKU\n  LEFT JOIN                                                          -- 【勘定科目用】\n       M_MEISHO_B                      AS  M_KANJO_KBN\n    ON M_KANJO_KBN.MEISHO_KBN          =   \qK033\q\n   AND M_KANJO_KBN.MEISHO_CD           =   TBL_A.KANJO_KAMOKU\n  LEFT JOIN                                                          -- 【担当部署用】\n       M_MEISHO_B                      AS  M_BUSHO_CD\n    ON M_BUSHO_CD.MEISHO_KBN           =   \qM001\q\n   AND M_BUSHO_CD.MEISHO_CD            =   TBL_A.BUSHO_CD\n  LEFT JOIN                                                          -- 【売上入力者用】\n       M_SHAIN                         AS  M_URI_NYU\n    ON M_URI_NYU.SHAIN_CD              =   TBL_A.URIAGE_NYURYOKUSHA_CD\n  LEFT JOIN                                                          -- 【営業担当者用】\n       M_SHAIN                         AS  M_EIGYO_TANTO\n    ON M_EIGYO_TANTO.SHAIN_CD          =   TBL_A.EIGYO_TANTO_CD\n  LEFT JOIN                                                          -- 【受注区分用】\n       M_MEISHO_B                      AS  M_JUCHU_KBN\n    ON M_JUCHU_KBN.MEISHO_KBN          =   \qK030\q\n   AND M_JUCHU_KBN.MEISHO_CD           =   TBL_A.JUCHU_KBN\n  LEFT JOIN                                                          -- 【売上ステータス用】\n       M_MEISHO_B                      AS  M_URIAGE_STS\n    ON M_URIAGE_STS.MEISHO_KBN         =   \qS007\q\n   AND M_URIAGE_STS.MEISHO_CD          =   TBL_A.URIAGE_STS\n  LEFT JOIN                                                          -- 【請求先用】\n       M_AITESAKI                      AS  M_SEIKYU\n    ON M_SEIKYU.AITE_CD                =   TBL_A.SEIKYU_CD\n  LEFT JOIN                                                          -- 【分析方法用】\n       M_MEISHO_B                      AS  M_BUN_HOHO\n    ON M_BUN_HOHO.MEISHO_KBN           =   \qK017\q\n   AND M_BUN_HOHO.MEISHO_CD            =   TBL_A.BUNSEKI_HOHO_CD\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="売上関連",150,1500
Page=売上関連
Left=150
Top=1500

