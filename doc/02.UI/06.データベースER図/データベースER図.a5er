Query=--*DataTitle V_BUN_JUCHU_REPORT_H(分析受注依頼書ヘッダビュー)\nSELECT T_BUN_JUCHU_H.JURI_NO                  AS JURI_NO              -- 受理No.\n      ,T_BUN_JUCHU_H.BUNSEKI_STS              AS BUNSEKI_STS          -- 分析ステータス\n      ,J_STS.DATA_1                           AS BUNSEKI_STS_MEI      -- 分析ステータス名\n      ,J_JUCHU_KBN.DATA_1                     AS JUCHU_KBN_MEI        -- 分析区分名\n      ,T_BUN_JUCHU_H.NYURYOKU_DATETIME        AS NYURYOKU_DATETIME    -- 入力日時\n      ,T_BUN_JUCHU_H.BUSHO_CD                 AS BUSHO_CD             -- 分析担当部署CD\n      ,J_BUSHO.DATA_1                         AS BUSHO_MEI            -- 分析担当部署名\n      ,T_BUN_JUCHU_H.NYURYOKUSHA_CD           AS NYURYOKUSHA_CD       -- 入力者CD\n      ,J_NYURYOKUSHA.SHIMEI                   AS NYURYOKUSHA_MEI      -- 入力社名\n      ,T_BUN_JUCHU_H.REF_JURI_NO              AS REF_JURI_NO          -- 参照元受理No.\n      ,T_BUN_JUCHU_H.MITSU_NO                 AS MITSU_NO             -- 見積No.\n      ,T_BUN_JUCHU_H.ATENA_CD                 AS ATENA_CD             -- 報告書宛名CD\n      ,T_BUN_JUCHU_H.ATENA                    AS ATENA                -- 報告書宛名\n      ,T_BUN_JUCHU_H.SEIKYU_CD                AS SEIKYU_CD            -- 請求先CD\n      ,J_SEIKYU.AITE_MEI                      AS SEIKYU_MEI           -- 請求先名\n      ,M_BUN_HOHO.DATA_1                      AS BUNSEKI_HOHO         -- 分析方法名\n      ,J_KEISHIKI_KBN.DATA_1                  AS KEISHIKI_KBN_MEI     -- 報告書形式名\n      ,T_BUN_JUCHU_H.NOHIN_HOHO               AS NOHIN_HOHO           -- 納品方法実績\n      ,T_BUN_JUCHU_H.KOSU                     AS KOSU                 -- 工数\n      ,T_BUN_JUCHU_H.EIGYO_TANTO_CD           AS EIGYO_TANTO_CD       -- 営業担当者CD\n      ,J_EIGYO_TANTO.SHIMEI                   AS EIGYO_TANTO_MEI      -- 営業担当者名\n      ,T_BUN_JUCHU_H.IRAI_DATE                AS IRAI_DATE            -- 依頼日／受付日\n      ,T_BUN_JUCHU_H.KANRYO_DATE              AS KANRYO_DATE          -- 分析担当者完了日\n      ,T_BUN_JUCHU_H.KANSEI_DATE              AS KANSEI_DATE          -- 報告書完成日／速報日\n      ,T_BUN_JUCHU_H.SOKUHO_KBN               AS SOKUHO_KBN           -- 速報区分\n      ,J_SOKUHO.DATA_1                        AS SOKUHO_KBN_MEI       -- 速報区分名\n      ,T_BUN_JUCHU_H.HAKKO_DATE               AS HAKKO_DATE           -- 報告書指定発行日\n      ,T_BUN_JUCHU_H.NOHIN_DATE               AS NOHIN_DATE           -- 納品日\n      ,T_BUN_JUCHU_H.KENMEI                   AS KENMEI               -- 件名\n      ,T_BUN_JUCHU_H.MOKUTEKI                 AS MOKUTEKI             -- 目的\n      ,T_BUN_JUCHU_H.KISAI_JIKO               AS KISAI_JIKO           -- 報告書記載事項\n      ,T_BUN_JUCHU_H.EDA_SHIRYO_FLG           AS EDA_SHIRYO_FLG       -- 枝番試料登録フラグ\n      ,T_BUN_JUCHU_H.SAISHU_KAISHA_MEI        AS SAISHU_KAISHA_MEI    -- 採取会社名\n      ,T_BUN_JUCHU_H.SAISHU_SHA               AS SAISHU_SHA           -- 採取者\n      ,T_BUN_JUCHU_H.SAISHU_HOHO              AS SAISHU_HOHO          -- 採取方法\n      ,T_BUN_JUCHU_H.SAISHU_BASHO             AS SAISHU_BASHO         -- 採取場所\n      ,T_BUN_JUCHU_H.TENKO                    AS TENKO                -- 天候\n      ,T_BUN_JUCHU_H.KION                     AS KION                 -- 気温\n      ,T_BUN_JUCHU_H.SUION                    AS SUION                -- 水温\n      ,T_BUN_JUCHU_H.SAISHU_DATE              AS SAISHU_DATE          -- 採取日\n      ,T_BUN_JUCHU_H.SAISHU_TIME              AS SAISHU_TIME          -- 採取時間\n      ,T_BUN_JUCHU_H.SHOKEN_FLG               AS SHOKEN_FLG           -- 報告書所見フラグ\n      ,J_GAISO.DATA_1                         AS GAISO_KBN_MEI        -- 報告書外装\n      ,T_BUN_JUCHU_H.YOKI                     AS YOKI                 -- 容器種類\n      ,T_BUN_JUCHU_H.HENKYAKU                 AS HENKYAKU             -- 試料・容器返却\n      ,J_HOKOKU_HOHO.DATA_1                   AS HOKOKU_HOHO_MEI      -- 報告方法\n      ,T_BUN_JUCHU_H.BUSU                     AS BUSU                 -- 発行部数\n      ,T_BUN_JUCHU_H.CHUKAN_DATE_1            AS CHUKAN_DATE_1        -- 中間報告日１\n      ,T_BUN_JUCHU_H.CHUKAN_NAIYO_1           AS CHUKAN_NAIYO_1       -- 中間報告内容１\n      ,T_BUN_JUCHU_H.CHUKAN_DATE_2            AS CHUKAN_DATE_2        -- 中間報告日２\n      ,T_BUN_JUCHU_H.CHUKAN_NAIYO_2           AS CHUKAN_NAIYO_2       -- 中間報告内容２\n      ,T_BUN_JUCHU_H.CHUKAN_DATE_3            AS CHUKAN_DATE_3        -- 中間報告日３\n      ,T_BUN_JUCHU_H.CHUKAN_NAIYO_3           AS CHUKAN_NAIYO_3       -- 中間報告内容３\n      ,T_BUN_JUCHU_H.BIKO                     AS BIKO                 -- 備考\n      ,T_BUN_JUCHU_H.FUYO_FLG                 AS FUYO_FLG             -- 営業確認不要フラグ\n      ,T_BUN_JUCHU_H.NEW_FLG                  AS NEW_FLG              -- 新規フラグ\n      ,T_BUN_JUCHU_H.HENKO_FLG                AS HENKO_FLG            -- 記載内容変更予定フラグ\n      ,T_BUN_JUCHU_H.HOKOKU_CD_1              AS HOKOKU_CD_1          -- 報告先CD１\n      ,T_BUN_JUCHU_H.HOKOKU_MEI_1             AS HOKOKU_MEI_1         -- 報告先名１\n      ,T_BUN_JUCHU_H.BUSHO_1                  AS BUSHO_1              -- 部署１\n      ,T_BUN_JUCHU_H.TANTO_1                  AS TANTO_1              -- 担当者１\n      ,T_BUN_JUCHU_H.TEL_1                    AS TEL_1                -- TEL１\n      ,T_BUN_JUCHU_H.FAX_1                    AS FAX_1                -- FAX１\n      ,T_BUN_JUCHU_H.MAIL_1                   AS MAIL_1               -- E-MAIL１\n      ,T_BUN_JUCHU_H.YUBIN_NO_1               AS YUBIN_NO_1           -- 郵便番号１\n      ,T_BUN_JUCHU_H.ADDRESS_1                AS ADDRESS_1            -- 住所１\n      ,T_BUN_JUCHU_H.CHUKAN_SOFU_FLG_1        AS CHUKAN_SOFU_FLG_1    -- 中間報告送付フラグ１\n      ,T_BUN_JUCHU_H.SOKUHO_SOFU_FLG_1        AS SOKUHO_SOFU_FLG_1    -- 速報送付フラグ１\n      ,T_BUN_JUCHU_H.HOKOKUSHO_SOFU_FLG_1     AS HOKOKUSHO_SOFU_FLG_1 -- 報告書送付フラグ１\n      ,T_BUN_JUCHU_H.HOKOKU_CD_2              AS HOKOKU_CD_2          -- 報告先CD２\n      ,T_BUN_JUCHU_H.HOKOKU_MEI_2             AS HOKOKU_MEI_2         -- 報告先名２\n      ,T_BUN_JUCHU_H.BUSHO_2                  AS BUSHO_2              -- 部署２\n      ,T_BUN_JUCHU_H.TANTO_2                  AS TANTO_2              -- 担当者２\n      ,T_BUN_JUCHU_H.TEL_2                    AS TEL_2                -- TEL２\n      ,T_BUN_JUCHU_H.FAX_2                    AS FAX_2                -- FAX２\n      ,T_BUN_JUCHU_H.MAIL_2                   AS MAIL_2               -- E-MAIL２\n      ,T_BUN_JUCHU_H.YUBIN_NO_2               AS YUBIN_NO_2           -- 郵便番号２\n      ,T_BUN_JUCHU_H.ADDRESS_2                AS ADDRESS_2            -- 住所２\n      ,T_BUN_JUCHU_H.CHUKAN_SOFU_FLG_2        AS CHUKAN_SOFU_FLG_2    -- 中間報告送付フラグ２\n      ,T_BUN_JUCHU_H.SOKUHO_SOFU_FLG_2        AS SOKUHO_SOFU_FLG_2    -- 速報送付フラグ２\n      ,T_BUN_JUCHU_H.HOKOKUSHO_SOFU_FLG_2     AS HOKOKUSHO_SOFU_FLG_2 -- 報告書送付フラグ２\n      ,T_BUN_JUCHU_H.HOKOKU_CD_3              AS HOKOKU_CD_3          -- 報告先CD３\n      ,T_BUN_JUCHU_H.HOKOKU_MEI_3             AS HOKOKU_MEI_3         -- 報告先名３\n      ,T_BUN_JUCHU_H.BUSHO_3                  AS BUSHO_3              -- 部署３\n      ,T_BUN_JUCHU_H.TANTO_3                  AS TANTO_3              -- 担当者３\n      ,T_BUN_JUCHU_H.TEL_3                    AS TEL_3                -- TEL３\n      ,T_BUN_JUCHU_H.FAX_3                    AS FAX_3                -- FAX３\n      ,T_BUN_JUCHU_H.MAIL_3                   AS MAIL_3               -- E-MAIL３\n      ,T_BUN_JUCHU_H.YUBIN_NO_3               AS YUBIN_NO_3           -- 郵便番号３\n      ,T_BUN_JUCHU_H.ADDRESS_3                AS ADDRESS_3            -- 住所３\n      ,T_BUN_JUCHU_H.CHUKAN_SOFU_FLG_3        AS CHUKAN_SOFU_FLG_3    -- 中間報告送付フラグ３\n      ,T_BUN_JUCHU_H.SOKUHO_SOFU_FLG_3        AS SOKUHO_SOFU_FLG_3    -- 速報送付フラグ３\n      ,T_BUN_JUCHU_H.HOKOKUSHO_SOFU_FLG_3     AS HOKOKUSHO_SOFU_FLG_3 -- 報告書送付フラグ３\n      ,J_DEMPYO_KBN.DATA_1                    AS DEMPYO_HAKKO_KBN     -- 伝票発行区分\n  FROM T_BUN_JUCHU_H\n  --ステータス\n  LEFT JOIN\n       M_MEISHO_B AS J_STS\n    ON J_STS.MEISHO_KBN = \qS002\q\n   AND J_STS.MEISHO_CD  = T_BUN_JUCHU_H.BUNSEKI_STS\n  --分析区分\n  LEFT JOIN\n       M_MEISHO_B AS J_JUCHU_KBN\n    ON J_JUCHU_KBN.MEISHO_KBN = \qK012\q\n   AND J_JUCHU_KBN.MEISHO_CD  = T_BUN_JUCHU_H.JUCHU_KBN\n   --部署名\n  LEFT JOIN\n       M_MEISHO_B AS J_BUSHO\n    ON J_BUSHO.MEISHO_KBN = \qM001\q\n   AND J_BUSHO.MEISHO_CD  = T_BUN_JUCHU_H.BUSHO_CD\n  --入力者名\n  LEFT JOIN\n       M_SHAIN AS J_NYURYOKUSHA\n    ON J_NYURYOKUSHA.SHAIN_CD = T_BUN_JUCHU_H.NYURYOKUSHA_CD\n  --請求先名\n  LEFT JOIN\n       M_AITESAKI AS J_SEIKYU\n    ON J_SEIKYU.AITE_CD = T_BUN_JUCHU_H.SEIKYU_CD\n  --分析方法\n  LEFT JOIN\n       M_MEISHO_B                      AS  M_BUN_HOHO\n    ON M_BUN_HOHO.MEISHO_KBN           =   \qK017\q\n   AND M_BUN_HOHO.MEISHO_CD            =   T_BUN_JUCHU_H.BUNSEKI_HOHO_CD\n  --報告書形式\n  LEFT JOIN\n       M_MEISHO_B AS J_KEISHIKI_KBN\n    ON J_KEISHIKI_KBN.MEISHO_KBN = \qK013\q\n   AND J_KEISHIKI_KBN.MEISHO_CD  = T_BUN_JUCHU_H.KEISHIKI_KBN\n  --営業担当者名\n  LEFT JOIN\n       M_SHAIN AS J_EIGYO_TANTO\n    ON J_EIGYO_TANTO.SHAIN_CD = T_BUN_JUCHU_H.EIGYO_TANTO_CD\n  --速報区分\n  LEFT JOIN\n       M_MEISHO_B AS J_SOKUHO\n    ON J_SOKUHO.MEISHO_KBN = \qK027\q\n   AND J_SOKUHO.MEISHO_CD  = T_BUN_JUCHU_H.SOKUHO_KBN\n  --報告書外装区分\n  LEFT JOIN\n       M_MEISHO_B AS J_GAISO\n    ON J_GAISO.MEISHO_KBN = \qK014\q\n   AND J_GAISO.MEISHO_CD  = T_BUN_JUCHU_H.GAISO_KBN\n  --報告方法区分\n  LEFT JOIN\n       M_MEISHO_B AS J_HOKOKU_HOHO\n    ON J_HOKOKU_HOHO.MEISHO_KBN = \qK016\q\n   AND J_HOKOKU_HOHO.MEISHO_CD  = T_BUN_JUCHU_H.HOKOKU_HOHO_KBN\n  --伝票発行区分\n  LEFT JOIN\n       M_MEISHO_B AS J_DEMPYO_KBN\n    ON J_DEMPYO_KBN.MEISHO_KBN = \qK026\q\n   AND J_DEMPYO_KBN.MEISHO_CD  = T_BUN_JUCHU_H.DEMPYO_HAKKO_KBN\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
