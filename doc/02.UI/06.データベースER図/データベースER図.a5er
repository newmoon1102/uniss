  -- 部署１\n      ,TBL_A.TANTO_1                   AS  TANTO_1                   -- 担当者１\n      ,TBL_A.TEL_1                     AS  TEL_1                     -- TEL１\n      ,TBL_A.FAX_1                     AS  FAX_1                     -- FAX１\n      ,TBL_A.MAIL_1                    AS  MAIL_1                    -- E-MAIL１\n      ,TBL_A.YUBIN_NO_1                AS  YUBIN_NO_1                -- 郵便番号１\n      ,TBL_A.ADDRESS_1                 AS  ADDRESS_1                 -- 住所１\n      ,TBL_A.CHUKAN_SOFU_FLG_1         AS  CHUKAN_SOFU_FLG_1         -- 中間報告送付フラグ１\n      ,TBL_A.SOKUHO_SOFU_FLG_1         AS  SOKUHO_SOFU_FLG_1         -- 速報送付フラグ１\n      ,TBL_A.HOKOKUSHO_SOFU_FLG_1      AS  HOKOKUSHO_SOFU_FLG_1      -- 報告書送付フラグ１\n      ,TBL_A.HOKOKU_CD_2               AS  HOKOKU_CD_2               -- 報告先CD２\n      ,TBL_A.HOKOKU_MEI_2              AS  HOKOKU_MEI_2              -- 報告先名２\n      ,TBL_A.BUSHO_2                   AS  BUSHO_2                   -- 部署２\n      ,TBL_A.TANTO_2                   AS  TANTO_2                   -- 担当者２\n      ,TBL_A.TEL_2                     AS  TEL_2                     -- TEL２\n      ,TBL_A.FAX_2                     AS  FAX_2                     -- FAX２\n      ,TBL_A.MAIL_2                    AS  MAIL_2                    -- E-MAIL２\n      ,TBL_A.YUBIN_NO_2                AS  YUBIN_NO_2                -- 郵便番号２\n      ,TBL_A.ADDRESS_2                 AS  ADDRESS_2                 -- 住所２\n      ,TBL_A.CHUKAN_SOFU_FLG_2         AS  CHUKAN_SOFU_FLG_2         -- 中間報告送付フラグ２\n      ,TBL_A.SOKUHO_SOFU_FLG_2         AS  SOKUHO_SOFU_FLG_2         -- 速報送付フラグ２\n      ,TBL_A.HOKOKUSHO_SOFU_FLG_2      AS  HOKOKUSHO_SOFU_FLG_2      -- 報告書送付フラグ２\n      ,TBL_A.HOKOKU_CD_3               AS  HOKOKU_CD_3               -- 報告先CD３\n      ,TBL_A.HOKOKU_MEI_3              AS  HOKOKU_MEI_3              -- 報告先名３\n      ,TBL_A.BUSHO_3                   AS  BUSHO_3                   -- 部署３\n      ,TBL_A.TANTO_3                   AS  TANTO_3                   -- 担当者３\n      ,TBL_A.TEL_3                     AS  TEL_3                     -- TEL３\n      ,TBL_A.FAX_3                     AS  FAX_3                     -- FAX３\n      ,TBL_A.MAIL_3                    AS  MAIL_3                    -- E-MAIL３\n      ,TBL_A.YUBIN_NO_3                AS  YUBIN_NO_3                -- 郵便番号３\n      ,TBL_A.ADDRESS_3                 AS  ADDRESS_3                 -- 住所３\n      ,TBL_A.CHUKAN_SOFU_FLG_3         AS  CHUKAN_SOFU_FLG_3         -- 中間報告送付フラグ３\n      ,TBL_A.SOKUHO_SOFU_FLG_3         AS  SOKUHO_SOFU_FLG_3         -- 速報送付フラグ３\n      ,TBL_A.HOKOKUSHO_SOFU_FLG_3      AS  HOKOKUSHO_SOFU_FLG_3      -- 報告書送付フラグ３\n      ,TBL_A.DEMPYO_HAKKO_KBN          AS  DEMPYO_HAKKO_KBN          -- 伝票発行区分\n      ,M_DEMPYO_HAKKO_KBN.DATA_1       AS  DEMPYO_HAKKO_KBN_MEI      -- 伝票発行区分名\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,  \q|\q + ISNULL(TBL_A.ATENA_CD       ,\q\q)    -- 報告書宛名CD\n       + \q|\q + ISNULL(TBL_A.ATENA          ,\q\q)    -- 報告書宛名\n       + \q|\q + ISNULL(TBL_A.SEIKYU_CD      ,\q\q)    -- 請求先CD\n       + \q|\q + ISNULL(M_SEIKYU_MEI.AITE_MEI,\q\q)    -- 請求先名\n       + \q|\q + ISNULL(M_BUN_HOHO.DATA_1    ,\q\q)    -- 分析方法名\n       + \q|\q + ISNULL(TBL_A.KENMEI         ,\q\q)    -- 件名\n       + \q|\q + ISNULL(TBL_A.MOKUTEKI       ,\q\q)    -- 目的\n       + \q|\q + ISNULL(TBL_A.KISAI_JIKO     ,\q\q)    -- 報告書記載事項\n       + \q|\q + ISNULL(TBL_A.CHUKAN_NAIYO_1 ,\q\q)    -- 中間報告内容１\n       + \q|\q + ISNULL(TBL_A.CHUKAN_NAIYO_2 ,\q\q)    -- 中間報告内容２\n       + \q|\q + ISNULL(TBL_A.CHUKAN_NAIYO_3 ,\q\q)    -- 中間報告内容３\n       + \q|\q + ISNULL(TBL_A.BIKO           ,\q\q)    -- ヘッダ備考\n       + \q|\q + ISNULL(TBL_A.HOKOKU_MEI_1   ,\q\q)    -- 報告先名１\n       + \q|\q + ISNULL(TBL_A.BUSHO_1        ,\q\q)    -- 部署１\n       + \q|\q + ISNULL(TBL_A.TANTO_1        ,\q\q)    -- 担当者１\n       + \q|\q + ISNULL(TBL_A.HOKOKU_MEI_2   ,\q\q)    -- 報告先名２\n       + \q|\q + ISNULL(TBL_A.BUSHO_2        ,\q\q)    -- 部署２\n       + \q|\q + ISNULL(TBL_A.TANTO_2        ,\q\q)    -- 担当者２\n       + \q|\q + ISNULL(TBL_A.HOKOKU_MEI_3   ,\q\q)    -- 報告先名３\n       + \q|\q + ISNULL(TBL_A.BUSHO_3        ,\q\q)    -- 部署３\n       + \q|\q + ISNULL(TBL_A.TANTO_3        ,\q\q)    -- 担当者３\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_BUN_JUCHU_H                   AS  TBL_A                     -- 【分析受注ヘッダテーブル】\n  LEFT JOIN                                                          -- 【分析ステータス用】\n       M_MEISHO_B                      AS  M_BUNSEKI_STS\n    ON M_BUNSEKI_STS.MEISHO_KBN        =   \qS003\q\n   AND M_BUNSEKI_STS.MEISHO_CD         =   TBL_A.BUNSEKI_STS\n  LEFT JOIN                                                          -- 【分析受注区分用】\n       M_MEISHO_B                      AS  M_JUCHU_KBN\n    ON M_JUCHU_KBN.MEISHO_KBN          =   \qK012\q\n   AND M_JUCHU_KBN.MEISHO_CD           =   TBL_A.JUCHU_KBN\n  LEFT JOIN                                                          -- 【担当部署用】\n       M_MEISHO_B                      AS  M_BUSHO\n    ON M_BUSHO.MEISHO_KBN              =   \qM001\q\n   AND M_BUSHO.MEISHO_CD               =   TBL_A.BUSHO_CD\n  LEFT JOIN                                                          -- 【入力者名用】\n       M_SHAIN                         AS  M_NYURYOKUSHA\n    ON M_NYURYOKUSHA.SHAIN_CD          =   TBL_A.NYURYOKUSHA_CD\n  LEFT JOIN                                                          -- 【請求先名用】\n       M_AITESAKI                      AS  M_SEIKYU_MEI\n    ON M_SEIKYU_MEI.AITE_CD            =   TBL_A.SEIKYU_CD\n  LEFT JOIN                                                          -- 【分析方法用】\n       M_MEISHO_B                      AS  M_BUN_HOHO\n    ON M_BUN_HOHO.MEISHO_KBN           =   \qK017\q\n   AND M_BUN_HOHO.MEISHO_CD            =   TBL_A.BUNSEKI_HOHO_CD\n  LEFT JOIN                                                          -- 【報告書形式用】\n       M_MEISHO_B                      AS  M_KEISHIKI_KBN\n    ON M_KEISHIKI_KBN.MEISHO_KBN       =   \qK013\q\n   AND M_KEISHIKI_KBN.MEISHO_CD        =   TBL_A.KEISHIKI_KBN\n  LEFT JOIN                                                          -- 【営業担当者名用】\n       M_SHAIN                         AS  M_EIGYO_TANTO\n    ON M_EIGYO_TANTO.SHAIN_CD          =   TBL_A.EIGYO_TANTO_CD\n  LEFT JOIN                                                          -- 【速報区分用】\n       M_MEISHO_B                      AS  M_SOKUHO_KBN\n    ON M_SOKUHO_KBN.MEISHO_KBN         =   \qK027\q\n   AND M_SOKUHO_KBN.MEISHO_CD          =   TBL_A.SOKUHO_KBN\n  LEFT JOIN                                                          -- 【報告書外装用】\n       M_MEISHO_B                      AS  M_GAISO_KBN\n    ON M_GAISO_KBN.MEISHO_KBN          =   \qK014\q\n   AND M_GAISO_KBN.MEISHO_CD           =   TBL_A.GAISO_KBN\n  LEFT JOIN                                                          -- 【報告方法用】\n       M_MEISHO_B                      AS  M_HOKOKU_HOHO_KBN\n    ON M_HOKOKU_HOHO_KBN.MEISHO_KBN    =   \qK016\q\n   AND M_HOKOKU_HOHO_KBN.MEISHO_CD     =   TBL_A.HOKOKU_HOHO_KBN\n  LEFT JOIN                                                          -- 【伝票発行区分用】\n       M_MEISHO_B                      AS  M_DEMPYO_HAKKO_KBN\n    ON M_DEMPYO_HAKKO_KBN.MEISHO_KBN   =   \qK026\q\n   AND M_DEMPYO_HAKKO_KBN.MEISHO_CD    =   TBL_A.DEMPYO_HAKKO_KBN\n     )                                 AS  TBL_A                     -- 【分析受注ヘッダテーブルＷＫ】\n\n\n  LEFT JOIN                                                          -- 【フリーワード(ボディ)】\n     (\n       SELECT TBL_B.JURI_NO\n             ,(                                    -- フリーワード検索用１\n               SELECT TBL_C.FREEWD1              AS  [data()]\n                 FROM V_C_BUN_JUCHU_B            AS  TBL_C\n                WHERE TBL_C.JURI_NO              =   TBL_B.JURI_NO\n                FOR XML PATH (\q\q)\n              )\n                                                 AS  FREEWD\n         FROM V_C_BUN_JUCHU_B                    AS  TBL_B\n        GROUP BY\n              TBL_B.JURI_NO\n     ) TBL_F1\n    ON TBL_F1.JURI_NO                  =   TBL_A.JURI_NO\n  LEFT JOIN                                                          -- 【フリーワード(詳細)】\n     (\n       SELECT TBL_B.JURI_NO\n             ,(                                    -- フリーワード検索用\n               SELECT TBL_C.FREEWD               AS  [data()]\n                 FROM V_C_BUN_JUCHU_SHO_K        AS  TBL_C\n                WHERE TBL_C.JURI_NO              =   TBL_B.JURI_NO\n                FOR XML PATH (\q\q)\n              )\n                                                 AS  FREEWD\n         FROM V_C_BUN_JUCHU_SHO_K                AS  TBL_B\n        GROUP BY\n              TBL_B.JURI_NO\n     ) TBL_F2\n    ON TBL_F2.JURI_NO                  =   TBL_A.JURI_NO\n
