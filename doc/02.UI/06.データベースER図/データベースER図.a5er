ViewOption=
Query=SELECT \n       TBL_H.SHIIRE_NO                                          -- 仕入No.\n      ,TBL_H.SHIIRE_DATE                                        -- 仕入日\n      ,TBL_H.SHIIRE_KAMOKU                                      -- 仕入科目CD\n      ,TBL_H.SHIIRE_KAMOKU_MEI                                  -- 仕入科目名\n      ,TBL_H.DENKU_KBN                                          -- 伝区CD\n      ,TBL_H.DENKU_KBN_MEI                                      -- 伝区名\n      ,TBL_H.KOBAI_KBN                                          -- 購買区分CD\n      ,TBL_H.KOBAI_KBN_MEI                                      -- 購買区分名\n      ,TBL_H.KANJO_KAMOKU                                       -- 勘定科目CD\n      ,TBL_H.KANJO_KAMOKU_MEI                                   -- 勘定科目名\n      ,TBL_H.SHIIRE_NYURYOKUSHA_CD                              -- 仕入入力者CD\n      ,TBL_H.SHIIRE_NYURYOKUSHA_MEI                             -- 仕入入力者名\n      ,TBL_H.SHIHARAI_DATE                                      -- 支払予定日\n      ,TBL_H.SHIIRE_STS                                         -- 仕入ステータスCD\n      ,TBL_H.SHIIRE_STS_MEI                                     -- 仕入ステータス名\n      ,TBL_H.SHIIRE_CD                                          -- 仕入先CD\n      ,TBL_H.SHIIRE_MEI                                         -- 仕入先名\n      ,TBL_H.TAX_RATE                                           -- 税率\n      ,TBL_H.NUKI_TOTAL                                         -- 税抜合計\n      ,TBL_H.TAX                                                -- 消費税\n      ,TBL_H.KOMI_TOTAL                                         -- 税込合計\n      ,TBL_H.SHOKAN_NO                                          -- 商管伝票No.\n      ,TBL_H.SHOKAN_LINK_DATETIME                               -- 連動日時\n      ,TBL_B.GYO_NO                                             -- 行No.\n      ,TBL_B.KOBAIHIN_CD                                        -- 購買品CD\n      ,TBL_B.KOBAIHIN_MEI                                       -- 購買品名\n      ,J_SHOHIN.SHOHIN_BUNRUI                                   -- 商品分類CD\n      ,J_SHOHIN_BUNRUI.DATA_1           AS SHOHIN_BUNRUI_MEI    -- 商品分類名\n      ,J_SHOHIN.SHIYAKU_FLG                                     -- 試薬フラグ\n      ,J_SHOHIN.CHOSEI_SHIYAKU_FLG                              -- 調製試薬フラグ\n      ,J_SHOHIN.DOKU_FLG                                        -- 毒劇物フラグ\n      ,J_SHOHIN.HYOJUN_KOSU                                     -- PRTR\n      ,TBL_B.TANKA                                              -- 単価\n      ,TBL_B.IRISU                                              -- 入数\n      ,TBL_B.IRISU_TANI                                         -- 入数単位\n      ,TBL_B.SURYO                                              -- 数量\n      ,TBL_B.BARA_TANI                                          -- バラ単位\n      ,TBL_B.SOSU                                               -- 注文総数\n      ,TBL_B.TOTAL                                              -- 合計金額\n      ,TBL_B.MAKER_CD                                           -- メーカーCD\n      ,TBL_B.MAKER_MEI                                          -- メーカー名\n      ,TBL_B.YOSAN_CD                                           -- 予算コード\n      ,TBL_B.JURI_NO                                            -- 受理No.\n      ,TBL_B.BIKO                                               -- 備考\n      ,TBL_B.KOBAI_NO                                           -- 購買申請No.\n      ,TBL_B.IRAI_NO                                            -- 依頼No.\n      ,TBL_H.DEL_FLG                                            -- 削除フラグ\n\n  FROM V_C_SHIIRE_H                     AS TBL_H\n  LEFT JOIN\n       V_C_SHIIRE_B                     AS TBL_B\n    ON TBL_B.SHIIRE_NO                  =  TBL_H.SHIIRE_NO\n  -- 商品マスタ\n  LEFT JOIN\n       M_SHOHIN                         AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD               =  TBL_B.KOBAIHIN_CD\n  -- 商品分類区分\n  LEFT JOIN\n       M_MEISHO_B                       AS J_SHOHIN_BUNRUI\n    ON J_SHOHIN_BUNRUI.MEISHO_KBN       =  \qK025\q\n   AND J_SHOHIN_BUNRUI.MEISHO_CD        =  J_SHOHIN.SHOHIN_BUNRUI\n\n
