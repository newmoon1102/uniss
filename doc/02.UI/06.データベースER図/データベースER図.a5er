LName=販売商品受注ボディビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT T_HAN_JUCHU_B.GYO_NO             AS GYO_NO           --行番号\n      ,T_HAN_JUCHU_B.ROW_KBN            AS ROW_KBN          --行区分\n      ,CASE J_SHOHIN.DOKU_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                              AS DOKU_FLG         --毒劇物フラグ\n      ,T_HAN_JUCHU_B.SHOHIN_CD          AS SHOHIN_CD        --商品CD\n      ,T_HAN_JUCHU_B.SHOHIN_MEI         AS SHOHIN_MEI       --商品名\n      ,T_HAN_JUCHU_B.JUCHU_SURYO        AS JUCHU_SURYO      --受注数量\n      ,T_HAN_JUCHU_B.JUCHU_TANI         AS JUCHU_TANI       --受注単位\n      ,T_HAN_JUCHU_B.HAMBAI_TANKA       AS HAMBAI_TANKA     --販売単価\n      ,T_HAN_JUCHU_B.HAMBAI_KINGAKU     AS HAMBAI_KINGAKU   --販売金額\n      ,T_HAN_JUCHU_B.TEKIYO             AS TEKIYO           --摘要\n      ,T_HAN_JUCHU_B.TEIKA              AS TEIKA            --定価\n      ,T_HAN_JUCHU_B.GENTANKA           AS GENTANKA         --原単価\n      ,J_SHOHIN.SHIIRE_IRISU            AS IRISU            --入数\n      ,T_HAN_JUCHU_B.BARA_TANI          AS BARA_TANI        --バラ単位\n      ,T_HAN_JUCHU_B.GENKA_SURYO        AS GENKA_SURYO      --原価数量\n      ,T_HAN_JUCHU_B.IRISU_TANI         AS IRISU_TANI       --入数単位\n      ,T_HAN_JUCHU_B.TOTAL              AS TOTAL            --総数\n      ,T_HAN_JUCHU_B.GENKA_KINGAKU      AS GENKA_KINGAKU    --原価金額\n      ,T_HAN_JUCHU_B.ARARIRITSU         AS ARARIRITSU       --粗利率\n      ,T_HAN_JUCHU_B.ARARIEKI           AS ARARIEKI         --粗利益\n      ,T_HAN_JUCHU_B.SHIIRE_CD          AS SHIIRE_CD        --仕入先CD\n      ,J_SHIIRE.AITE_MEI                AS SHIIRE_MEI       --仕入先名\n      ,T_HAN_JUCHU_B.MAKER_CD           AS MAKER_CD         --メーカーCD\n      ,J_MAKER.AITE_MEI                 AS MAKER_MEI        --メーカー名\n      ,T_HAN_JUCHU_B.JURI_NO            AS JURI_NO          --受理No.\n  FROM T_HAN_JUCHU_B\n  --商品\n  LEFT JOIN\n       M_SHOHIN     AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD = T_HAN_JUCHU_B.SHOHIN_CD\n  --仕入先\n  LEFT JOIN\n       M_AITESAKI   AS J_SHIIRE\n    ON J_SHIIRE.AITE_CD = T_HAN_JUCHU_B.SHIIRE_CD\n  --メーカー\n  LEFT JOIN\n       M_AITESAKI   AS J_MAKER\n    ON J_MAKER.AITE_CD = T_HAN_JUCHU_B.MAKER_CD\n
