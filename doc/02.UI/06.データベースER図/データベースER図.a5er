[View]
PName=V_AITESAKI_LIST
LName=相手先マスタリストビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT \q＞\q                                 AS  GYO_MOJI        --行文字\n      ,CASE M_AITESAKI.SEIKYU_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  SEIKYU_FLG      --請求フラグ\n      ,CASE M_AITESAKI.SHIRE_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  SHIIRE_FLG       --仕入フラグ\n      ,CASE M_AITESAKI.HASSO_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  HASSO_FLG       --発送フラグ\n      ,CASE M_AITESAKI.HOKOKU_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  HOKOKU_FLG      --報告フラグ\n      ,CASE M_AITESAKI.MAKER_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  MAKER_FLG       --メーカフラグ\n      ,CASE M_AITESAKI.GAICHU_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  GAICHU_FLG      --外注フラグ\n      ,CASE M_AITESAKI.KANKO_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  KANKO_FLG       --官公庁フラグ\n      ,CASE M_AITESAKI.DOGYO_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  DOGYO_FLG       --同業者フラグ\n      ,CASE M_AITESAKI.SPOT_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  SPOT_FLG        --スポットフラグ\n      ,M_AITESAKI.AITE_CD                   AS  AITE_CD         --相手先CD\n      ,M_AITESAKI.AITE_MEI                  AS  AITE_MEI        --相手先名\n      ,EIGYO.SHIMEI                         AS  EIGYO_MEI       --営業担当者名\n      ,M_AITESAKI.YUBIN_NO                  AS  YUBIN_NO        --相手先郵便番号\n      ,M_AITESAKI.ADDRESS_1                 AS  ADDRESS_1       --相手先住所1\n      ,M_AITESAKI.ADDRESS_2                 AS  ADDRESS_2       --相手先住所2\n      ,M_AITESAKI.TEL                       AS  TEL             --相手先TEL\n      ,M_AITESAKI.FAX                       AS  FAX             --相手先FAX\n      ,M_AITESAKI.MAIL                      AS  MAIL            --相手先MAIL\n      ,M_AITESAKI.BUSHO_MEI                 AS  BUSHO_MEI       --相手先部署名\n      ,M_AITESAKI.TANTO_MEI                 AS  TANTO_MEI       --相手先担当者名\n      ,M_AITESAKI.BIKO                      AS  BIKO            --備考\n      ,CASE M_AITESAKI.MISHIYO_FLG \n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS  MISHIYO_FLG     --未使用フラグ\n      ,M_AITESAKI.DBS_CREATE_DATE           AS  DBS_CREATE_DATA --登録日時\n      ,M_AITESAKI.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE --更新日時\n      ,M_AITESAKI.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD  --営業担当者CD\n      , \q|\q + M_AITESAKI.AITE_MEI + \n        \q|\q + ISNULL(EIGYO.SHIMEI,\q\q) + \n        \q|\q + M_AITESAKI.TANTO_MEI + \n        \q|\q + M_AITESAKI.ADDRESS_1 + \n        \q|\q                                 AS FREEWD           --フリーワード検索用\n  FROM M_AITESAKI\n--営業担当名\n  LEFT JOIN M_SHAIN                         AS  EIGYO\n    ON M_AITESAKI.EIGYO_TANTO_CD = EIGYO.SHAIN_CD\n
