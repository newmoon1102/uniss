[View]
PName=V_URIAGE_RENDO_LIST
LName=売上連動リストビュー
Tag=ビュー
Comment=UNNH0090【売上連動】
ViewOption=
Query=--*DataTitle V_URIAGE_RENDO_LIST(売上連動リストビュー)\nSELECT TBL_H.URIAGE_STS_MEI            AS  URIAGE_STS_MEI            -- 売上ステータス名\n      ,TBL_H.DEMPYO_KBN_MEI            AS  DEMPYO_KBN_MEI            -- 伝票区分名\n      ,TBL_H.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,TBL_H.SEIKYU_MEI                AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_H.DEMPYO_SOFU_MEI           AS  DEMPYO_SOFU_MEI           -- 伝票送付先名\n      ,M_SEIKYUHOHO.DATA_1             AS  SEIKYUHOHO                -- 請求締日\n      ,TBL_H.URIAGE_DATE               AS  URIAGE_DATE               -- 売上日\n      ,TBL_H.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_H.KAISHU_DATE               AS  KAISHU_DATE               -- 回収予定日\n      ,TBL_H.URI_NYU_MEI               AS  URI_NYU_MEI               -- 売上入力者名\n      ,TBL_H.NUKI_TOTAL                AS  NUKI_TOTAL                -- 税抜合計\n      ,TBL_H.TAX                       AS  TAX                       -- 消費税\n      ,TBL_H.KOMI_TOTAL                AS  KOMI_TOTAL                -- 税込合計\n      ,TBL_H.GENKA_TOTAL               AS  GENKA_TOTAL               -- 原価合計\n      ,TBL_H.ARARIEKI                  AS  ARARIEKI                  -- 粗利益\n      ,TBL_H.ARARIRITSU                AS  ARARIRITSU                -- 粗利率\n      ,TBL_H.JURI_NO                   AS  JURI_NO                   -- 対象受理No.\n      ,TBL_H.URIAGE_NO                 AS  URIAGE_NO                 -- 売上No.\n      ,TBL_H.DEMPYO_KBN                AS  DEMPYO_KBN                -- 伝票区分\n      ,TBL_H.URIAGE_NYURYOKUSHA_CD     AS  URIAGE_NYURYOKUSHA_CD     -- 売上入力者CD\n      ,TBL_H.URIAGE_STS                AS  URIAGE_STS                -- 売上ステータス\n      ,M_SEIKYUSAKI.SEIKYUHOHO_KBN     AS  SEIKYUHOHO_KBN            -- 請求締日区分\n      ,TBL_H.FREEWD2                   AS  FREEWD                    -- フリーワード検索用\n  FROM V_C_URIAGE_H                    AS  TBL_H\n  LEFT JOIN\n       M_AITESAKI                      AS  M_SEIKYUSAKI\n    ON M_SEIKYUSAKI.AITE_CD            =   TBL_H.SEIKYU_CD\n  LEFT JOIN\n       M_MEISHO_B                      AS  M_SEIKYUHOHO\n    ON M_SEIKYUHOHO.MEISHO_KBN         =   \qK002\q\n   AND M_SEIKYUHOHO.MEISHO_CD          =   M_SEIKYUSAKI.SEIKYUHOHO_KBN\n WHERE TBL_H.DEL_FLG                   =   \qFalse\q\n--   AND TBL_H.DEMPYO_KBN                <>  \q5\q      -- 結合用を除く\n   AND TBL_H.URIAGE_STS                <>  \q4\q      -- 売上連携済を除く\n   AND TBL_H.RENDO_KBN                 =   \q2\q      -- 未連動のみ\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="売上関連",2450,2050
