[View]
PName=V_SHIIRE_RENDO_LIST
LName=仕入連動リストビュー
Tag=ビュー
Comment=UNKB0220(仕入連動)
ViewOption=
Query=--*DataTitle V_SHIIRE_RENDO_LIST(仕入連動リストビュー)\nSELECT TBL_H.SHIIRE_STS_MEI            AS  SHIIRE_STS_MEI            -- 仕入ステータス名\n      ,TBL_H.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_H.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_H.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_H.SHIIRE_DATE               AS  SHIIRE_DATE               -- 仕入日\n      ,TBL_H.SHIHARAI_DATE             AS  SHIHARAI_DATE             -- 支払予定日\n      ,TBL_H.SHIIRE_NYURYOKUSHA_MEI    AS  SHIIRE_NYURYOKUSHA_MEI    -- 仕入入力者名\n      ,TBL_H.NUKI_TOTAL                AS  NUKI_TOTAL                -- 税抜合計\n      ,TBL_H.TAX                       AS  TAX                       -- 消費税\n      ,TBL_H.KOMI_TOTAL                AS  KOMI_TOTAL                -- 税込合計\n      ,TBL_H.SHIIRE_NO                 AS  SHIIRE_NO                 -- 仕入No.\n      ,CASE\n       WHEN TBL_H.SHOKAN_LINK_DATETIME IS  NULL\n       THEN \qFalse\q\n       ELSE \qTrue\q\n       END                             AS  RENDO_SUMI_FLG            -- 連動済フラグ\n      ,TBL_H.SHIIRE_STS                AS  SHIIRE_STS                -- 仕入ステータス\n      ,TBL_H.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_H.SHIIRE_NYURYOKUSHA_CD     AS  SHIIRE_NYURYOKUSHA_CD     -- 仕入入力者CD\n      ,TBL_H.FREEWD2                   AS  FREEWD                    -- フリーワード検索用\n  FROM V_C_SHIIRE_H                    AS  TBL_H                     -- 【仕入ヘッダ共通ビュー】\n WHERE TBL_H.DEL_FLG                   =   \qFalse\q\n   AND TBL_H.SHIIRE_STS                <>  \q3\q\n\n
