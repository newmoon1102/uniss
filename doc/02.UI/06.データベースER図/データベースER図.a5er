Tag=ビュー
Comment=
ViewOption=
Query=SELECT V_C_BUN_JUCHU_B.JURI_NO            AS JURI_NO              --受理No.\n      ,V_C_BUN_JUCHU_B.JURI_EDA_NO        AS JURI_EDA_NO          --枝番\n      ,V_C_BUN_JUCHU_B.SAISHU_KAISHA_MEI  AS SAISHU_KAISHA_MEI    --採取会社名\n      ,V_C_BUN_JUCHU_B.SAISHU_SHA         AS SAISHU_SHA           --採取者\n      ,V_C_BUN_JUCHU_B.SAISHU_HOHO        AS SAISHU_HOHO          --採取方法\n      ,V_C_BUN_JUCHU_B.SAISHU_BASHO       AS SAISHU_BASHO         --採取場所\n      ,V_C_BUN_JUCHU_B.TENKO              AS TENKO                --天候\n      ,V_C_BUN_JUCHU_B.KION               AS KION                 --気温\n      ,V_C_BUN_JUCHU_B.SUION              AS SUION                --水温\n      ,V_C_BUN_JUCHU_B.SAISHU_DATE        AS SAISHU_DATE          --採取日\n      ,V_C_BUN_JUCHU_B.SAISHU_TIME        AS SAISHU_TIME          --採取時間\n      ,V_C_BUN_JUCHU_B.SHIRYO_SHURUI      AS SHIRYO_SHURUI_MEI    --試料種類\n      ,V_C_BUN_JUCHU_B.SHIRYO_MEI         AS SHIRYO_MEI           --試料名\n      ,V_C_BUN_JUCHU_B.IKKATSU_M          AS IKKATSU_M            --一括記載m(左辺)\n      ,V_C_BUN_JUCHU_B.IKKATSU_N          AS IKKATSU_N            --一括記載n(右辺)\n      ,V_C_BUN_JUCHU_B.BUNSEKI_GUN_CD     AS BUNSEKI_GUN_CD       --分析項目群CD\n      ,V_C_BUN_JUCHU_B.BUNSEKI_GUN_MEI    AS BUNSEKI_GUN_MEI      --分析項目群名 \n      ,V_C_BUN_JUCHU_B.EBIKO              AS EBIKO                --枝番備考\n      ,T_BUN_JUCHU_SHOSAI.GYO_NO          AS GYO_NO               --行No.\n      ,T_BUN_JUCHU_SHOSAI.BUNSEKI_CD      AS BUNSEKI_CD           --分析項目CD\n      ,T_BUN_JUCHU_SHOSAI.BUNSEKI_MEI     AS BUNSEKI_MEI          --分析項目名\n      ,T_BUN_JUCHU_SHOSAI.BUNSEKI_TANI    AS BUNSEKI_TANI         --単位\n      ,T_BUN_JUCHU_SHOSAI.BUNSEKI_DATA    AS BUNSEKI_DATA         --データ\n      ,T_BUN_JUCHU_SHOSAI.BUNSEKI_NO      AS BUNSEKI_NO           --分析番号\n  FROM V_C_BUN_JUCHU_B\n  --詳細テーブル\n  LEFT JOIN\n       T_BUN_JUCHU_SHOSAI\n    ON T_BUN_JUCHU_SHOSAI.JURI_NO     = V_C_BUN_JUCHU_B.JURI_NO\n   AND T_BUN_JUCHU_SHOSAI.JURI_EDA_NO = V_C_BUN_JUCHU_B.JURI_EDA_NO\n
