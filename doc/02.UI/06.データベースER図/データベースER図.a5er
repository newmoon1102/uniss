[View]
PName=V_URIAGE_REPORT2
LName=売上集計表ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_URIAGE_REPORT2(売上集計表ビュー)\nSELECT V_C_URIAGE_H.URIAGE_DATE     AS URIAGE_DATE      --売上日\n      ,V_C_URIAGE_H.URIAGE_NO       AS URIAGE_NO        --売上№\n      ,V_C_URIAGE_H.EIGYO_TANTO_CD  AS EIGYO_TANTO_CD   --営業担当者CD\n      ,V_C_URIAGE_H.EIGYO_TANTO_MEI AS EIGYO_TANTO_MEI  --営業担当者名\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q081100\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_1 --081100計量証明事業水質\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q081200\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_2 --081200研究支援分析売上高\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q081300\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_3 --081300作業環境売上高\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q001300\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_4 --001300大気測定売上高\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q121000\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_5 --121000ﾀﾞｲｵｷｼﾝ類売上高\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q081001\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_6 --081001商品売上高-販売商品\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q081002\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_7 --081002商品売上高-営業委託\n      ,CASE V_C_URIAGE_H.KANJO_KAMOKU WHEN \q081003\q THEN T_URIAGE_B.HAMBAI_KINGAKU ELSE NULL END AS URI_KIN_8 --081003商品売上高-その他\n  FROM V_C_URIAGE_H\n   LEFT JOIN\n       T_URIAGE_B\n    ON T_URIAGE_B.URIAGE_NO = V_C_URIAGE_H.URIAGE_NO\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="売上関連",1800,1800
Page=売上関連
Left=1800
