[View]
PName=V_KOBAI_B_SANSHO
LName=購買申請入力参照作成ボディビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT V_C_KOBAI_B.KOBAI_GYO_NO         AS KOBAI_GYO_NO         --行\n      ,NULL                             AS ALERT                --警告\n      ,NULL                             AS JOGAI_CHECK          --除外チェックボックス\n      ,\qFalse\q                          AS JOGAI_FLG            --除外フラグ\n      ,NULL                             AS KOBAI_SEQ            --SEQ\n      ,1                                AS KOBAI_STS            --ステータスCD\n--       ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n--        WHEN \q\q THEN V_C_KOBAI_B.HOKAN_BASHO_KBN\n--        ELSE J_KOBAIHIN.HOKANBASHO_KBN\n--        END                              AS HOKAN_BASHO_KBN      --保管場所区分\n      ,V_C_KOBAI_B.HOKAN_BASHO_KBN      AS HOKAN_BASHO_KBN      --保管場所区分\n      ,V_C_KOBAI_B.YOSAN_MEI            AS YOSAN_MEI            --予算コード名\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.SHIIRE_MEI\n       ELSE J_SHIIRE.AITE_MEI\n       END                              AS SHIIRE_MEI           --仕入先名\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.MAKER_MEI\n       ELSE J_MAKER.AITE_MEI\n       END                              AS MAKER_MEI            --メーカー名\n      ,J_SHIIRE.SHIIRE_HASU_KBN         AS SHIIRE_HASU_KBN      --仕入先端数処理区分\n      ,V_C_KOBAI_B.SHIKIICHI            AS SHIKIICHI            --閾値\n      ,V_C_KOBAI_B.HYOJUN_NOKI          AS HYOJUN_NOKI          --標準納期\n      ,V_C_KOBAI_B.LAST_SHINSEI_DATE    AS LAST_SHINSEI_DATE    --最終申請日\n      ,V_C_KOBAI_B.SHOHIN_BUNRUI_CD     AS SHOHIN_BUNRUI_CD     --商品分類区分\n      ,V_C_KOBAI_B.SHIYAKU_FLG          AS SHIYAKU_FLG          --試薬フラグ\n      ,V_C_KOBAI_B.KOBAIHIN_KBN         AS KOBAIHIN_KBN         --購買品区分\n      ,J_KOBAIHIN.SHOHIN_CD             AS KOBAIHIN_CD          --購買品CD\n      ,V_C_KOBAI_B.KOBAIHIN_MEI         AS KOBAIHIN_MEI         --購買品名\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.TANKA\n       ELSE J_KOBAIHIN.SHIIRE_TANKA\n       END                              AS TANKA                --単価\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.IRISU\n       ELSE J_KOBAIHIN.SHIIRE_IRISU\n       END                              AS IRISU                --入数\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.BARA_TANI\n       ELSE J_KOBAIHIN.BARA_TANI\n       END                              AS BARA_TANI            --バラ単位\n      ,V_C_KOBAI_B.SURYO                AS SURYO                --数量\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.IRISU_TANI\n       ELSE J_KOBAIHIN.SHIIRE_IRISU_TANI\n       END                              AS IRISU_TANI           --入数単位\n      ,NULL                             AS SOSU                 --注文総数\n      ,NULL                             AS TOTAL                --合計金額\n      ,NULL                             AS NOKI                 --希望納期\n      ,J_HOKANBASHO.DATA_1              AS HOKAN_BASHO_KBN_MEI  --保管場所\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.SHIIRE_CD\n       ELSE J_KOBAIHIN.SHIIRE_CD\n       END                              AS SHIIRE_CD            --仕入先CD\n      ,NULL                             AS SHIIRE_MEI_DISP      --仕入先名(表示)\n      ,CASE V_C_KOBAI_B.KOBAIHIN_CD\n       WHEN \q\q THEN V_C_KOBAI_B.MAKER_CD\n       ELSE J_KOBAIHIN.MAKER_CD\n       END                              AS MAKER_CD             --メーカーCD\n      ,NULL                             AS MAKER_MEI_DISP       --メーカー名(表示)\n      ,NULL                             AS YOSAN_CD             --予算コード\n      ,NULL                             AS JURI_NO              --受理No.\n      ,V_C_KOBAI_B.BIKO                 AS BIKO                 --備考\n      ,J_STS.DATA_1                     AS KOBAI_STS_MEI        --ステータス名\n      ,NULL                             AS IRAI_NO              --依頼No.\n      ,V_C_KOBAI_B.KOBAI_NO             AS REF_KOBAI_NO         --参照用購買申請No.\n  FROM V_C_KOBAI_B\n  LEFT JOIN\n       T_IRAI\n    ON T_IRAI.KOBAI_NO  = V_C_KOBAI_B.KOBAI_NO\n   AND T_IRAI.KOBAI_SEQ = V_C_KOBAI_B.KOBAI_SEQ\n  LEFT JOIN\n       M_SHOHIN     AS J_KOBAIHIN\n    ON J_KOBAIHIN.SHOHIN_CD   = V_C_KOBAI_B.KOBAIHIN_CD\n   AND J_KOBAIHIN.MISHIYO_FLG = \qFalse\q\n  LEFT JOIN\n       M_AITESAKI   AS J_SHIIRE\n    ON J_SHIIRE.AITE_CD     = J_KOBAIHIN.SHIIRE_CD\n   AND J_SHIIRE.MISHIYO_FLG = \qFalse\q\n  LEFT JOIN\n       M_AITESAKI   AS J_MAKER\n    ON J_MAKER.AITE_CD     = J_KOBAIHIN.MAKER_CD\n   AND J_MAKER.MISHIYO_FLG = \qFalse\q\n  LEFT JOIN\n       M_MEISHO_B   AS J_HOKANBASHO\n    ON J_HOKANBASHO.MEISHO_KBN = \qM002\q\n   AND J_HOKANBASHO.MEISHO_CD  = J_KOBAIHIN.HOKANBASHO_KBN\n  LEFT JOIN\n       M_MEISHO_B   AS J_STS\n    ON J_STS.MEISHO_KBN = \qS005\q\n   AND J_STS.MEISHO_CD  = 1\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="購買関連(入力)",1450,450
Page=購買関連(入力)
Left=1450
Top=450

