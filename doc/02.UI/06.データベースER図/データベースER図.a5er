[View]
PName=V_URIAGE_LIST
LName=売上検索リストビュー
Tag=ビュー
Comment=UNNH0020【売上検索】
ViewOption=
Query=--*DataTitle V_URIAGE_LIST(売上検索リストビュー)\nSELECT \q＞\q                            AS  GYO_MOJI                  -- 選択\n      ,TBL_H.URIAGE_STS_MEI            AS  URIAGE_STS_MEI            -- 売上ステータス名\n      ,TBL_H.DEMPYO_KBN_MEI            AS  DEMPYO_KBN_MEI            -- 伝票区分名\n      ,TBL_H.RENDO_KBN_MEI             AS  RENDO_KBN_MEI             -- 商魂連動区分名\n      ,TBL_H.URIAGE_NO                 AS  URIAGE_NO                 -- 売上No.\n      ,TBL_H.URIAGE_DATE               AS  URIAGE_DATE               -- 売上日\n      ,TBL_H.KAISHU_DATE               AS  KAISHU_DATE               -- 回収予定日\n      ,TBL_H.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,TBL_H.SEIKYU_MEI                AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_H.DEMPYO_SOFU_MEI           AS  DEMPYO_SOFU_MEI           -- 伝票送付先名\n      ,TBL_H.BUSHO_MEI                 AS  BUSHO_MEI                 -- 担当部署名\n      ,TBL_H.EIGYO_TANTO_MEI           AS  EIGYO_TANTO_MEI           -- 営業担当者名\n      ,TBL_H.URI_NYU_MEI               AS  URI_NYU_MEI               -- 売上入力者名\n--       ,TBL_H.JURI_NO                   AS  JURI_NO                   -- 対象受理No.\n      ,CASE WHEN\n            ( SELECT COUNT(*)\n                FROM T_URIAGE_JUCHU_SELECT AS TBL_W\n               WHERE TBL_W.URIAGE_NO       =  TBL_H.URIAGE_NO\n            ) >= 1\n       THEN \q受注複数選択\q\n       ELSE TBL_H.JURI_NO\n       END                             AS  JURI_NO                   -- 対象受理No.\n      ,TBL_H.SAKI_URIAGE_NO            AS  SAKI_URIAGE_NO            -- 結合先売上No.\n      ,TBL_H.NYURYOKU_DATE_TIME        AS  NYURYOKU_DATE_TIME        -- 入力日時\n      ,TBL_H.TEKIYO                    AS  TEKIYO                    -- 摘要\n      ,TBL_H.SHOKON_NO                 AS  SHOKON_NO                 -- 商魂伝票No.\n      ,TBL_H.SHOKON_LIST_DATE_TIME     AS  SHOKON_LIST_DATE_TIME     -- 連動日時\n      ,CASE TBL_H.DEL_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                             AS  DEL                       -- 削除\n      ,TBL_H.DEMPYO_KBN                AS  DEMPYO_KBN                -- 伝票区分\n      ,TBL_H.RENDO_KBN                 AS  RENDO_KBN                 -- 商魂連動区分\n      ,TBL_H.BUSHO_CD                  AS  BUSHO_CD                  -- 担当部署CD\n      ,TBL_H.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,TBL_H.URIAGE_NYURYOKUSHA_CD     AS  URIAGE_NYURYOKUSHA_CD     -- 売上入力者CD\n      ,TBL_H.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_H.URIAGE_STS                AS  URIAGE_STS                -- 売上ステータス\n      ,TBL_H.BUNSEKI_HOHO_CD           AS  BUNSEKI_HOHO_CD           -- 分析方法CD\n      ,TBL_H.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,TBL_H.FREEWD2                   AS  FREEWD                    -- フリーワード検索用\n  FROM V_C_URIAGE_H                    AS  TBL_H\n
LNameByComment=1
