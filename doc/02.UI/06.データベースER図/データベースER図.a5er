[View]
PName=V_HAN_JUCHU_H
LName=販売商品受注ヘッダビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT T_HAN_JUCHU_H.JURI_NO                AS JURI_NO              --受理No.\n      ,T_HAN_JUCHU_H.HAMBAI_STS             AS HAMBAI_STS_CD        --販売受注ステータスCD\n      ,HAMBAI_STS.DATA_1                    AS HAMBAI_STS           --販売受注ステータス\n      ,HAMBAI_KBN.DATA_1                    AS HAMBAI_KBN           --販売受注区分\n      ,T_HAN_JUCHU_H.NYURYOKU_DATETIME      AS NYURYOKU_DATETIME    --入力日時\n      ,DEMPYO.DATA_1                        AS DEMPYO_HAKKO_KBN     --伝票発行区分\n      ,T_HAN_JUCHU_H.REF_JURI_NO            AS REF_JURI_NO          --参照元受理No.\n      ,T_HAN_JUCHU_H.NYURYOKUSHA_CD         AS NYURYOKUSHA_CD       --入力者名CD\n      ,NYURYOKU.SHIMEI                      AS NYURYOKUSHA          --入力者名\n      ,T_HAN_JUCHU_H.MITSU_NO               AS MITSU_NO             --対象見積No.\n      ,T_HAN_JUCHU_H.JUCHU_DATE             AS JUCHU_DATE           --受注日\n      ,T_HAN_JUCHU_H.NOHIN_DATE             AS NOHIN_DATE           --納品日\n      ,T_HAN_JUCHU_H.SEIKYU_CD              AS SEIKYUSAKI_CD        --請求先CD\n      ,SEIKYUSAKI.AITE_MEI                  AS SEIKYUSAKI_MEI       --請求先名\n      ,SEIKYUSAKI.SEIKYU_HASU_KBN           AS SEIKYU_HASU_KBN      --請求先端数処理区分\n      ,T_HAN_JUCHU_H.SEIKYU_TANTO_MEI       AS SEIKYU_TANTO_MEI     --請求先担当者名\n      ,T_HAN_JUCHU_H.KENMEI                 AS KENMEI               --件名\n      ,T_HAN_JUCHU_H.EIGYO_TANTO_CD         AS EIGYO_TANTO_CD       --営業担当者名CD\n      ,EIGYO_TANTO.SHIMEI                   AS EIGYO_TANTO          --営業担当者名\n      ,T_HAN_JUCHU_H.HASSO_CD               AS HASSO_CD             --発送先CD\n      ,T_HAN_JUCHU_H.HASSO_MEI              AS HASSO_MEI            --発送先名\n      ,T_HAN_JUCHU_H.HASSO_YUBIN_NO         AS HASSO_YUBIN_NO       --発送先郵便番号\n      ,T_HAN_JUCHU_H.HASSO_ADDRESS_1        AS HASSO_ADDRESS_1      --発送先住所１\n      ,T_HAN_JUCHU_H.HASSO_ADDRESS_2        AS HASSO_ADDRESS_2      --発送先住所２\n      ,T_HAN_JUCHU_H.HASSO_TEL              AS HASSO_TEL            --発送先ＴＥＬ\n      ,T_HAN_JUCHU_H.HASSO_FAX              AS HASSO_FAX            --発送先ＦＡＸ\n      ,T_HAN_JUCHU_H.HASSO_TANTO_MEI        AS HASSO_TANTO_MEI      --発送先担当者名\n      ,T_HAN_JUCHU_H.MAKER_FLG              AS MAKER_FLG            --メーカー直送フラグ\n      ,T_HAN_JUCHU_H.TAX_RATE               AS TAX_RATE             --消費税率\n      ,T_HAN_JUCHU_H.NUKI_TOTAL             AS NUKI_TOTAL           --税抜合計\n      ,T_HAN_JUCHU_H.TAX                    AS TAX                  --消費税\n      ,T_HAN_JUCHU_H.KOMI_TOTAL             AS KOMI_TOTAL           --税込合計\n      ,T_HAN_JUCHU_H.GENKA_TOTAL            AS GENKA_TOTAL          --原価合計\n      ,T_HAN_JUCHU_H.ARARIEKI               AS ARARIEKI             --粗利益\n      ,T_HAN_JUCHU_H.ARARIRITSU             AS ARARIRITSU           --粗利率\n      ,T_HAN_JUCHU_H.DEL_FLG                AS DEL_FLG              --削除フラグ\n  FROM T_HAN_JUCHU_H\n  --販売受注ステータス\n  LEFT JOIN\n       M_MEISHO_B             AS HAMBAI_STS\n    ON HAMBAI_STS.MEISHO_KBN = \qS004\q\n   AND T_HAN_JUCHU_H.HAMBAI_STS = HAMBAI_STS.MEISHO_CD\n  --販売受注区分\n  LEFT JOIN\n       M_MEISHO_B             AS HAMBAI_KBN\n    ON HAMBAI_KBN.MEISHO_KBN = \qK020\q\n   AND T_HAN_JUCHU_H.HAMBAI_KBN = HAMBAI_KBN.MEISHO_CD\n  --入力者\n  LEFT JOIN\n       M_SHAIN                AS NYURYOKU\n    ON T_HAN_JUCHU_H.NYURYOKUSHA_CD = NYURYOKU.SHAIN_CD\n  --請求先\n  LEFT JOIN\n       M_AITESAKI             AS SEIKYUSAKI\n    ON T_HAN_JUCHU_H.SEIKYU_CD = SEIKYUSAKI.AITE_CD\n  --営業担当者\n  LEFT JOIN\n       M_SHAIN                AS EIGYO_TANTO\n    ON T_HAN_JUCHU_H.EIGYO_TANTO_CD = EIGYO_TANTO.SHAIN_CD\n  --伝票発行区分\n  LEFT JOIN\n       M_MEISHO_B             AS DEMPYO\n    ON DEMPYO.MEISHO_KBN = \qK026\q\n   AND DEMPYO.MEISHO_CD  = T_HAN_JUCHU_H.DEMPYO_HAKKO_KBN\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
