PName=V_C_NYUSHUKKO
LName=入出庫共通ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_C_NYUSHUKKO(入出庫共通ビュー)\nSELECT TBL_A.NYUSHUKKO_NO              AS  NYUSHUKKO_NO              -- 入出庫No.\n      ,TBL_A.ZAIKO_NO                  AS  ZAIKO_NO                  -- 在庫管理No.\n      ,TBL_A.NYUSHUKKO_KBN             AS  NYUSHUKKO_KBN             -- 入出庫区分\n      ,TBL_A.NYURYOKUSHA_CD            AS  NYURYOKUSHA_CD            -- 入力者CD\n      ,M_NYURYOKUSHA.SHIMEI            AS  NYURYOKUSHA_MEI           -- 入力者名\n      ,TBL_A.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_A.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_A.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN           -- 保管場所区分\n      ,M_HOKAN_BASHO_KBN.DATA_1        AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_A.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_A.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_A.NYUKO_SURYO               AS  NYUKO_SURYO               -- 入庫数量\n      ,TBL_A.SHUKKO_DATE               AS  SHUKKO_DATE               -- 出庫日\n      ,TBL_A.SHUKKO_SURYO              AS  SHUKKO_SURYO              -- 出庫数量\n      ,TBL_A.SHUKKO_JIYU               AS  SHUKKO_JIYU               -- 出庫事由\n      ,  \q|\q + ISNULL(TBL_A.SHUKKO_JIYU,\q\q)        -- 出庫事由\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_NYUSHUKKO                     AS  TBL_A                     -- 【入出庫テーブル】\n  LEFT JOIN                                                          -- 【入力者名用】\n       M_SHAIN                         AS  M_NYURYOKUSHA\n    ON M_NYURYOKUSHA.SHAIN_CD          =   TBL_A.NYURYOKUSHA_CD\n  LEFT JOIN                                                          -- 【保管場所区分用】\n       M_MEISHO_B                      AS  M_HOKAN_BASHO_KBN\n    ON M_HOKAN_BASHO_KBN.MEISHO_KBN    =   \qM002\q\n   AND M_HOKAN_BASHO_KBN.MEISHO_CD     =   TBL_A.HOKAN_BASHO_KBN\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="入出庫関連",100,750
Page=入出庫関連
Left=100
Top=750

[View]
PName=V_C_ZAIKO
LName=在庫管理共通ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_C_ZAIKO(在庫管理共通ビュー)\nSELECT TBL_A.ZAIKO_NO                  AS  ZAIKO_NO                  -- 在庫管理No.\n      ,TBL_A.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,M_KOBAIHIN_CD.SHOHIN_MEI        AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_A.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN           -- 保管場所区分\n      ,M_HOKAN_BASHO_KBN.DATA_1        AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_A.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,M_SHIIRE_CD.AITE_MEI            AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_A.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,M_MAKER_CD.AITE_MEI             AS  MAKER_MEI                 -- メーカー名\n      ,TBL_A.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_A.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_A.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_A.ZAIKO_SURYO               AS  ZAIKO_SURYO               -- 在庫数量\n      ,TBL_A.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_A.LAST_NYUKO_DATE           AS  LAST_NYUKO_DATE           -- 最終入庫日\n      ,TBL_A.LAST_SHUKKO_DATE          AS  LAST_SHUKKO_DATE          -- 最終出庫日\n      ,  \q|\q + ISNULL(M_KOBAIHIN_CD.SHOHIN_MEI,\q\q) -- 購買品名\n       + \q|\q + ISNULL(M_SHIIRE_CD.AITE_MEI    ,\q\q) -- 仕入先名\n       + \q|\q + ISNULL(M_MAKER_CD.AITE_MEI     ,\q\q) -- メーカー名\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_ZAIKO                         AS  TBL_A                     -- 【在庫管理テーブル】\n  LEFT JOIN                                                          -- 【購買品CD用】\n       M_SHOHIN                        AS  M_KOBAIHIN_CD\n    ON M_KOBAIHIN_CD.SHOHIN_CD         =   TBL_A.KOBAIHIN_CD\n  LEFT JOIN                                                          -- 【保管場所区分用】\n       M_MEISHO_B                      AS  M_HOKAN_BASHO_KBN\n    ON M_HOKAN_BASHO_KBN.MEISHO_KBN    =   \qM002\q\n   AND M_HOKAN_BASHO_KBN.MEISHO_CD     =   TBL_A.HOKAN_BASHO_KBN\n  LEFT JOIN                                                          -- 【仕入先CD用】\n       M_AITESAKI                      AS  M_SHIIRE_CD\n    ON M_SHIIRE_CD.AITE_CD             =   TBL_A.SHIIRE_CD\n  LEFT JOIN                                                          -- 【メーカーCD用】\n       M_AITESAKI                      AS  M_MAKER_CD\n    ON M_MAKER_CD.AITE_CD              =   TBL_A.MAKER_CD\n
LNameByComment=1
