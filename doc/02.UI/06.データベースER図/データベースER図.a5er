[View]
PName=V_ZAIKO_KENSAKU_LIST
LName=試薬在庫検索リストビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT NULL                         AS GYO_NO               --行番号\n      ,TBL_A.KOBAIHIN_CD            AS KOBAIHIN_CD          --購買品CD\n      ,TBL_A.KOBAIHIN_MEI           AS KOBAIHIN_MEI         --購買品名\n      ,J_MAKER.AITE_CD              AS MAKER_CD             --メーカーCD\n      ,J_MAKER.AITE_MEI             AS MAKER_MEI            --メーカー名\n      ,J_SHOHIN.SHIIRE_IRISU        AS IRISU                --入数\n      ,J_SHOHIN.BARA_TANI           AS BARA_TANI            --バラ単位\n      ,SUM(TBL_A.ZAIKO_SURYO)       AS ZAIKO_SURYO          --在庫数\n      ,J_SHOHIN.SHIIRE_IRISU_TANI   AS IRISU_TANI           --入数単位\n      ,TBL_A.HOKAN_BASHO_KBN_MEI    AS HOKAN_BASHO_KBN_MEI  --保管場所名\n      ,CASE J_SHOHIN.DOKU_FLG\n       WHEN \qTrue\q THEN \q○\q\n       ELSE NULL\n       END                          AS DOKU_MARK            --毒劇物\n      ,J_SHIIRESAKI.AITE_CD         AS SHIIRE_CD            --仕入先CD\n      ,J_SHIIRESAKI.AITE_MEI        AS SHIIRE_MEI           --仕入先名\n      ,MAX(TBL_A.LAST_NYUKO_DATE)   AS LAST_NYUKO_DATE      --最終入庫日\n      ,MAX(TBL_A.LAST_SHUKKO_DATE)  AS LAST_SHUKKO_DATE     --最終出庫日\n      ,TBL_A.HOKAN_BASHO_KBN        AS HOKAN_BASHO_KBN      --保管場所区分\n      ,J_SHOHIN.DOKU_FLG            AS DOKU_FLG             --毒劇物フラグ\n      ,TBL_A.FREEWD                 AS FREEWD               --フリーワード\n  FROM V_C_ZAIKO AS TBL_A\n\n  LEFT JOIN\n       M_SHOHIN AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD = TBL_A.KOBAIHIN_CD\n  LEFT JOIN\n       M_AITESAKI AS J_SHIIRESAKI\n    ON J_SHIIRESAKI.AITE_CD = J_SHOHIN.SHIIRE_CD\n  LEFT JOIN\n       M_AITESAKI AS J_MAKER\n    ON J_MAKER.AITE_CD = J_SHOHIN.MAKER_CD\n WHERE J_SHOHIN.SHIYAKU_FLG   = \qTrue\q\n GROUP BY\n       TBL_A.KOBAIHIN_CD\n      ,TBL_A.KOBAIHIN_MEI\n      ,TBL_A.HOKAN_BASHO_KBN\n      ,TBL_A.HOKAN_BASHO_KBN_MEI\n      ,J_SHIIRESAKI.AITE_CD\n      ,J_SHIIRESAKI.AITE_MEI\n      ,J_MAKER.AITE_CD\n      ,J_MAKER.AITE_MEI\n      ,J_SHOHIN.SHIIRE_IRISU\n      ,J_SHOHIN.SHIIRE_IRISU_TANI\n      ,J_SHOHIN.BARA_TANI\n      ,J_SHOHIN.DOKU_FLG\n      ,TBL_A.FREEWD\nHAVING SUM(TBL_A.ZAIKO_SURYO) > 0\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="入出庫関連",2950,2050
Page=入出庫関連
Left=2950
Top=2050

[View]
PName=V_SHIIRE_KOBAI_DATA
LName=仕入接続対象購買データビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT\n        W_SHIIRE_KOBAI.W_USER_ID            AS W_USER_ID                --ユーザーID\n       ,W_SHIIRE_KOBAI.W_SERIAL             AS W_SERIAL                 --シリアル\n       ,NULL                                AS W_PAGE                   --ページ番号\n       ,ROW_NUMBER() OVER (PARTITION BY W_SHIIRE_KOBAI.SHIIRE_CD\n                                       ,W_SHIIRE_KOBAI.KENSHU_DATE\n                               ORDER BY W_SHIIRE_KOBAI.W_ROW )\n                                            AS W_ROW                    --行番号\n       ,0                                   AS W_MODE                   --モード\n       ,NULL                                AS SHIIRE_NO                --仕入No.\n       ,W_SHIIRE_KOBAI.KENSHU_DATE          AS SHIIRE_DATE              --仕入日\n       ,0                                   AS SHIIRE_KAMOKU            --仕入科目\n       ,0                                   AS DENKU                    --仕入伝区\n       ,W_SHIIRE_KOBAI.KOBAI_KBN_CD         AS KOBAI_KBN                --購買区分CD\n       ,NULL                                AS KANJO_KAMOKU             --勘定科目\n       ,NULL                                AS SHIIRE_NYURYOKUSHA_CD    --仕入入力者CD\n       ,NULL                                AS SHIHARAI_DATE            --支払日\n       ,2                                   AS SHIIRE_STS               --仕入ステータス\n       ,W_SHIIRE_KOBAI.SHIIRE_CD            AS SHIIRE_CD                --仕入先CD\n       ,T_KOBAI_H.TAX_RATE                  AS TAX_RATE                 --消費税率\n       ,T_KOBAI_H.NUKI_TOTAL                AS NUKI_TOTAL               --税抜合計\n       ,T_KOBAI_H.TAX                       AS TAX                      --消費税\n       ,T_KOBAI_H.KOMI_TOTAL                AS KOMI_TOTAL               --税込合計\n       ,NULL                                AS SHOKAN_NO                --商管伝票No.\n       ,NULL                                AS SHOKAN_LINK_DATETIME     --商管連動日時\n       ,ROW_NUMBER() OVER (PARTITION BY W_SHIIRE_KOBAI.SHIIRE_CD\n                               ORDER BY W_SHIIRE_KOBAI.W_ROW )\n                                            AS GYO_NO                   --行番号\n       ,W_SHIIRE_KOBAI.KOBAIHIN_CD          AS KOBAIHIN_CD              --購買品CD\n       ,W_SHIIRE_KOBAI.KOBAIHIN_MEI         AS KOBAIHIN_MEI             --購買品名\n       ,W_SHIIRE_KOBAI.TANKA                AS TANKA                    --単価\n       ,W_SHIIRE_KOBAI.IRISU                AS IRISU                    --入数\n       ,W_SHIIRE_KOBAI.IRISU_TANI           AS IRISU_TANI               --入数単位\n       ,W_SHIIRE_KOBAI.SURYO                AS SURYO                    --数量\n       ,W_SHIIRE_KOBAI.BARA_TANI            AS BARA_TANI                --バラ単位\n       ,W_SHIIRE_KOBAI.SOSU                 AS SOSU                     --総数\n       ,W_SHIIRE_KOBAI.TOTAL                AS TOTAL                    --合計金額\n       ,W_SHIIRE_KOBAI.MAKER_CD             AS MAKER_CD                 --メーカーCD\n       ,W_SHIIRE_KOBAI.YOSAN_CD             AS YOSAN_CD                 --予算コード\n       ,W_SHIIRE_KOBAI.JURI_NO              AS JURI_NO                  --受理No.\n       ,W_SHIIRE_KOBAI.BIKO                 AS BIKO                     --備考\n       ,W_SHIIRE_KOBAI.KOBAI_NO             AS KOBAI_NO                 --購買申請No.\n       ,W_SHIIRE_KOBAI.KOBAI_SEQ            AS KOBAI_SEQ                --購買申請SEQ\n       ,\qFalse\q                             AS DEL_FLG                  --削除フラグ\n\n       ,1                                   AS DBS_STATUS               --DBSステータス\n       ,W_SHIIRE_KOBAI.DBS_UPDATE_USER      AS DBS_CREATE_USER          --DBS作成ユーザー\n       ,W_SHIIRE_KOBAI.DBS_UPDATE_DATE      AS DBS_CREATE_DATE          --DBS作成日時\n       ,W_SHIIRE_KOBAI.DBS_UPDATE_USER      AS DBS_UPDATE_USER          --DBS更新ユーザー\n       ,W_SHIIRE_KOBAI.DBS_UPDATE_DATE      AS DBS_UPDATE_DATE          --DBS更新日時\n   FROM W_SHIIRE_KOBAI\n   LEFT JOIN\n        T_KOBAI_H\n     ON T_KOBAI_H.KOBAI_NO = W_SHIIRE_KOBAI.KOBAI_NO\n  WHERE W_SHIIRE_KOBAI.SELECT_FLG  = \qTrue\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="仕入関連",3500,100
Page=仕入関連
Left=3500
Top=100

[View]
PName=V_SHIIRE_KOBAI_LIST
LName=仕入計上用購買データ検索ビュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT \n       TBL_V.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_V.KOBAI_STS_MEI             AS  KOBAI_STS_MEI             -- 購買ステータス名\n      ,TBL_V.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,Convert(varchar(10), TBL_V.SHINSEI_DATETIME,111)\n                                       AS  SHINSEI_DATE              -- 申請日\n      ,TBL_V.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_V.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_V.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_V.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_V.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_V.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_V.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_V.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_V.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_V.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_V.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_V.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_V.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_V.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_V.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_V.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_V.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_V.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_V.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_V.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_V.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_V.CHUMON_NO                 AS  CHUMON_NO                 -- 注文No.\n      ,TBL_U.SHIIRE_NO_S               AS  SHIIRE_NO_S               -- 仕入No.\n      ,TBL_V.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_V.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_V.KOBAI_STS                 AS  KOBAI_STS                 -- 購買ステータス\n      ,TBL_V.YOSAN_MEI                 AS  YOSAN_MEI                 -- 予算名\n      ,TBL_V.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,(SELECT CASE COUNT(*)\n              WHEN 0\n              THEN \qTrue\q\n              ELSE \qFalse\q\n              END\n         FROM T_SHIIRE_B TBL_UH\n        WHERE TBL_UH.KOBAI_NO          =   TBL_V.KOBAI_NO\n          AND TBL_UH.KOBAI_SEQ         =   TBL_V.KOBAI_SEQ\n          AND TBL_UH.DEL_FLG           =   \qFalse\q\n      )                                AS  SHIIRE_MIKEIJO_FLG        -- 仕入未計上フラグ\n      ,TBL_V.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n  FROM V_KOBAI_LIST                    AS  TBL_V                     -- 【購買申請リストビュー】\n\n  LEFT JOIN                                                          -- 【売上No.用】\n     (\n       SELECT TBL_A.KOBAI_NO\n             ,TBL_A.KOBAI_SEQ\n             ,REPLACE(\n              (\n               SELECT TBL_B.SHIIRE_NO  AS  [data()]\n                 FROM T_SHIIRE_B       AS  TBL_B\n                WHERE TBL_B.KOBAI_NO   =   TBL_A.KOBAI_NO\n                  AND TBL_B.KOBAI_SEQ  =   TBL_A.KOBAI_SEQ\n                FOR XML PATH (\q\q)\n              ),\q \q,\q,\q)               AS  SHIIRE_NO_S\n         FROM T_SHIIRE_B               AS  TBL_A\n        WHERE TBL_A.KOBAI_NO           <>  \q\q\n          AND TBL_A.DEL_FLG            =   \qFalse\q\n        GROUP BY\n              TBL_A.KOBAI_NO\n             ,TBL_A.KOBAI_SEQ\n     ) TBL_U\n    ON TBL_U.KOBAI_NO                  =   TBL_V.KOBAI_NO\n   AND TBL_U.KOBAI_SEQ                 =   TBL_V.KOBAI_SEQ\n\n WHERE TBL_V.KOBAI_STS                 IN  (3,4,5,6)\n   AND TBL_V.DEL_FLG                   =   \qFalse\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="仕入関連",700,100
Page=仕入関連
Left=700
Top=100

