[View]
PName=V_KOBAI_LIST
LName=購買申請リストビュー
Tag=ビュー
Comment=購買申請関連ビューの元となるビュー
ViewOption=
Query=--*DataTitle V_KOBAI_LIST(購買申請リストビュー)\nSELECT \q＞\q                            AS  GYO_MOJI                  -- 選択\n      ,CASE TBL_C.NYUKA_HAKKO_FLG\n       WHEN \qTrue\q\n       THEN \q済\q\n       ELSE NULL\n       END                             AS  NYUKA_HAKKO_DISP          -- 入荷予定発行表示\n      ,CASE\n       WHEN ISNULL(TBL_B.KOBAIHIN_CD,\q\q)\n                                       =  \q\q\n       THEN NULL\n\n       WHEN TBL_B.SURYO                >  TBL_B.SHIKIICHI\n       THEN \q注文数要確認\q\n\n       WHEN TBL_H.SHINSEI_DATETIME     IS  NOT  NULL\n        AND ISNULL(TBL_B.NOKI,\q\q)      <>  \q\q\n        AND DATEDIFF(day,CONVERT(date,TBL_H.SHINSEI_DATETIME),CONVERT(date,TBL_B.NOKI))\n                                       <   TBL_B.HYOJUN_NOKI\n       THEN \q標準納期外\q\n\n       WHEN \n     ( SELECT COUNT(*)                 AS  CNT\n         FROM T_KOBAI_H                AS  TBL_X\n         LEFT JOIN\n              T_KOBAI_B                AS  TBL_Y\n           ON TBL_Y.KOBAI_NO           =   TBL_X.KOBAI_NO\n        WHERE TBL_X.KOBAI_NO           <>  TBL_H.KOBAI_NO\n          AND TBL_Y.KOBAIHIN_CD        =   TBL_B.KOBAIHIN_CD\n--          AND TBL_Y.KOBAI_STS          IN  ( \q3\q,\q4\q,\q5\q,\q6\q )\n          AND TBL_X.SHINSEI_DATETIME   IS  NOT  NULL\n          AND TBL_H.SHINSEI_DATETIME   IS  NOT  NULL\n          AND TBL_X.SHINSEI_DATETIME   <   TBL_H.SHINSEI_DATETIME\n          AND DATEDIFF(day,CONVERT(date,TBL_X.SHINSEI_DATETIME),CONVERT(date,TBL_H.SHINSEI_DATETIME))\n                                       <=  7\n     )                                 >   0\n       THEN \q直近注文済\q\n\n       ELSE NULL\n       END                             AS  WARNING                   -- 警告\n      ,TBL_H.KOBAI_KBN_MEI             AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_B.KOBAI_STS_MEI             AS  KOBAI_STS_MEI             -- 購買ステータス名\n      ,TBL_H.SHINSEISHA_MEI            AS  SHINSEISHA_MEI            -- 申請者名\n      ,TBL_H.SHINSEI_DATETIME          AS  SHINSEI_DATETIME          -- 申請日時\n      ,TBL_B.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_B.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,TBL_B.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_B.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_B.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_B.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_B.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_B.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_B.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_C.HATCHU_DATE               AS  HATCHU_DATE               -- 発注日\n      ,TBL_B.NOKI                      AS  NOKI                      -- 希望納期\n      ,TBL_N.UKEIRE_DATE               AS  UKEIRE_DATE               -- 受入日\n      ,TBL_N.KENSHU_DATE               AS  KENSHU_DATE               -- 検収日\n      ,TBL_B.HOKAN_BASHO_KBN_MEI       AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_B.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,TBL_B.SHIIRE_MEI                AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_B.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,TBL_B.MAKER_MEI                 AS  MAKER_MEI                 -- メーカー名\n      ,TBL_B.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,TBL_B.YOSAN_MEI                 AS  YOSAN_MEI                 -- 予算名\n      ,TBL_B.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_B.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_H.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_B.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_I.IRAI_NO                   AS  IRAI_NO                   -- 依頼No.\n      ,TBL_C.CHUMON_NO                 AS  CHUMON_NO                 -- 注文No.\n      ,CASE TBL_H.DEL_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                             AS  DEL_DISP                  -- 削除表示\n\n      ,TBL_C.NYUKA_HAKKO_FLG           AS  NYUKA_HAKKO_FLG           -- 入荷予定発行フラグ\n      ,TBL_H.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,TBL_B.KOBAI_STS                 AS  KOBAI_STS                 -- 購買ステータス\n      ,TBL_H.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,TBL_H.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,TBL_B.JOGAI_FLG                 AS  JOGAI_FLG                 -- 除外フラグ\n      ,TBL_H.FREEWD\n      +TBL_B.FREEWD                    AS  FREEWD                    -- フリーワード検索用\n  FROM V_C_KOBAI_H                     AS  TBL_H                     -- 【購買申請ヘッダ共通ビュー】\n  LEFT JOIN                                                          -- 【購買申請ボディ共通ビュー】\n       V_C_KOBAI_B                     AS  TBL_B\n    ON TBL_B.KOBAI_NO                  =   TBL_H.KOBAI_NO\n  LEFT JOIN                                                          -- 【依頼テーブル】\n       T_IRAI                          AS  TBL_I\n    ON TBL_I.KOBAI_NO                  =   TBL_B.KOBAI_NO\n   AND TBL_I.KOBAI_SEQ                 =   TBL_B.KOBAI_SEQ\n  LEFT JOIN                                                          -- 【注文テーブル】\n       T_CHUMON                        AS  TBL_C\n    ON TBL_C.KOBAI_NO                  =   TBL_B.KOBAI_NO\n   AND TBL_C.KOBAI_SEQ                 =   TBL_B.KOBAI_SEQ\n  LEFT JOIN                                                          -- 【入出庫テーブル】（依頼No.ごとに入出庫No.が最大のレコード）\n     ( SELECT TBL_1.IRAI_NO                         -- 依頼No.\n             ,TBL_1.UKEIRE_DATE                     -- 受入日\n             ,TBL_1.KENSHU_DATE                     -- 検収日\n         FROM T_NYUSHUKKO              AS  TBL_1\n        WHERE TBL_1.NYUSHUKKO_NO       =\n                ( SELECT MAX(TBL_2.NYUSHUKKO_NO)\n                    FROM T_NYUSHUKKO   AS  TBL_2\n                   WHERE TBL_2.IRAI_NO       =  TBL_1.IRAI_NO\n                     AND TBL_2.NYUSHUKKO_KBN =  1\n                )\n     )                                 AS  TBL_N\n    ON TBL_N.IRAI_NO                   =   TBL_I.IRAI_NO\n\n
LNameByComment=1
Color=$000000
BkColor=$99FFFF
Position="購買関連",750,100
Page=購買関連
Left=750
Top=100

[View]
PName=V_C_KOBAI_B
LName=購買申請ボディ共通ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_C_KOBAI_B(購買申請ボディ共通ビュー)\nSELECT TBL_A.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_A.KOBAI_SEQ                 AS  KOBAI_SEQ                 -- 購買申請SEQ\n      ,TBL_A.KOBAI_GYO_NO              AS  KOBAI_GYO_NO              -- 購買申請行No.\n      ,TBL_A.KOBAI_STS                 AS  KOBAI_STS                 -- 購買ステータス\n      ,M_KOBAI_STS.DATA_1              AS  KOBAI_STS_MEI             -- 購買ステータス名\n      ,TBL_A.JOGAI_FLG                 AS  JOGAI_FLG                 -- 除外フラグ\n      ,TBL_A.KOBAIHIN_CD               AS  KOBAIHIN_CD               -- 購買品CD\n      ,TBL_A.KOBAIHIN_MEI              AS  KOBAIHIN_MEI              -- 購買品名\n      ,M_KOBAIHIN_CD.HYOJUN_NOKI       AS  HYOJUN_NOKI               -- 標準納期\n      ,M_KOBAIHIN_CD.SHIKIICHI         AS  SHIKIICHI                 -- 購買閾値\n      ,M_KOBAIHIN_CD.SHOHIN_BUNRUI     AS  SHOHIN_BUNRUI_CD          -- 商品分類CD\n      ,M_KOBAIHIN_KBN.DATA_2           AS  KOBAIHIN_KBN              -- 購買品区分\n      ,M_KOBAIHIN_CD.SHIYAKU_FLG       AS  SHIYAKU_FLG               -- 試薬フラグ\n      ,( SELECT DISTINCT\n                CONVERT(VARCHAR(10), MAX(T_KOBAI_H.SHINSEI_DATETIME), 111)\n           FROM T_KOBAI_H\n           LEFT JOIN\n                T_KOBAI_B\n             ON T_KOBAI_B.KOBAI_NO    = T_KOBAI_H.KOBAI_NO\n          WHERE T_KOBAI_B.KOBAIHIN_CD = TBL_A.KOBAIHIN_CD\n            AND T_KOBAI_H.KOBAI_NO    < TBL_A.KOBAI_NO\n       )                               AS  LAST_SHINSEI_DATE         -- 最終申請日\n      ,TBL_A.TANKA                     AS  TANKA                     -- 単価\n      ,TBL_A.IRISU                     AS  IRISU                     -- 入数\n      ,TBL_A.IRISU_TANI                AS  IRISU_TANI                -- 入数単位\n      ,TBL_A.SURYO                     AS  SURYO                     -- 数量\n      ,TBL_A.SOSU                      AS  SOSU                      -- 注文総数\n      ,TBL_A.BARA_TANI                 AS  BARA_TANI                 -- バラ単位\n      ,TBL_A.TOTAL                     AS  TOTAL                     -- 合計金額\n      ,TBL_A.NOKI                      AS  NOKI                      -- 希望納期\n      ,TBL_A.HOKAN_BASHO_KBN           AS  HOKAN_BASHO_KBN           -- 保管場所区分\n      ,M_HOKAN_BASHO_KBN.DATA_1        AS  HOKAN_BASHO_KBN_MEI       -- 保管場所区分名\n      ,TBL_A.SHIIRE_CD                 AS  SHIIRE_CD                 -- 仕入先CD\n      ,M_SHIIRE_CD.AITE_MEI            AS  SHIIRE_MEI                -- 仕入先名\n      ,TBL_A.MAKER_CD                  AS  MAKER_CD                  -- メーカーCD\n      ,M_MAKER_CD.AITE_MEI             AS  MAKER_MEI                 -- メーカー名\n      ,TBL_A.YOSAN_CD                  AS  YOSAN_CD                  -- 予算コード\n      ,M_YOSAN_CD.YOSAN_MEI            AS  YOSAN_MEI                 -- 予算名\n      ,TBL_A.JURI_NO                   AS  JURI_NO                   -- 受理No.\n      ,TBL_A.BIKO                      AS  BIKO                      -- 備考\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,  \q|\q + ISNULL(TBL_A.KOBAIHIN_CD ,\q\q)       -- 購買品CD\n       + \q|\q + ISNULL(TBL_A.KOBAIHIN_MEI,\q\q)       -- 購買品名\n       + \q|\q + ISNULL(TBL_A.BIKO        ,\q\q)       -- 備考\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_KOBAI_B                       AS  TBL_A                     -- 【購買申請ボディテーブル】\n  LEFT JOIN                                                          -- 【購買ステータス用】\n       M_MEISHO_B                      AS  M_KOBAI_STS\n    ON M_KOBAI_STS.MEISHO_KBN          =   \qS005\q\n   AND M_KOBAI_STS.MEISHO_CD           =   TBL_A.KOBAI_STS\n  LEFT JOIN                                                          -- 【購買品CD用】\n       M_SHOHIN                        AS  M_KOBAIHIN_CD\n    ON M_KOBAIHIN_CD.SHOHIN_CD         =   TBL_A.KOBAIHIN_CD\n  LEFT JOIN                                                          -- 【保管場所区分用】\n       M_MEISHO_B                      AS  M_HOKAN_BASHO_KBN\n    ON M_HOKAN_BASHO_KBN.MEISHO_KBN    =   \qM002\q\n   AND M_HOKAN_BASHO_KBN.MEISHO_CD     =   TBL_A.HOKAN_BASHO_KBN\n  LEFT JOIN                                                          -- 【仕入先CD用】\n       M_AITESAKI                      AS  M_SHIIRE_CD\n    ON M_SHIIRE_CD.AITE_CD             =   TBL_A.SHIIRE_CD\n  LEFT JOIN                                                          -- 【メーカーCD用】\n       M_AITESAKI                      AS  M_MAKER_CD\n    ON M_MAKER_CD.AITE_CD              =   TBL_A.MAKER_CD\n  LEFT JOIN                                                          -- 【予算コード用】\n       M_YOSAN                         AS  M_YOSAN_CD\n    ON M_YOSAN_CD.YOSAN_CD             =   TBL_A.YOSAN_CD\n  LEFT JOIN\n       M_MEISHO_B                      AS  M_KOBAIHIN_KBN            -- 【購買品区分用】\n    ON M_KOBAIHIN_KBN.MEISHO_KBN       =   \qK025\q\n   AND M_KOBAIHIN_KBN.MEISHO_CD        =   M_KOBAIHIN_CD.SHOHIN_BUNRUI\n\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="購買関連",100,650
Page=購買関連
Left=100
Top=650

