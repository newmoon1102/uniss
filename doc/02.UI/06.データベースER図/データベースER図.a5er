Query=SELECT\n       M_AITESAKI.AITE_CD            AS  AITE_CD             --相手先CD\n      ,M_AITESAKI.AITE_MEI           AS  AITE_MEI            --相手先名\n      ,M_AITESAKI.YUBIN_NO           AS  YUBIN_NO            --郵便番号\n      ,M_AITESAKI.ADDRESS_1          AS  ADDRESS_1           --住所1\n      ,M_AITESAKI.ADDRESS_2          AS  ADDRESS_2           --住所2\n      ,M_AITESAKI.TEL                AS  TEL                 --TEL\n      ,M_AITESAKI.FAX                AS  FAX                 --FAX\n      ,M_AITESAKI.MAIL               AS  MAIL                --E-MAIL\n      ,M_AITESAKI.BUSHO_MEI          AS  BUSHO_MEI           --部署名\n      ,M_AITESAKI.TANTO_MEI          AS  TANTO_MEI           --担当者名\n      ,M_AITESAKI.KEISHO             AS  KEISHO              --敬称\n      ,M_AITESAKI.BIKO               AS  BIKO                --備考\n--取引区分\n      ,M_AITESAKI.SEIKYU_FLG         AS  SEIKYU_FLG          --請求フラグ\n      ,M_AITESAKI.SHIRE_FLG          AS  SHIIRE_FLG           --仕入フラグ\n      ,M_AITESAKI.HASSO_FLG          AS  HASSO_FLG           --発送フラグ\n      ,M_AITESAKI.HOKOKU_FLG         AS  HOKOKU_FLG          --報告フラグ\n      ,M_AITESAKI.MAKER_FLG          AS  MAKER_FLG           --メーカフラグ\n      ,M_AITESAKI.GAICHU_FLG         AS  GAICHU_FLG          --外注フラグ\n      ,M_AITESAKI.KANKO_FLG          AS  KANKO_FLG           --官公庁フラグ\n      ,M_AITESAKI.DOGYO_FLG          AS  DOGYO_FLG           --同業者フラグ\n      ,M_AITESAKI.SPOT_FLG           AS  SPOT_FLG            --スポットフラグ\n--請求先\n      ,EIGYO.SHIMEI                  AS  EIGYO_TANTO_MEI     --営業担当者名\n      ,SEIKYU_BUNRUI.DATA_1          AS  SEIKYU_BUNRUI       --請求分類\n      ,M_AITESAKI.SEIKYU_CHUKI       AS  SEIKYU_CHUKI        --請求注記\n      ,SEIKYU_HOHO.DATA_1            AS  SEIKYU_HOHO         --請求方法\n      ,KAISHU_SHUBETSU.DATA_1        AS  KAISHU_SHUBETSU     --回収種別\n      ,M_AITESAKI.KAISHU_MONTH       AS  KAISHU_MONTH        --回収月\n      ,M_AITESAKI.KAISHU_DAY         AS  KAISHU_DAY          --回収日\n      ,TAX_BUNRUI.DATA_1             AS  TAX_BUNRUI          --消費税計算区分\n      ,M_AITESAKI.SEIKYUSHO_FLG      AS  SEIKYUSHO_FLG       --請求書発行フラグ\n      ,M_AITESAKI.JURYOSHO_FLG       AS  JURYOSHO_FLG        --受領書発行フラグ\n      ,M_AITESAKI.NOHINSHO_FLG       AS  NOHINSHO_FLG        --納品書発行フラグ\n      ,M_AITESAKI.HIKAE_FLG          AS  HIKAE_FLG           --控発行フラグ\n      ,HASU_SHORI.DATA_1             AS  HASU_SHORI          --請求先端数処理区分\n      ,TESURYO_SHORI.DATA_1          AS  TESURYO_SHORI       --請求先手数料処理区分\n--仕入先\n      ,SHIHARAI_HOHO.DATA_1          AS  SHIHARAI_HOHO       --支払方法区分\n      ,M_AITESAKI.SHIIRE_CHUKI       AS  SHIIRE_CHUKI        --仕入先注記\n      ,SHIIRE_HASU_SHORI.DATA_1      AS  SHIIRE_HASU_SHORI   --仕入先端数処理区分\n      ,SHIIRE_TESURYO.DATA_1         AS  SHIIRE_TESURYO      --仕入先手数料負担区分\n--その他\n      ,M_AITESAKI.EIGYO_TANTO_CD     AS  EIGYO_TANTO_CD      --営業担当CD\n      ,M_AITESAKI.SEIKYU_BUNRUI      AS  SEIKYUSAKI_BUNRUI_CD    --請求分類CD\n      ,M_AITESAKI.LINK_DATETIME      AS  LINK_DATETIME       --連動日時\n      ,M_AITESAKI.MISHIYO_FLG        AS  MISHIYO_FLG         --未使用フラグ\n      ,M_AITESAKI.DBS_CREATE_DATE    AS  DBS_CREATE_DATE     --登録日時\n      ,M_AITESAKI.DBS_UPDATE_DATE    AS  DBS_UPDATE_DATE     --更新日時\n  FROM\n--営業担当者名\n       M_AITESAKI\n  LEFT JOIN\n       M_SHAIN                       AS  EIGYO\n    ON M_AITESAKI.EIGYO_TANTO_CD = EIGYO.SHAIN_CD\n--請求方法\n  LEFT JOIN\n       M_MEISHO_B                    AS  SEIKYU_HOHO\n    ON SEIKYU_HOHO.MEISHO_KBN = \qK002\q\n   AND SEIKYU_HOHO.MEISHO_CD = M_AITESAKI.SEIKYUHOHO_KBN\n--回収種別\n  LEFT JOIN\n       M_MEISHO_B                    AS  KAISHU_SHUBETSU\n    ON KAISHU_SHUBETSU.MEISHO_KBN = \qK003\q\n   AND KAISHU_SHUBETSU.MEISHO_CD = M_AITESAKI.KAISHU_KBN\n  LEFT JOIN\n       M_MEISHO_B                    AS  SEIKYU_BUNRUI\n    ON SEIKYU_BUNRUI.MEISHO_KBN = \qK001\q\n   AND SEIKYU_BUNRUI.MEISHO_CD = M_AITESAKI.SEIKYU_BUNRUI\n--消費税計算区分\n  LEFT JOIN\n       M_MEISHO_B                    AS  TAX_BUNRUI\n    ON TAX_BUNRUI.MEISHO_KBN = \qK004\q\n   AND TAX_BUNRUI.MEISHO_CD = M_AITESAKI.TAX_KBN\n--請求先端数処理区分\n  LEFT JOIN\n       M_MEISHO_B                    AS  HASU_SHORI\n    ON HASU_SHORI.MEISHO_KBN = \qK005\q\n   AND HASU_SHORI.MEISHO_CD = M_AITESAKI.SEIKYU_HASU_KBN\n--請求先手数料処理区分\n  LEFT JOIN\n       M_MEISHO_B                    AS  TESURYO_SHORI\n    ON TESURYO_SHORI.MEISHO_KBN = \qK024\q\n   AND TESURYO_SHORI.MEISHO_CD = M_AITESAKI.SEIKYU_TESURYO_KBN\n--仕入先支払方法区分\n  LEFT JOIN\n       M_MEISHO_B                    AS  SHIHARAI_HOHO\n    ON SHIHARAI_HOHO.MEISHO_KBN = \qK006\q\n   AND SHIHARAI_HOHO.MEISHO_CD = M_AITESAKI.SHIHARAIHOHO_KBN\n--仕入先端数処理区分\n  LEFT JOIN\n       M_MEISHO_B                    AS  SHIIRE_HASU_SHORI\n    ON SHIIRE_HASU_SHORI.MEISHO_KBN = \qK005\q\n   AND SHIIRE_HASU_SHORI.MEISHO_CD = M_AITESAKI.SHIIRE_HASU_KBN\n--仕入先手数料負担区分\n  LEFT JOIN\n       M_MEISHO_B                    AS  SHIIRE_TESURYO\n    ON SHIIRE_TESURYO.MEISHO_KBN = \qK024\q\n   AND SHIIRE_TESURYO.MEISHO_CD = M_AITESAKI.SHIIRE_TESURYO_KBN
