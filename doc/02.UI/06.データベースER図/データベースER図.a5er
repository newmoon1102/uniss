[View]
PName=V_SHOHIN_DATA_SANSHO
LName=商品マスタデータビュー参照作成用
Tag=ビュー
Comment=
ViewOption=
Query=SELECT M_SHOHIN.BUNSEKI_FLG         AS BUNSEKI_FLG          --分析項目フラグ\n      ,M_SHOHIN.SHOHIN_FLG          AS HAMBAI_SHOHIN_FLG    --販売商品フラグ\n      ,M_SHOHIN.KOBAIHIN_FLG        AS KOBAIHIN_FLG         --購買品フラグ\n      ,M_SHOHIN.SHOKEIHI_FLG        AS SHOKEIHI_FLG         --諸経費フラグ\n      ,J_SHOHIN_STATUS.DATA_1       AS SHOHIN_STS           --商品ステータス\n      ,J_SHOHIN_BUNRUI.DATA_1       AS BUNRUI               --商品分類\n      ,M_SHOHIN.SHOHIN_CD           AS SHOHIN_CD            --商品CD\n      ,M_SHOHIN.SHOHIN_MEI          AS SHOHIN_MEI           --商品名\n      ,M_SHOHIN.AITE_SHOHIN_MEI     AS AITESAKI_SHOHIN_MEI  --相手先商品名\n      ,M_SHOHIN.HIMBAN              AS HIMBAN               --品番\n      ,M_SHOHIN.SHIYAKU_MEI         AS SHIYAKU_MEI          --調製試薬名\n      ,M_SHOHIN.SHIYAKU_NAIYO       AS SHIYAKU_NAIYO        --調製試薬内容\n      ,M_SHOHIN.DOKU_MEISHO         AS DOKU_MEISHO          --毒劇物名称\n      ,M_SHOHIN.DOKU_SURYO          AS DOKU_SURYO           --毒劇物数量\n      ,M_SHOHIN.CHOSEI_SHIYAKU_FLG  AS CHOSEI_SHIYAKU_FLG   --調整試薬フラグ\n      ,M_SHOHIN.DOKU_FLG            AS DOKU_FLG             --毒劇物フラグ\n      ,M_SHOHIN.SHIYAKU_FLG         AS SHIYAKU_FLG          --試薬フラグ\n      ,J_SHIIRESAKI.AITE_CD         AS SHIIRE_CD            --仕入先CD\n      ,J_SHIIRESAKI.AITE_MEI        AS SHIIRESAKI_MEI       --仕入先名\n      ,J_MAKER.AITE_CD              AS MAKER_CD             --メーカーCD\n      ,J_MAKER.AITE_MEI             AS MAKER_MEI            --メーカー名\n      ,J_GAICHU.AITE_CD             AS GAICHU_CD            --外注先CD\n      ,J_GAICHU.AITE_MEI            AS GAICHU_MEI           --外注先名\n\n-- <商品・購買品・諸経費情報>\n      ,M_SHOHIN.IRISU               AS IRISU                --入数\n      ,M_SHOHIN.IRISU_TANI          AS IRISU_TANI           --入数単位\n      ,M_SHOHIN.BARA_TANI           AS BARA_TANI            --バラ単位\n      ,M_SHOHIN.HYOJUN_KAKAKU       AS HYOJUN_KAKAKU        --標準価格\n      ,M_SHOHIN.GENKA               AS GENKA                --原価\n      ,M_SHOHIN.BAIKA               AS BAIKA                --売価\n      ,M_SHOHIN.SHIIRE_TANKA        AS SHIIRE_TANKA         --仕入単価\n      ,M_SHOHIN.TEIKA               AS TEIKA                --定価\n      ,M_SHOHIN.SAITEI_KAKAKU       AS SAITEI_KAKAKU        --最低価格\n      ,M_SHOHIN.SHIJO_KAKAKU        AS SHIJO_KAKAKU         --市場価格\n      ,M_SHOHIN.SHIIRE_IRISU        AS SHIIRE_IRISU         --仕入入数\n      ,M_SHOHIN.SHIIRE_IRISU_TANI   AS SHIIRE_IRISU_TANI    --仕入入数単位\n      ,M_SHOHIN.HYOJUN_NOKI         AS HYOJUN_NOKI          --標準納期\n      ,M_SHOHIN.HYOJUN_KOSU         AS HYOJUN_KOSU          --標準工数\n      ,M_SHOHIN.SHIKIICHI           AS SHIKIICHI            --購買閾値\n      ,J_HOKAN_BASHO.DATA_1           AS HOKAN_BASHO        --保管場所\n      \n-- <分析項目情報>\n      ,M_SHOHIN.BUNSEKI_TANI        AS BUNSEKI_TANI         --分析単位\n      ,M_SHOHIN.BUNSEKI_NO          AS BUNSEKI_NO           --分析番号\n      ,M_SHOHIN.BUNSEKI_FLOW        AS BUNSEKI_FLOW         --分析フロー\n      ,M_SHOHIN.BIKO                AS BUNSEKI_BIKO         --備考\n      \n-- <その他>\n      ,\qFalse\q                      AS MISHIYO_FLG          --未使用フラグ\n      ,NULL                         AS CREATE_DATE          --登録日時\n      ,NULL                         AS UPDATE_DATE          --更新日時\n\n-- <非表示データ>\n      ,M_SHOHIN.SHOHIN_BUNRUI       AS SHOHIN_BUNRUI_KBN    --商品分類区分\n      ,M_SHOHIN.HOKANBASHO_KBN      AS HOKANBASHO_KBN       --保管場所区分\n      ,NULL                         AS LINK_DATETIME        --連動日時\n      ,M_SHOHIN.DBS_CREATE_USER     AS DBS_CREATE_USER      --作成者\n      ,M_SHOHIN.DBS_UPDATE_USER     AS DBS_UPDATE_USER      --更新者\n      ,M_SHOHIN.DBS_STATUS          AS DBS_STATUS           --DBSステータス\n\n\n  FROM M_SHOHIN \n  LEFT JOIN\n       M_MEISHO_B                   AS J_SHOHIN_STATUS        --商品ステータス用名称マスタ\n    ON J_SHOHIN_STATUS.MEISHO_KBN   =  \qS001\q\n   AND M_SHOHIN.SHOHIN_STS          =  J_SHOHIN_STATUS.MEISHO_CD\n  LEFT JOIN\n       M_MEISHO_B                   AS J_SHOHIN_BUNRUI        --商品分類用名称マスタ\n    ON J_SHOHIN_BUNRUI.MEISHO_KBN   =  \qK025\q\n   AND M_SHOHIN.SHOHIN_BUNRUI       =  J_SHOHIN_BUNRUI.MEISHO_CD\n  LEFT JOIN \n       M_AITESAKI                   AS J_SHIIRESAKI           --仕入先用相手先マスタ\n    ON M_SHOHIN.SHIIRE_CD           =  J_SHIIRESAKI.AITE_CD\n   AND J_SHIIRESAKI.MISHIYO_FLG     =  \qFalse\q\n  LEFT JOIN\n       M_AITESAKI                   AS J_MAKER                --メーカー用相手先マスタ\n    ON M_SHOHIN.MAKER_CD            =  J_MAKER.AITE_CD\n   AND J_MAKER.MISHIYO_FLG          =  \qFalse\q\n  LEFT JOIN\n       M_AITESAKI                   AS J_GAICHU               --外注先用相手先マスタ\n    ON M_SHOHIN.GAICHU_CD           =  J_GAICHU.AITE_CD\n   AND J_GAICHU.MISHIYO_FLG         =  \qFalse\q\n   \n--商品・購買品・諸経費情報\n  LEFT JOIN\n       M_MEISHO_B                   AS J_HOKAN_BASHO          --保管場所用名称マスタ\n    ON J_HOKAN_BASHO.MEISHO_KBN = \qM002\q\n   AND M_SHOHIN.HOKANBASHO_KBN  = J_HOKAN_BASHO.MEISHO_CD\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="マスタ関連(VIEW)",2000,700
Page=マスタ関連(VIEW)
Left=2000
Top=700

