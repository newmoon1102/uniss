[View]
PName=V_C_KOBAI_H
LName=購買申請ヘッダ共通ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_C_KOBAI_H(購買申請ヘッダ共通ビュー)\nSELECT TBL_A.KOBAI_NO                  AS  KOBAI_NO                  -- 購買申請No.\n      ,TBL_A.SHINSEISHA_CD             AS  SHINSEISHA_CD             -- 申請者CD\n      ,M_SHINSEISHA_CD.SHIMEI          AS  SHINSEISHA_MEI            -- 申請者名\n      ,TBL_A.SHINSEI_DATETIME          AS  SHINSEI_DATETIME          -- 申請日時\n      ,TBL_A.SHINSEI_GAIYO             AS  SHINSEI_GAIYO             -- 申請概要\n      ,TBL_A.KOBAI_KBN                 AS  KOBAI_KBN                 -- 購買区分\n      ,M_KOBAI_KBN.DATA_1              AS  KOBAI_KBN_MEI             -- 購買区分名\n      ,TBL_A.TAX_RATE                  AS  TAX_RATE                  -- 税率\n      ,TBL_A.NUKI_TOTAL                AS  NUKI_TOTAL                -- 税抜合計\n      ,TBL_A.TAX                       AS  TAX                       -- 消費税\n      ,TBL_A.KOMI_TOTAL                AS  KOMI_TOTAL                -- 税込合計\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,  \q|\q + ISNULL(TBL_A.SHINSEI_GAIYO,\q\q)      -- 申請概要\n                                       AS  FREEWD                    -- フリーワード検索用\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_KOBAI_H                       AS  TBL_A                     -- 【購買申請ヘッダテーブル】\n  LEFT JOIN                                                          -- 【申請者CD用】\n       M_SHAIN                         AS  M_SHINSEISHA_CD\n    ON M_SHINSEISHA_CD.SHAIN_CD        =   TBL_A.SHINSEISHA_CD\n  LEFT JOIN                                                          -- 【購買区分用】\n       M_MEISHO_B                      AS  M_KOBAI_KBN\n    ON M_KOBAI_KBN.MEISHO_KBN          =   \qK021\q\n   AND M_KOBAI_KBN.MEISHO_CD           =   TBL_A.KOBAI_KBN\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="購買関連",100,100
Page=購買関連
Left=100
Top=100

