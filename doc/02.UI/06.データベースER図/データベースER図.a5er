Query=--*DataTitle V_C_URIAGE_H(売上ヘッダ共通ビュー)\nSELECT TBL_A.URIAGE_NO                 AS  URIAGE_NO                 -- 売上No.\n      ,TBL_A.DEMPYO_KBN                AS  DEMPYO_KBN                -- 伝票区分\n      ,M_DEMPYO_KBN.DATA_1             AS  DEMPYO_KBN_MEI            -- 伝票区分名\n      ,TBL_A.RENDO_KBN                 AS  RENDO_KBN                 -- 商魂連動区分\n      ,M_RENDO_KBN.DATA_1              AS  RENDO_KBN_MEI             -- 商魂連動区分名\n      ,TBL_A.DENKU                     AS  DENKU_KBN                 -- 売上伝区\n      ,M_DENKU_KBN.DATA_1              AS  DENKU_KBN_MEI             -- 売上伝区名\n      ,TBL_A.KANJO_KAMOKU              AS  KANJO_KAMOKU              -- 勘定科目\n      ,M_KANJO_KBN.DATA_1              AS  KANJO_KAMOKU_MEI          -- 勘定科目名\n      ,TBL_A.URIAGE_DATE               AS  URIAGE_DATE               -- 売上日\n      ,TBL_A.BUSHO_CD                  AS  BUSHO_CD                  -- 担当部署CD\n      ,M_BUSHO_CD.DATA_1               AS  BUSHO_MEI                 -- 担当部署名\n      ,TBL_A.URIAGE_NYURYOKUSHA_CD     AS  URIAGE_NYURYOKUSHA_CD     -- 売上入力者CD\n      ,M_URI_NYU.SHIMEI                AS  URI_NYU_MEI               -- 売上入力者名\n      ,TBL_A.EIGYO_TANTO_CD            AS  EIGYO_TANTO_CD            -- 営業担当者CD\n      ,M_EIGYO_TANTO.SHIMEI            AS  EIGYO_TANTO_MEI           -- 営業担当者名\n      ,TBL_A.NYURYOKU_DATE_TIME        AS  NYURYOKU_DATE_TIME        -- 入力日時\n      ,TBL_A.JUCHU_KBN                 AS  JUCHU_KBN                 -- 受注区分\n      ,M_JUCHU_KBN.DATA_1              AS  JUCHU_KBN_MEI             -- 受注区分名\n      ,TBL_A.JURI_NO                   AS  JURI_NO                   -- 対象受理No.\n--       ,CASE WHEN\n--             ( SELECT COUNT(*)\n--                 FROM T_URIAGE_JUCHU_SELECT AS TBL_W\n--                WHERE TBL_W.URIAGE_NO       =  TBL_A.URIAGE_NO\n--             ) >= 1\n--        THEN\n--             ( SELECT IIF( ISNULL(T_URIAGE_JUCHU_SELECT.JURI_NO,\q\q)=\q\q,\q\q, T_URIAGE_JUCHU_SELECT.JURI_NO + \q,\q)\n--                 FROM T_URIAGE_JUCHU_SELECT\n--                WHERE T_URIAGE_JUCHU_SELECT.URIAGE_NO = TBL_A.URIAGE_NO\n--                  FOR XML PATH (\q\q)\n--             )\n--        ELSE TBL_A.JURI_NO\n--        END                             AS  JURI_NO                   -- 対象受理No.\n      ,TBL_A.MITSU_NO                  AS  MITSU_NO                  -- 対象見積No.\n      ,TBL_A.SAKI_URIAGE_NO            AS  SAKI_URIAGE_NO            -- 結合先売上No.\n      ,TBL_A.NOHIN_DATE                AS  NOHIN_DATE                -- 納品日\n      ,TBL_A.URIAGE_STS                AS  URIAGE_STS                -- 売上ステータス\n      ,M_URIAGE_STS.DATA_1             AS  URIAGE_STS_MEI            -- 売上ステータス名\n      ,TBL_A.DOJI_FLG                  AS  DOJI_FLG                  -- 同時発行フラグ\n      ,TBL_A.SEIKYU_CD                 AS  SEIKYU_CD                 -- 請求先CD\n      ,M_SEIKYU.AITE_MEI               AS  SEIKYU_MEI                -- 請求先名\n      ,TBL_A.KAISHU_DATE               AS  KAISHU_DATE               -- 回収予定日\n      ,TBL_A.BUNSEKI_HOHO_CD           AS  BUNSEKI_HOHO_CD           -- 分析方法CD\n      ,M_BUN_HOHO.DATA_1               AS  BUNSEKI_HOHO              -- 分析方法名\n      ,TBL_A.TEKIYO                    AS  TEKIYO                    -- 摘要\n      ,TBL_A.DEMPYO_SOFU_MEI           AS  DEMPYO_SOFU_MEI           -- 伝票送付先名\n      ,TBL_A.KEISHO                    AS  KEISHO                    -- 敬称\n      ,TBL_A.YUBIN_NO                  AS  YUBIN_NO                  -- 送付先郵便番号\n      ,TBL_A.ADDRESS_1                 AS  ADDRESS_1                 -- 送付先住所１\n      ,TBL_A.ADDRESS_2                 AS  ADDRESS_2                 -- 送付先住所２\n      ,TBL_A.TEL                       AS  TEL                       -- 送付先TEL\n      ,TBL_A.FAX                       AS  FAX                       -- 送付先FAX\n      ,TBL_A.SOFU_TANTO                AS  SOFU_TANTO                -- 送付先担当者\n      ,TBL_A.TEISEI_BIKO               AS  TEISEI_BIKO               -- 訂正備考\n      ,TBL_A.JUSHO_NO_PRINT_FLG        AS  JUSHO_NO_PRINT_FLG        -- 住所印字不要フラグ\n      ,TBL_A.TAX_RATE                  AS  TAX_RATE                  -- 税率\n      ,TBL_A.NUKI_TOTAL                AS  NUKI_TOTAL                -- 税抜合計\n      ,TBL_A.TAX                       AS  TAX                       -- 消費税\n      ,TBL_A.KOMI_TOTAL                AS  KOMI_TOTAL                -- 税込合計\n      ,TBL_A.GENKA_TOTAL               AS  GENKA_TOTAL               -- 原価合計\n      ,TBL_A.ARARIEKI                  AS  ARARIEKI                  -- 粗利益\n      ,TBL_A.ARARIRITSU                AS  ARARIRITSU                -- 粗利率\n      ,TBL_A.SHOKON_NO                 AS  SHOKON_NO                 -- 商魂伝票No.\n      ,TBL_A.SHOKON_LIST_DATE_TIME     AS  SHOKON_LIST_DATE_TIME     -- 連動日時\n      ,TBL_A.DEL_FLG                   AS  DEL_FLG                   -- 削除フラグ\n      ,  \q|\q + TBL_A.TEKIYO                -- 摘要(ヘッダ)\n       + \q|\q + TBL_A.DEMPYO_SOFU_MEI       -- 伝票送付先名\n       + \q|\q + TBL_A.SOFU_TANTO            -- 送付先担当者\n       + \q|\q + TBL_A.TEISEI_BIKO           -- 訂正備考\n                                       AS  FREEWD1                   -- フリーワード検索用１\n      ,  \q|\q + TBL_A.TEKIYO                -- 摘要(ヘッダ)\n       + \q|\q + TBL_A.DEMPYO_SOFU_MEI       -- 伝票送付先名\n       + \q|\q + TBL_A.SOFU_TANTO            -- 送付先担当者\n       + \q|\q + TBL_A.TEISEI_BIKO           -- 訂正備考\n       + \q|\q + TBL_F.FREEWD                -- フリーワード(ボディ)\n                                       AS  FREEWD2                   -- フリーワード検索用２\n      ,TBL_A.DBS_STATUS                AS  DBS_STATUS                -- レコード状態\n      ,TBL_A.DBS_CREATE_USER           AS  DBS_CREATE_USER           -- 作成ユーザＩＤ\n      ,TBL_A.DBS_CREATE_DATE           AS  DBS_CREATE_DATE           -- 作成日時\n      ,TBL_A.DBS_UPDATE_USER           AS  DBS_UPDATE_USER           -- 更新ユーザＩＤ\n      ,TBL_A.DBS_UPDATE_DATE           AS  DBS_UPDATE_DATE           -- 更新日時\n  FROM T_URIAGE_H                      AS  TBL_A                     -- 【売上ヘッダテーブル】\n  LEFT JOIN                                                          -- 【伝票区分用】\n       M_MEISHO_B                      AS  M_DEMPYO_KBN\n    ON M_DEMPYO_KBN.MEISHO_KBN         =   \qK023\q\n   AND M_DEMPYO_KBN.MEISHO_CD          =   TBL_A.DEMPYO_KBN\n  LEFT JOIN                                                          -- 【商魂連動区分用】\n       M_MEISHO_B                      AS  M_RENDO_KBN\n    ON M_RENDO_KBN.MEISHO_KBN          =   \qK029\q\n   AND M_RENDO_KBN.MEISHO_CD           =   TBL_A.RENDO_KBN\n  LEFT JOIN                                                          -- 【売上伝区用】\n       M_MEISHO_B                      AS  M_DENKU_KBN\n    ON M_DENKU_KBN.MEISHO_KBN          =   \qK032\q\n   AND M_DENKU_KBN.MEISHO_CD           =   TBL_A.DENKU\n  LEFT JOIN                                                          -- 【勘定科目用】\n       M_MEISHO_B                      AS  M_KANJO_KBN\n    ON M_KANJO_KBN.MEISHO_KBN          =   \qK033\q\n   AND M_KANJO_KBN.MEISHO_CD           =   TBL_A.KANJO_KAMOKU\n  LEFT JOIN                                                          -- 【担当部署用】\n       M_MEISHO_B                      AS  M_BUSHO_CD\n    ON M_BUSHO_CD.MEISHO_KBN           =   \qM001\q\n   AND M_BUSHO_CD.MEISHO_CD            =   TBL_A.BUSHO_CD\n  LEFT JOIN                                                          -- 【売上入力者用】\n       M_SHAIN                         AS  M_URI_NYU\n    ON M_URI_NYU.SHAIN_CD              =   TBL_A.URIAGE_NYURYOKUSHA_CD\n  LEFT JOIN                                                          -- 【営業担当者用】\n       M_SHAIN                         AS  M_EIGYO_TANTO\n    ON M_EIGYO_TANTO.SHAIN_CD          =   TBL_A.EIGYO_TANTO_CD\n  LEFT JOIN                                                          -- 【受注区分用】\n       M_MEISHO_B                      AS  M_JUCHU_KBN\n    ON M_JUCHU_KBN.MEISHO_KBN          =   \qK030\q\n   AND M_JUCHU_KBN.MEISHO_CD           =   TBL_A.JUCHU_KBN\n  LEFT JOIN                                                          -- 【売上ステータス用】\n       M_MEISHO_B                      AS  M_URIAGE_STS\n    ON M_URIAGE_STS.MEISHO_KBN         =   \qS007\q\n   AND M_URIAGE_STS.MEISHO_CD          =   TBL_A.URIAGE_STS\n  LEFT JOIN                                                          -- 【請求先用】\n       M_AITESAKI                      AS  M_SEIKYU\n    ON M_SEIKYU.AITE_CD                =   TBL_A.SEIKYU_CD\n  LEFT JOIN                                                          -- 【分析方法用】\n       M_MEISHO_B                      AS  M_BUN_HOHO\n    ON M_BUN_HOHO.MEISHO_KBN           =   \qK017\q\n   AND M_BUN_HOHO.MEISHO_CD            =   TBL_A.BUNSEKI_HOHO_CD\n  LEFT JOIN                                                          -- 【フリーワード(ボディ)】\n     (\n       SELECT TBL_A.URIAGE_NO\n             ,(                                    -- 商品コード\n               SELECT \q|\q + TBL_B.SHOHIN_CD      AS  [data()]\n                 FROM T_URIAGE_B                 AS  TBL_B\n                WHERE TBL_B.URIAGE_NO            =   TBL_A.URIAGE_NO\n                FOR XML PATH (\q\q)\n              )\n             +(                                    -- 商品名\n               SELECT \q|\q + TBL_B.SHOHIN_MEI     AS  [data()]\n                 FROM T_URIAGE_B                 AS  TBL_B\n                WHERE TBL_B.URIAGE_NO            =   TBL_A.URIAGE_NO\n                FOR XML PATH (\q\q)\n              )\n             +(                                    -- 摘要\n               SELECT \q|\q + TBL_B.TEKIYO         AS  [data()]\n                 FROM T_URIAGE_B                 AS  TBL_B\n                WHERE TBL_B.URIAGE_NO            =   TBL_A.URIAGE_NO\n                FOR XML PATH (\q\q)\n              )\n                                                 AS  FREEWD\n         FROM T_URIAGE_B TBL_A\n        GROUP BY\n              TBL_A.URIAGE_NO\n     ) TBL_F\n    ON TBL_F.URIAGE_NO                 =   TBL_A.URIAGE_NO\n
LNameByComment=1
Color=$000000
BkColor=$D1D1FF
Position="売上関連",150,100
Page=売上関連
Left=150
Top=100

[View]
PName=V_HAN_JUCHU_B_MITSU
LName=販売商品受注ボディビュー見積接続用
Tag=ビュー
Comment=
ViewOption=
Query=SELECT T_MITSU_B.GYO_NO                     AS GYO_NO           --行番号\n      ,T_MITSU_B.ROW_KBN                    AS ROW_KBN          --行区分\n      ,CASE J_SHOHIN.DOKU_FLG\n       WHEN \qTrue\q THEN \q×\q\n       ELSE NULL\n       END                                  AS DOKU_FLG         --毒劇物フラグ\n      ,CASE T_MITSU_B.SHOHIN_CD\n       WHEN \qnebiki_per\q THEN \qnebiki_dir\q\n       ELSE T_MITSU_B.SHOHIN_CD\n       END                                  AS SHOHIN_CD        --商品CD\n      ,T_MITSU_B.SHOHIN_MEI                 AS SHOHIN_MEI       --商品名\n      ,CASE T_MITSU_B.SHOHIN_CD\n       WHEN \qnebiki_per\q THEN NULL\n       ELSE T_MITSU_B.SURYO\n       END                                  AS JUCHU_SURYO      --受注数量\n      ,CASE T_MITSU_B.SHOHIN_CD\n       WHEN \qnebiki_per\q THEN NULL\n       ELSE T_MITSU_B.TANI\n       END                                  AS JUCHU_TANI       --受注単位\n      ,T_MITSU_B.TANKA                      AS HAMBAI_TANKA     --販売単価\n      ,T_MITSU_B.KINGAKU                    AS HAMBAI_KINGAKU   --販売金額\n      ,T_MITSU_B.TEKIYO                     AS TEKIYO           --摘要\n      ,T_MITSU_B.TEIKA                      AS TEIKA            --定価\n      ,T_MITSU_B.GENTANKA                   AS GENTANKA         --原単価\n      ,J_SHOHIN.SHIIRE_IRISU                AS IRISU            --入数\n      ,J_SHOHIN.BARA_TANI                   AS BARA_TANI        --バラ単位\n      ,NULL                                 AS GENKA_SURYO      --原価数量\n      ,J_SHOHIN.SHIIRE_IRISU_TANI           AS IRISU_TANI       --入数単位\n      ,NULL                                 AS TOTAL            --総数\n      ,NULL                                 AS GENKA_KINGAKU    --原価金額\n      ,NULL                                 AS ARARIRITSU       --粗利率\n      ,NULL                                 AS ARARIEKI         --粗利益\n      ,T_MITSU_B.SHIIRE_CD                  AS SHIIRE_CD        --仕入先CD\n      ,J_SHIIRE.AITE_MEI                    AS SHIIRE_MEI       --仕入先名\n      ,J_MAKER.AITE_CD                      AS MAKER_CD         --メーカーCD\n      ,J_MAKER.AITE_MEI                     AS MAKER_MEI        --メーカー名\n      ,T_MITSU_B.MITSU_NO                   AS JURI_NO         --対象見積No.\n  FROM T_MITSU_B\n  --商品\n  LEFT JOIN\n       M_SHOHIN     AS J_SHOHIN\n    ON J_SHOHIN.SHOHIN_CD = T_MITSU_B.SHOHIN_CD\n  --仕入先\n  LEFT JOIN\n       M_AITESAKI   AS J_SHIIRE\n    ON J_SHIIRE.AITE_CD = T_MITSU_B.SHIIRE_CD\n  --メーカー\n  LEFT JOIN\n       M_AITESAKI   AS J_MAKER\n    ON J_MAKER.AITE_CD = J_SHOHIN.MAKER_CD\n\n WHERE T_MITSU_B.ROW_KBN <> 4\n   AND T_MITSU_B.SHOHIN_CD <> \q\q\n\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="販売商品関連",2150,1100
Page=販売商品関連
Left=2150
Top=1100

[View]
PName=V_HAN_JUCHU_H_MITSU
LName=販売商品受注ヘッダビュー見積接続用
Tag=ビュー
Comment=
ViewOption=
Query=SELECT NULL                                 AS JURI_NO              --受理No.\n      ,2                                    AS HAMBAI_STS_CD        --販売受注ステータスCD\n      ,HAMBAI_STS.DATA_1                    AS HAMBAI_STS           --販売受注ステータス\n      ,NULL                                 AS HAMBAI_KBN           --販売受注区分\n      ,NULL                                 AS NYURYOKU_DATETIME    --入力日時\n      ,DEMPYO.DATA_1                        AS DEMPYO_HAKKO_KBN     --伝票発行区分\n      ,NULL                                 AS REF_JURI_NO          --参照元受理No.\n      ,NULL                                 AS NYURYOKUSHA_CD       --入力者名CD\n      ,NULL                                 AS NYURYOKUSHA          --入力者名\n      ,T_MITSU_H.MITSU_NO                   AS MITSU_NO             --対象見積No.\n      ,CONVERT(VARCHAR(10),GETDATE(),111)   AS JUCHU_DATE           --受注日\n      ,NULL                                 AS NOHIN_DATE           --納品日\n      ,T_MITSU_H.SEIKYU_CD                  AS SEIKYUSAKI_CD        --請求先CD\n      ,SEIKYUSAKI.AITE_MEI                  AS SEIKYUSAKI_MEI       --請求先名\n      ,SEIKYUSAKI.SEIKYU_HASU_KBN           AS SEIKYU_HASU_KBN      --請求先端数処理区分\n      ,T_MITSU_H.SEMPO_TANTO_MEI            AS SEIKYU_TANTO_MEI     --請求先担当者名\n      ,T_MITSU_H.KENMEI                     AS KENMEI               --件名\n      ,SEIKYUSAKI.EIGYO_TANTO_CD            AS EIGYO_TANTO_CD       --営業担当者名CD\n      ,EIGYO_TANTO.SHIMEI                   AS EIGYO_TANTO          --営業担当者名\n      ,NULL                                 AS HASSO_CD             --発送先CD\n      ,NULL                                 AS HASSO_MEI            --発送先名\n      ,NULL                                 AS HASSO_YUBIN_NO       --発送先郵便番号\n      ,NULL                                 AS HASSO_ADDRESS_1      --発送先住所１\n      ,NULL                                 AS HASSO_ADDRESS_2      --発送先住所２\n      ,NULL                                 AS HASSO_TEL            --発送先ＴＥＬ\n      ,NULL                                 AS HASSO_FAX            --発送先ＦＡＸ\n      ,NULL                                 AS HASSO_TANTO_MEI      --発送先担当者名\n      ,\qFalse\q                              AS MAKER_FLG            --メーカー直送フラグ\n      ,T_MITSU_H.TAX_RATE                   AS TAX_RATE             --消費税率\n      ,T_MITSU_H.NUKI_TOTAL                 AS NUKI_TOTAL           --税抜合計\n      ,T_MITSU_H.TAX                        AS TAX                  --消費税\n      ,T_MITSU_H.KOMI_TOTAL                 AS KOMI_TOTAL           --税込合計\n      ,T_MITSU_H.GENKA_TOTAL                AS GENKA_TOTAL          --原価合計\n      ,T_MITSU_H.ARARIEKI                   AS ARARIEKI             --粗利益\n      ,T_MITSU_H.RIEKI_RATE                 AS ARARIRITSU           --粗利率\n      ,\qFalse\q                              AS DEL_FLG              --削除フラグ\n  FROM T_MITSU_H\n  --販売受注ステータス\n  LEFT JOIN\n       M_MEISHO_B             AS HAMBAI_STS\n    ON HAMBAI_STS.MEISHO_KBN = \qS004\q\n   AND HAMBAI_STS.MEISHO_CD  = \q1\q\n  --請求先\n  LEFT JOIN\n       M_AITESAKI             AS SEIKYUSAKI\n    ON T_MITSU_H.SEIKYU_CD = SEIKYUSAKI.AITE_CD\n  --営業担当者\n  LEFT JOIN\n       M_SHAIN                AS EIGYO_TANTO\n    ON SEIKYUSAKI.EIGYO_TANTO_CD = EIGYO_TANTO.SHAIN_CD\n  --伝票発行区分\n  LEFT JOIN\n       M_MEISHO_B             AS DEMPYO\n    ON DEMPYO.MEISHO_KBN = \qK026\q\n   AND DEMPYO.MEISHO_CD  = \q2\q\n
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="販売商品関連",2150,100
Page=販売商品関連
Left=2150
Top=100

[View]
PName=V_HAN_JUCHU_H_SANSHO
LName=販売商品受注ヘッダビュー参照作成用
Tag=ビュー
Comment=
ViewOption=
Query=SELECT NULL                                 AS JURI_NO              --受理No.\n      ,2                                    AS HAMBAI_STS_CD        --販売受注ステータスCD\n      ,HAMBAI_STS.DATA_1                    AS HAMBAI_STS           --販売受注ステータス\n      ,HAMBAI_KBN.DATA_1                    AS HAMBAI_KBN           --販売受注区分\n      ,NULL                                 AS NYURYOKU_DATETIME    --入力日時\n      ,DEMPYO.DATA_1                        AS DEMPYO_HAKKO_KBN     --伝票発行区分\n      ,T_HAN_JUCHU_H.JURI_NO                AS REF_JURI_NO          --参照元受理No.\n      ,NULL                                 AS NYURYOKUSHA_CD       --入力者名CD\n      ,NULL                                 AS NYURYOKUSHA          --入力者名\n      ,T_HAN_JUCHU_H.MITSU_NO               AS MITSU_NO             --対象見積No.\n      ,CONVERT(VARCHAR(10),GETDATE(),111)   AS JUCHU_DATE           --受注日\n      ,NULL                                 AS NOHIN_DATE           --納品日\n      ,SEIKYUSAKI.AITE_CD                   AS SEIKYUSAKI_CD        --請求先CD\n      ,SEIKYUSAKI.AITE_MEI                  AS SEIKYUSAKI_MEI       --請求先名\n      ,SEIKYUSAKI.SEIKYU_HASU_KBN           AS SEIKYU_HASU_KBN      --請求先端数処理区分\n      ,T_HAN_JUCHU_H.SEIKYU_TANTO_MEI       AS SEIKYU_TANTO_MEI     --請求先担当者名\n      ,T_HAN_JUCHU_H.KENMEI                 AS KENMEI               --件名\n      ,EIGYO_TANTO.SHAIN_CD                 AS EIGYO_TANTO_CD       --営業担当者名CD\n      ,EIGYO_TANTO.SHIMEI                   AS EIGYO_TANTO          --営業担当者名\n      ,T_HAN_JUCHU_H.HASSO_CD               AS HASSO_CD             --発送先CD\n      ,T_HAN_JUCHU_H.HASSO_MEI              AS HASSO_MEI            --発送先名\n      ,T_HAN_JUCHU_H.HASSO_YUBIN_NO         AS HASSO_YUBIN_NO       --発送先郵便番号\n      ,T_HAN_JUCHU_H.HASSO_ADDRESS_1        AS HASSO_ADDRESS_1      --発送先住所１\n      ,T_HAN_JUCHU_H.HASSO_ADDRESS_2        AS HASSO_ADDRESS_2      --発送先住所２\n      ,T_HAN_JUCHU_H.HASSO_TEL              AS HASSO_TEL            --発送先ＴＥＬ\n      ,T_HAN_JUCHU_H.HASSO_FAX              AS HASSO_FAX            --発送先ＦＡＸ\n      ,T_HAN_JUCHU_H.HASSO_TANTO_MEI        AS HASSO_TANTO_MEI      --発送先担当者名\n      ,T_HAN_JUCHU_H.MAKER_FLG              AS MAKER_FLG            --メーカー直送フラグ\n      ,T_HAN_JUCHU_H.TAX_RATE               AS TAX_RATE             --消費税率\n      ,T_HAN_JUCHU_H.NUKI_TOTAL             AS NUKI_TOTAL           --税抜合計\n      ,T_HAN_JUCHU_H.TAX                    AS TAX                  --消費税\n      ,T_HAN_JUCHU_H.KOMI_TOTAL             AS KOMI_TOTAL           --税込合計\n      ,T_HAN_JUCHU_H.GENKA_TOTAL            AS GENKA_TOTAL          --原価合計\n      ,T_HAN_JUCHU_H.ARARIEKI               AS ARARIEKI             --粗利益\n      ,T_HAN_JUCHU_H.ARARIRITSU             AS ARARIRITSU           --粗利率\n      ,\qFalse\q                              AS DEL_FLG              --削除フラグ\n  FROM T_HAN_JUCHU_H\n  --販売受注ステータス\n  LEFT JOIN\n       M_MEISHO_B             AS HAMBAI_STS\n    ON HAMBAI_STS.MEISHO_KBN = \qS004\q\n   AND HAMBAI_STS.MEISHO_CD  = \q1\q\n  --販売受注区分\n  LEFT JOIN\n       M_MEISHO_B             AS HAMBAI_KBN\n    ON HAMBAI_KBN.MEISHO_KBN    = \qK020\q\n   AND T_HAN_JUCHU_H.HAMBAI_KBN = HAMBAI_KBN.MEISHO_CD\n  --請求先\n  LEFT JOIN\n       M_AITESAKI             AS SEIKYUSAKI\n    ON T_HAN_JUCHU_H.SEIKYU_CD = SEIKYUSAKI.AITE_CD\n   AND SEIKYUSAKI.MISHIYO_FLG  = \qFalse\q\n  --営業担当者\n  LEFT JOIN\n       M_SHAIN                AS EIGYO_TANTO\n    ON SEIKYUSAKI.EIGYO_TANTO_CD = EIGYO_TANTO.SHAIN_CD\n   AND EIGYO_TANTO.MISHIYO_FLG   = \qFalse\q\n  --伝票発行区分(未発行)\n  LEFT JOIN\n       M_MEISHO_B             AS DEMPYO\n    ON DEMPYO.MEISHO_KBN = \qK026\q\n   AND DEMPYO.MEISHO_CD  = \q2\q\n\n
LNameByComment=1
