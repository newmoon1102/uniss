PName=V_MITSU_R_LIST
LName=見積訂正履歴リストボディービュー
Tag=ビュー
Comment=
ViewOption=
Query=SELECT \q＞\q                                                      AS GYO_MOZI         --選択\n--      ,T_MITSU_H_R.DBS_UPDATE_DATE\n      ,SUBSTRING(T_MITSU_H_R.DBS_UPDATE_DATE, 3,4)+ \q/\q +\n       SUBSTRING(T_MITSU_H_R.DBS_UPDATE_DATE, 8,2)+ \q/\q + \n       SUBSTRING(T_MITSU_H_R.DBS_UPDATE_DATE,11,2)+ \q \q +\n       SUBSTRING(T_MITSU_H_R.DBS_UPDATE_DATE,14,8)              AS UPDATE_DATE      --更新日時\n      ,T_MITSU_H_R.SHORI_KBN                                    AS SHORI_KBN        --処理区分\n      ,T_MITSU_H_R.MITSU_DATE                                   AS MITSU_DATE       --見積日\n      ,T_MITSU_H_R.KENMEI                                       AS KENMEI           --件名\n      ,R_NYURYOKUSHA.SHIMEI                                     AS SHIMEI           --入力者名\n      ,T_MITSU_H_R.NUKI_TOTAL                                   AS NUKI_TOTAL       --税抜合計\n      ,T_MITSU_H_R.ARARIEKI                                     AS ARARIEKI         --粗利益\n      ,T_MITSU_H_R.RIEKI_RATE                                   AS RIEKI_RATE       --粗利率\n      ,T_MITSU_H_R.UKE_KIJITSU                                  AS UKE_KIJITSU      --受渡期日\n      ,T_MITSU_H_R.UKE_BASHO                                    AS UKE_BASHO        --受渡場所\n      ,T_MITSU_H_R.TORI_JOKEN                                   AS TORI_JOKEN       --取引条件\n      ,T_MITSU_H_R.YUKO_KIGEN                                   AS YUKO_KIGEN       --有効期限\n      ,CASE T_MITSU_H_R.MODE_KBN                                \n       WHEN 1 THEN \q通常\q\n       ELSE \q単価\q\n       END                                                      AS MODE             --種別\n      ,T_MITSU_H_R.MITSU_NO                                     AS MITSU_NO         --見積No.\n      ,T_MITSU_H_R.RIREKI_NO                                    AS RIREKI_NO        --履歴No.\n      \n\n  FROM T_MITSU_H_R\n  \n  LEFT JOIN M_SHAIN             AS R_NYURYOKUSHA\n    ON T_MITSU_H_R.NYURYOKUSHA_CD = R_NYURYOKUSHA.SHAIN_CD\n    
