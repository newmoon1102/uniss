[View]
PName=V_URIAGE_REPORT
LName=売上一覧表ビュー
Tag=ビュー
Comment=
ViewOption=
Query=--*DataTitle V_URIAGE_REPORT(売上一覧表ビュー)\nSELECT V_C_URIAGE_H.URIAGE_DATE     AS URIAGE_DATE      --売上日\n      ,V_C_URIAGE_H.URIAGE_NO       AS URIAGE_NO        --売上№\n      ,V_C_URIAGE_H.SEIKYU_CD       AS SEIKYU_CD        --請求先CD\n      ,V_C_URIAGE_H.SEIKYU_MEI      AS SEIKYU_MEI       --請求先名\n      ,T_URIAGE_B.SHOHIN_CD         AS SHOHIN_CD        --商品CD\n      ,T_URIAGE_B.SHOHIN_MEI        AS SHOHIN_MEI       --商品名\n      ,T_URIAGE_B.URIAGE_SURYO      AS SURYO            --数量\n      ,T_URIAGE_B.URIAGE_SURYO_TANI AS TANI             --単位\n      ,T_URIAGE_B.HAMBAI_TANKA      AS TANKA            --単価\n      ,T_URIAGE_B.HAMBAI_KINGAKU    AS KINGAKU          --金額\n      ,V_C_URIAGE_H.TAX_RATE*\n       T_URIAGE_B.HAMBAI_KINGAKU    AS TAX              --消費税(未使用)\n      ,T_URIAGE_B.ARARIEKI          AS ARARIEKI         --粗利益(未使用)\n      ,V_C_URIAGE_H.URI_NYU_MEI     AS NYURYOKUSHA_MEI  --入力者(未使用)\n      ,NULL                         AS BUNSEKI_HOHO     --分析方法名\n      ,NULL                         AS BUSHO_MEI        --担当部署\n      ,NULL                         AS EIGYO_TANTO_MEI  --営業担当\n      ,T_URIAGE_B.GYO_NO            AS GYO_NO           --行番号\n  FROM V_C_URIAGE_H\n\n   LEFT JOIN\n       T_URIAGE_B\n    ON T_URIAGE_B.URIAGE_NO = V_C_URIAGE_H.URIAGE_NO\nUNION\nSELECT NULL                         AS URIAGE_DATE      --売上日\n      ,T_URIAGE_B.URIAGE_NO         AS URIAGE_NO        --売上№\n      ,NULL                         AS SEIKYU_CD        --請求先CD\n      ,NULL                         AS SEIKYU_MEI       --請求先名\n      ,NULL                         AS SHOHIN_CD        --商品CD\n      ,\q※伝票計※\q                 AS SHOHIN_MEI       --商品名\n      ,SUM(URIAGE_SURYO)            AS URIAGE_SURYO     --数量\n      ,NULL                         AS TANI             --単位\n      ,SUM(HAMBAI_TANKA)            AS HAMBAI_TANKA     --単価\n      ,SUM(HAMBAI_KINGAKU)          AS HAMBAI_KINGAKU   --金額\n      ,(SELECT V_C_URIAGE_H.TAX\n          FROM V_C_URIAGE_H\n         WHERE V_C_URIAGE_H.URIAGE_NO = T_URIAGE_B.URIAGE_NO)\n                                    AS TAX              --消費税(未使用)\n      ,SUM(T_URIAGE_B.ARARIEKI)     AS ARARIEKI         --粗利益(未使用)\n      ,NULL                         AS NYURYOKUSHA_MEI  --入力者(未使用)\n      ,(SELECT V_C_URIAGE_H.BUNSEKI_HOHO\n          FROM V_C_URIAGE_H\n         WHERE V_C_URIAGE_H.URIAGE_NO = T_URIAGE_B.URIAGE_NO)\n                                    AS BUNSEKI_HOHO     --分析方法名\n      ,(SELECT V_C_URIAGE_H.BUSHO_MEI\n          FROM V_C_URIAGE_H\n         WHERE V_C_URIAGE_H.URIAGE_NO = T_URIAGE_B.URIAGE_NO)\n                                    AS BUSHO_MEI        --担当部署\n      ,(SELECT V_C_URIAGE_H.EIGYO_TANTO_MEI\n          FROM V_C_URIAGE_H\n         WHERE V_C_URIAGE_H.URIAGE_NO = T_URIAGE_B.URIAGE_NO)\n                                    AS EIGYO_TANTO_MEI  --営業担当\n      ,100                          AS GYO_NO           --行番号\n  FROM T_URIAGE_B\n  LEFT JOIN\n       V_C_URIAGE_H\n    ON V_C_URIAGE_H.URIAGE_NO = T_URIAGE_B.URIAGE_NO\n GROUP BY\n       T_URIAGE_B.URIAGE_NO\n
LNameByComment=1
